=> {\n    40    let tabId = findSessionByModule('models');\n    41    const isNew = !tabId;\n    42    if (!tabId) {\n    43      tabId = createSession({\n    44        label: '3D Models & Characters',\n    45        accentColor: '#a78bfa',\n    46        moduleId: 'models',\n    47        projectPath,\n    48      });\n    49    }\n    50    setActiveTab(tabId);\n    51\n    52    const dispatch = () => {\n    53      window.dispatchEvent(\n    54        new CustomEvent('pof-cli-prompt', {\n    55          detail: { tabId, prompt },\n    56        })\n    57      );\n    58    };\n    59    if (isNew) setTimeout(dispatch, 150);\n    60    else dispatch();\n    61  }, [findSessionByModule, createSession, setActiveTab, projectPath]);\n    62\n    63  if (!mod || !cat) return null;\n    64\n    65  const extraTabs: ExtraTab[] = [\n    66    {\n    67      id: 'inventory',\n    68      label: 'Asset Inventory',\n    69      icon: FolderSearch,\n    70      render: () => <AssetInventory />,\n    71    },\n    72    {\n    73      id: 'pipeline',\n    74      label: 'Asset Pipeline',\n    75      icon: Boxes,\n    76      render: () => (\n    77        <div className=\"flex flex-col items-center pt-4\">\n    78          <h2 className=\"text-base font-semibold text-text mb-1\">\n    79            3D Asset Import Pipeline\n    80          </h2>\n    81          <p className=\"text-xs text-text-muted text-center max-w-sm mb-6\">\n    82            Work through each stage to set up your FBX/glTF import workflow.\n    83          </p>\n    84          <AssetPipelineDiagram\n    85            completedStages={completedStages}\n    86            onRunPrompt={sendPrompt}\n    87            isRunning={false}\n    88          />\n    89        </div>\n    90      ),\n    91    },\n    92  ];\n    93\n    94  return (\n    95    <ReviewableModuleView\n    96      moduleId=\"models\"\n    97      moduleLabel={mod.label}\n    98      moduleDescription={mod.description}\n    99      moduleIcon={mod.icon}\n   100      accentColor={cat.accentColor}\n   101      checklist={getModuleChecklist('models')}\n   102      quickActions={mod.quickActions}\n   103      extraTabs={extraTabs}\n   104    />\n   105  );\n   106}\n   107"}]},"parent_tool_use_id":"toolu_015LYvNDLdiXJBovEGLT7xRB","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"d10bba11-b275-40cb-8189-c9aad01f2278"}
