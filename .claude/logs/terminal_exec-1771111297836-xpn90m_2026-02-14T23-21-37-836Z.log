[2026-02-14T23:23:49.156Z] [STDOUT] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_019E6Jm7THiZZwMFEitx1Cbr","type":"tool_result","content":"     1'use client';\n     2\n     3import { useState, useMemo, useCallback } from 'react';\n     4import { Sparkles, Search, Zap, Droplets, Flame, Eye, Shield, Snowflake, Waves, Gem } from 'lucide-react';\n     5import type { LucideIcon } from 'lucide-react';\n     6\n     7//  Types \n     8\n     9export type MaterialCategory = 'elemental' | 'stylized' | 'utility';\n    10\n    11export interface MaterialPattern {\n    12  id: string;\n    13  name: string;\n    14  category: MaterialCategory;\n    15  icon: LucideIcon;\n    16  description: string;\n    17  /** High-level approach using HLSL / material nodes */\n    18  approach: string;\n    19  /** Key HLSL snippet or node description */\n    20  hlslSnippet: string;\n    21  tags: string[];\n    22}\n    23\n    24export interface MaterialPatternCatalogConfig {\n    25  patterns: MaterialPattern[];\n    26}\n    27\n    28//  Static Pattern Data \n    29\n    30const CATEGORY_META: Record<MaterialCategory, { label: string; color: string }> = {\n    31  elemental: { label: 'Elemental', color: '#f97316' },\n    32  stylized:  { label: 'Stylized',  color: '#a78bfa' },\n    33  utility:   { label: 'Utility',   color: '#38bdf8' },\n    34};\n    35\n    36export const MATERIAL_PATTERNS: MaterialPattern[] = [\n    37  {\n    38    id: 'mat-water',\n    39    name: 'Water Surface',\n    40    category: 'elemental',\n    41    icon: Droplets,\n    42    description: 'Translucent water with animated normals, depth-based opacity fade, refraction, caustics projection, and foam at shore edges.',\n    43    approach: 'Two-layer scrolling normal maps blended with Lerp. Depth fade via SceneDepth  PixelDepth for shore transparency. Refraction offset driven by normal map intensity. Caustics from a projected texture panning in world space.',\n    44    hlslSnippet: `// Depth-based opacity\n    45float Depth = SceneDepth - PixelDepth;\n    46float Shore = saturate(Depth / ShoreDistance);\n    47// Dual normal blend\n    48float3 N1 = tex2D(NormalA, UV + Time * Flow1);\n    49float3 N2 = tex2D(NormalB, UV + Time * Flow2);\n    50float3 FinalNormal = normalize(lerp(N1, N2, 0.5));`,\n    51    tags: ['translucent', 'animated', 'refraction', 'depth-fade'],\n    52  },\n    53  {\n    54    id: 'mat-fire',\n    55    name: 'Fire / Embers',\n    56    category: 'elemental',\n    57    icon: Flame,\n    58    description: 'Emissive fire material with scrolling distortion mask, color ramp from red  orange  yellow, and flickering intensity.',\n    59    approach: 'Vertical UV scroll on a noise texture for shape. Color ramp using a gradient texture or lerp chain. Distortion via a second noise texture offsetting UVs. Emissive intensity modulated by Time-based sine for flicker.',\n    60    hlslSnippet: `// Scrolling fire shape\n    61float2 FireUV = UV + float2(0, -Time * ScrollSpeed);\n    62float Shape = tex2D(NoiseTex, FireUV).r;\n    63// Color ramp\n    64float3 Color = lerp(RedBase, YellowTip, Shape);\n    65// Flicker\n    66float Flicker = 1.0 + 0.3 * sin(Time * FlickerRate);\n    67EmissiveColor = Color * Shape * Flicker * Intensity;`,\n    68    tags: ['emissive', 'animated', 'noise', 'color-ramp'],\n    69  },\n    70  {\n    71    id: 'mat-dissolve',\n    72    name: 'Dissolve Effect',\n    73    category: 'utility',\n    74    icon: Sparkles,\n    75    description: 'Opacity-mask dissolve driven by a noise texture and a scalar parameter (01). Glowing edge emission at the dissolve boundary.',\n    76    approach: 'Compare noise texture value against a DissolveAmount parameter for OpacityMask. Edge glow by checking a thin band around the threshold. Feed edge mask into emissive for a burning/glowing edge look.',\n    77    hlslSnippet: `// Dissolve mask\n    78float Noise = tex2D(DissolveTex, UV).r;\n    79float Mask = step(DissolveAmount, Noise);\n    80OpacityMask = Mask;\n    81// Edge glow\n    82float Edge = smoothstep(DissolveAmount, DissolveAmount + EdgeWidth, Noise);\n    83float EdgeMask = Mask * (1.0 - Edge);\n    84EmissiveColor = EdgeColor * EdgeMask * EdgeIntensity;`,\n    85    tags: ['opacity-mask', 'noise', 'emissive-edge', 'parameter-driven'],\n    86  },\n    87  {\n    88    id: 'mat-hologram',\n    89    name: 'Hologram',\n    90    category: 'stylized',\n    91    icon: Eye,\n    92    description: 'Translucent holographic shader with scanlines, Fresnel rim glow, subtle vertex jitter, and chromatic aberration.',\n    93    approach: 'Fresnel node drives rim intensity. Scanlines from frac(WorldPosition.Z * LineFreq). Vertex offset with small random jitter on a timer for glitch. RGB channels offset slightly for chromatic aberration.',\n    94    hlslSnippet: `// Scanlines\n    95float Scan = frac(WorldPos.Z * LineFreq + Time * ScanSpeed);\n    96Scan = step(0.5, Scan);\n    97// Fresnel rim\n    98float Rim = pow(1.0 - saturate(dot(Normal, ViewDir)), FresnelExp);\n    99// Glitch jitter\n   100float Jitter = frac(sin(Time * 43758.5453)) * GlitchAmt;\n   101WorldPosOffset = float3(Jitter, 0, 0);\n   102EmissiveColor = HoloColor * (Scan * 0.5 + Rim) * Intensity;`,\n   103    tags: ['translucent', 'fresnel', 'scanlines', 'vertex-offset'],\n   104  },\n   105  {\n   106    id: 'mat-forcefield',\n   107    name: 'Force Field',\n   108    category: 'stylized',\n   109    icon: Shield,\n   110    description: 'Translucent shield bubble with Fresnel edge glow, hexagonal pattern, and impact ripple effect driven by a world-space hit location.',\n   111    approach: 'Fresnel for edge visibility. Hexagon pattern from a tiling texture or procedural hex math. Impact ripple: distance from HitPoint in world space, expanding ring via Time. Depth fade for soft intersection with geometry.',\n   112    hlslSnippet: `// Fresnel edge\n   113float Fresnel = pow(1.0 - saturate(dot(N, V)), 3.0);\n   114// Hex pattern\n   115float Hex = tex2D(HexTex, UV * HexTile).r;\n   116// Impact ripple\n   117float Dist = distance(WorldPos, HitPoint);\n   118float Ring = smoothstep(RippleRadius, RippleRadius + 0.1,\n   119             Dist) * (1.0 - smoothstep(RippleRadius + 0.1,\n   120             RippleRadius + 0.3, Dist));\n   121// Depth intersection glow\n   122float Intersect = 1.0 - saturate((SceneDepth - PixelDepth) / SoftDist);\n   123Opacity = saturate(Fresnel + Hex * 0.3 + Ring + Intersect);`,\n   124    tags: ['translucent', 'fresnel', 'hexagon', 'impact-ripple', 'depth-fade'],\n   125  },\n   126  {\n   127    id: 'mat-glass',\n   128    name: 'Glass / Crystal',\n   129    category: 'elemental',\n   130    icon: Gem,\n   131    description: 'Physically-based glass with refraction, specular highlights, tint color, and optional frosted roughness variation.',\n   132    approach: 'Translucent shading model with high specular (0.9+), low roughness. Refraction via SceneColor offset. Tint via BaseColor multiply on refracted scene. Frosted look by lerping roughness with a noise texture.',\n   133    hlslSnippet: `// PBR glass parameters\n   134BaseColor = TintColor;\n   135Metallic = 0.0;\n   136Specular = 0.95;\n   137Roughness = lerp(0.02, 0.4, FrostMask);\n   138// Refraction\n   139float2 RefractOffset = Normal.xy * RefractionStrength;\n   140float3 SceneCol = tex2D(SceneColorTex, ScreenUV + RefractOffset);\n   141EmissiveColor = SceneCol * TintColor * Opacity;`,\n   142    tags: ['translucent', 'refraction', 'pbr', 'specular'],\n   143  },\n   144  {\n   145    id: 'mat-ice',\n   146    name: 'Ice / Frost',\n   147    category: 'elemental',\n   148    icon: Snowflake,\n   149    description: 'Sub-surface ice material with blue-white color ramp, crystalline normal detail, inner glow via SSS, and surface frost layer.',\n   150    approach: 'Subsurface shading model with blue subsurface color. Two normal map layers: large ice cracks + fine crystal detail. Frost overlay controlled by a mask parameter. Emissive inner glow modulated by depth.',\n   151    hlslSnippet: `// Subsurface ice\n   152SubsurfaceColor = float3(0.4, 0.7, 1.0);\n   153// Dual normal detail\n   154float3 NCracks = tex2D(CrackNormal, UV * 2.0);\n   155float3 NCrystal = tex2D(CrystalNormal, UV * 8.0);\n   156Normal = BlendAngleCorrected(NCracks, NCrystal);\n   157// Frost overlay\n   158float Frost = tex2D(FrostMask, UV).r * FrostAmount;\n   159Roughness = lerp(0.1, 0.8, Frost);\n   160BaseColor = lerp(IceColor, float3(0.9, 0.95, 1.0), Frost);`,\n   161    tags: ['subsurface', 'dual-normal', 'frost-mask', 'pbr'],\n   162  },\n   163  {\n   164    id: 'mat-lava',\n   165    name: 'Lava / Magma',\n   166    category: 'elemental',\n   167    icon: Waves,\n   168    description: 'Emissive lava with animated flow via panning UVs, dark crust breakup, heat distortion, and temperature-based color shift.',\n   169    approach: 'Base crust from a dark rock texture. Emissive lava showing through crust cracks via a noise mask. UV panning for slow flow. Temperature parameter shifts emissive from deep red to bright yellow-white. Heat haze via refraction offset.',\n   170    hlslSnippet: `// Crust vs lava mask\n   171float2 FlowUV = UV + Time * float2(0.02, 0.01);\n   172float CrustMask = tex2D(CrustNoise, FlowUV).r;\n   173CrustMask = smoothstep(CrustThreshold, CrustThreshold + 0.1, CrustMask);\n   174// Temperature color\n   175float3 LavaColor = lerp(DeepRed, BrightYellow, Temperature);\n   176// Combine\n   177BaseColor = lerp(RockColor, float3(0,0,0), 1.0 - CrustMask);\n   178EmissiveColor = LavaColor * (1.0 - CrustMask) * EmissiveIntensity;\n   179Roughness = lerp(0.2, 0.9, CrustMask);`,\n   180    tags: ['emissive', 'animated', 'noise', 'temperature'],\n   181  },\n   182];\n   183\n   184//  Component \n   185\n   186const ACCENT = '#f59e0b';\n   187\n   188interface MaterialPatternCatalogProps {\n   189  onGenerate: (pattern: MaterialPattern) => void;\n   190  isGenerating: boolean;\n   191}\n   192\n   193export function MaterialPatternCatalog({ onGenerate, isGenerating }: MaterialPatternCatalogProps) {\n   194  const [search, setSearch] = useState('');\n   195  const [selectedCategory, setSelectedCategory] = useState<MaterialCategory | 'all'>('all');\n   196  const [expandedId, setExpandedId] = useState<string | null>(null);\n   197\n   198  const filtered = useMemo(() => {\n   199    return MATERIAL_PATTERNS.filter((p) => {\n   200      if (selectedCategory !== 'all' && p.category !== selectedCategory) return false;\n   201      if (search.trim()) {\n   202        const q = search.toLowerCase();\n   203        return (\n   204          p.name.toLowerCase().includes(q) ||\n   205          p.description.toLowerCase().includes(q) ||\n   206          p.tags.some((t) => t.toLowerCase().includes(q))\n   207        );\n   208      }\n   209      return true;\n   210    });\n   211  }, [search, selectedCategory]);\n   212\n   213  const handleToggle = useCallback((id: string) => {\n   214    setExpandedId((prev) => (prev === id ? null : id));\n   215  }, []);\n   216\n   217  return (\n   218    <div className=\"w-full space-y-4\">\n   219      {/* Header */}\n   220      <div className=\"flex items-center gap-2\">\n   221        <Sparkles className=\"w-4 h-4\" style={{ color: ACCENT }} />\n   222        <div>\n   223          <h3 className=\"text-xs font-semibold text-text\">Material Pattern Library</h3>\n   224          <p className=\"text-2xs text-text-muted\">\n   225            {MATERIAL_PATTERNS.length} patterns  click to expand, generate with Claude\n   226          </p>\n   227        </div>\n   228      </div>\n   229\n   230      {/* Search + Filter */}\n   231      <div className=\"flex items-center gap-2\">\n   232        <div className=\"flex-1 relative\">\n   233          <Search className=\"absolute left-2.5 top-1/2 -translate-y-1/2 w-3 h-3 text-[#4a4e6a]\" />\n   234          <input\n   235            type=\"text\"\n   236            value={search}\n   237            onChange={(e) => setSearch(e.target.value)}\n   238            placeholder=\"Search patterns...\"\n   239            className=\"w-full pl-7 pr-3 py-1.5 bg-surface border border-border rounded-lg text-xs text-text placeholder-[#4a4e6a] outline-none focus:border-border-bright transition-colors\"\n   240          />\n   241        </div>\n   242        <div className=\"flex gap-1\">\n   243          {(['all', 'elemental', 'stylized', 'utility'] as const).map((cat) => {\n   244            const isActive = selectedCategory === cat;\n   245            const color = cat === 'all' ? ACCENT : CATEGORY_META[cat].color;\n   246            const label = cat === 'all' ? 'All' : CATEGORY_META[cat].label;\n   247            return (\n   248              <button\n   249                key={cat}\n   250                onClick={() => setSelectedCategory(cat)}\n   251                className=\"px-2 py-1 rounded text-2xs font-medium transition-colors\"\n   252                style={{\n   253                  backgroundColor: isActive ? `${color}20` : 'transparent',\n   254                  color: isActive ? color : 'var(--text-muted)',\n   255                  border: `1px solid ${isActive ? `${color}40` : 'transparent'}`,\n   256                }}\n   257              >\n   258                {label}\n   259              </button>\n   260            );\n   261          })}\n   262        </div>\n   263      </div>\n   264\n   265      {/* Pattern List */}\n   266      <div className=\"space-y-2\">\n   267        {filtered.map((pattern) => (\n   268          <PatternCard\n   269            key={pattern.id}\n   270            pattern={pattern}\n   271            isExpanded={expandedId === pattern.id}\n   272            onToggle={() => handleToggle(pattern.id)}\n   273            onGenerate={() => onGenerate(pattern)}\n   274            isGenerating={isGenerating}\n   275          />\n   276        ))}\n   277        {filtered.length === 0 && (\n   278          <p className=\"text-xs text-[#4a4e6a] text-center py-6\">No patterns match your search.</p>\n   279        )}\n   280      </div>\n   281    </div>\n   282  );\n   283}\n   284\n   285//  Pattern Card \n   286\n   287interface PatternCardProps {\n   288  pattern: MaterialPattern;\n   289  isExpanded: boolean;\n   290  onToggle: () => void;\n   291  onGenerate: () => void;\n   292  isGenerating: boolean;\n   293}\n   294\n   295function PatternCard({ pattern, isExpanded, onToggle, onGenerate, isGenerating }: PatternCardProps) {\n   296  const Icon = pattern.icon;\n   297  const catMeta = CATEGORY_META[pattern.category];\n   298\n   299  return (\n   300    <div\n   301      className=\"rounded-xl border transition-all duration-200\"\n   302      style={{\n   303        borderColor: isExpanded ? `${catMeta.color}40` : 'var(--border)',\n   304        backgroundColor: isExpanded ? `${catMeta.color}06` : 'var(--surface-deep)',\n   305      }}\n   306    >\n   307      {/* Collapsed header  always visible */}\n   308      <button\n   309        onClick={onToggle}\n   310        className=\"w-full flex items-center gap-3 px-4 py-3 text-left group\"\n   311      >\n   312        <div\n   313          className=\"flex-shrink-0 w-7 h-7 rounded-lg flex items-center justify-center\"\n   314          style={{\n   315            background: `linear-gradient(135deg, ${catMeta.color}18, ${catMeta.color}08)`,\n   316            border: `1px solid ${catMeta.color}25`,\n   317          }}\n   318        >\n   319          <Icon className=\"w-3.5 h-3.5\" style={{ color: catMeta.color }} />\n   320        </div>\n   321\n   322        <div className=\"flex-1 min-w-0\">\n   323          <div className=\"flex items-center gap-2\">\n   324            <span className=\"text-xs font-semibold text-text\">{pattern.name}</span>\n   325            <span\n   326              className=\"text-2xs font-medium uppercase tracking-widest px-1.5 py-0.5 rounded\"\n   327              style={{ backgroundColor: `${catMeta.color}12`, color: catMeta.color }}\n   328            >\n   329              {catMeta.label}\n   330            </span>\n   331          </div>\n   332          <p className=\"text-xs text-text-muted mt-0.5 line-clamp-1\">{pattern.description}</p>\n   333        </div>\n   334\n   335        <span\n   336          className=\"text-2xs font-medium opacity-0 group-hover:opacity-100 transition-opacity flex-shrink-0\"\n   337          style={{ color: catMeta.color }}\n   338        >\n   339          {isExpanded ? 'Collapse' : 'Expand'} \n   340        </span>\n   341      </button>\n   342\n   343      {/* Expanded content */}\n   344      {isExpanded && (\n   345        <div className=\"px-4 pb-4 space-y-3\">\n   346          {/* Description */}\n   347          <p className=\"text-xs text-[#9b9ec0] leading-relaxed\">{pattern.description}</p>\n   348\n   349          {/* Approach */}\n   350          <div>\n   351            <h4 className=\"text-2xs font-semibold text-text uppercase tracking-widest mb-1\">Approach</h4>\n   352            <p className=\"text-xs text-text-muted-hover leading-relaxed\">{pattern.approach}</p>\n   353          </div>\n   354\n   355          {/* HLSL Snippet */}\n   356          <div>\n   357            <h4 className=\"text-2xs font-semibold text-text uppercase tracking-widest mb-1\">HLSL Reference</h4>\n   358            <pre className=\"text-2xs leading-relaxed px-3 py-2 rounded-lg bg-[#0a0a1e] border border-border overflow-x-auto text-[#a0a4c4] font-mono whitespace-pre\">\n   359              {pattern.hlslSnippet}\n   360            </pre>\n   361          </div>\n   362\n   363          {/* Tags */}\n   364          <div className=\"flex flex-wrap gap-1\">\n   365            {pattern.tags.map((tag) => (\n   366              <span\n   367                key={tag}\n   368                className=\"text-2xs px-1.5 py-0.5 rounded bg-surface text-text-muted border border-border\"\n   3