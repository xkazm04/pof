st next = e.key === 'ArrowDown'\n    34→        ? buttons[(idx + 1) % buttons.length]\n    35→        : buttons[(idx - 1 + buttons.length) % buttons.length];\n    36→      next?.focus();\n    37→    }\n    38→  }, []);\n    39→\n    40→  return (\n    41→    <AnimatePresence mode=\"wait\">\n    42→      {category && subModules.length > 0 && (\n    43→        <motion.nav\n    44→          key={activeCategory}\n    45→          aria-label={`${category.label} modules`}\n    46→          initial={prefersReduced ? { opacity: 1, width: 180 } : { width: 0, opacity: 0 }}\n    47→          animate={{ width: 180, opacity: 1 }}\n    48→          exit={prefersReduced ? { opacity: 0 } : { width: 0, opacity: 0 }}\n    49→          transition={prefersReduced ? { duration: 0 } : { duration: 0.22, ease: [0.16, 1, 0.3, 1] }}\n    50→          className=\"h-full border-r border-border bg-surface-deep overflow-hidden\"\n    51→        >\n    52→          <div className=\"w-[180px] flex flex-col h-full\">\n    53→            <div className=\"px-3 py-3 border-b border-border\">\n    54→              <h2\n    55→                className=\"text-xs font-semibold uppercase tracking-wider\"\n    56→                style={{ color: category.accentColor }}\n    57→              >\n    58→                {category.label}\n    59→              </h2>\n    60→            </div>\n    61→            <StaggerContainer ref={listRef} className=\"flex-1 overflow-y-auto py-2\" role=\"listbox\" aria-label={`${category.label} modules`}>\n    62→              {subModules.map((mod) => {\n    63→                const isActive = activeSubModule === mod.id;\n    64→                const Icon = mod.icon;\n    65→                return (\n    66→                  <StaggerItem key={mod.id}>\n    67→                  <button\n    68→                    data-sidebar-item\n    69→                    onClick={() => setActiveSubModule(mod.id)}\n    70→                    onKeyDown={handleKeyDown}\n    71→               