€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n    96â†’\n    97â†’export function saveSuggestion(suggestion: GenreEvolutionSuggestion): void {\n    98â†’  ensureTables();\n    99â†’  const db = getDb();\n   100â†’  db.prepare(`\n   101â†’    INSERT OR REPLACE INTO genre_suggestions\n   102â†’      (id, sub_genre, label, description, confidence, patterns, status, proposed_changes, created_at, resolved_at)\n   103â†’    VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)\n   104â†’  `).run(\n   105â†’    suggestion.id,\n   106â†’    suggestion.subGenre,\n   107â†’    suggestion.label,\n   108â†’    suggestion.description,\n   109â†’    suggestion.confidence,\n   110â†’    JSON.stringify(suggestion.patterns),\n   111â†’    suggestion.status,\n   112â†’    JSON.stringify(suggestion.proposedChanges),\n   113â†’    suggestion.createdAt,\n   114â†’    suggestion.resolvedAt,\n   115â†’  );\n   116â†’}\n   117â†’\n   118â†’export function getPendingSuggestions(): GenreEvolutionSuggestion[] {\n   119â†’  ensureTables();\n   120â†’  const db = getDb();\n   121â†’  const rows = db.prepare(\n   122â†’    \"SELECT * FROM genre_suggestions WHERE status = 'pending' ORDER BY confidence DESC\"\n   123â†’  ).all() as Record<string, unknown>[];\n   124â†’  return rows.map(rowToSuggestion);\n   125â†’}\n   126â†’\n   127â†’export function getAcceptedSubGenres(): SubGenreId[] {\n   128â†’  ensureTables();\n   129â†’  const db = getDb();\n   130â†’  const rows = db.prepare(\n   131â†’    \"SELECT DISTINCT sub_genre FROM genre_suggestions WHERE status = 'accepted'\"\n   132â†’  ).all() as { sub_genre: string }[];\n   133â†’  return rows.map(r => r.sub_genre as SubGenreId);\n   134â†’}\n   135â†’\n   136â†’export function resolveSuggestion(id: string, action: 'accept' | 'dismiss'): void {\n   137â†’  ensureTables();\n   138â†’  const db = getDb();\n   139â†’  db.prepare(`\n   140â†’    UPDATE genre_suggestions SET status = ?, resolved_at = datetime('now') WHERE id = ?\n   141â†’  `).run(action === 'accept' ? 'accepted' : 'dismissed', id);\n   142â†’}\n   143â†’\n   144â†’export function getAllSuggestions(): GenreEvolutionSuggestion[] {\n   145â†’  ensureTables();\n   146â†’  const db = getDb();\n   147â†’  const rows = db.prepare(\n   148â†’    'SELECT * FROM genre_suggestions ORDER BY created_at DESC'\n   149â†’  ).all() as Record<string, unknown>[];\n   150â†’  return rows.map(rowToSuggestion);\n   151â†’}\n   152â†’\n   153â†’// â”€â”€â”€ Stats â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n   154â†’\n   155â†’export function getTelemetryStats(): TelemetryStats {\n   156â†’  ensureTables();\n   157â†’  const db = getDb();\n   158â†’\n   159â†’  const countRow = db.prepare('SELECT COUNT(*) as c FROM telemetry_snapshots').get() as { c: number };\n   160â†’  const latest = getLatestSnapshot();\n   161â†’  const pending = getPendingSuggestions();\n   162â†’  const accepted = getAcceptedSubGenres();\n   163â†’\n   164â†’  return {\n   165â†’    totalScans: countRow.c,\n   166â†’    lastScanAt: latest?.scannedAt ?? null,\n   167â†’    detectedPatterns: latest?.detectedPatterns ?? [],\n   168â†’    activeSuggestions: pending,\n   169â†’    acceptedSubGenres: accepted,\n   170â†’  };\n   171â†’}\n   172â†’\n   173â†’// â”€â”€â”€ Row mappers â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n   174â†’\n   175â†’function rowToSnapshot(row: Record<string, unknown>): TelemetrySnapshot {\n   176â†’  return {\n   177â†’    id: row.id as string,\n   178â†’    scannedAt: row.scanned_at as string,\n   179â†’    projectPath: row.project_path as string,\n   180â†’    signals: JSON.parse((row.signals as string) || '{}'),\n   181â†’    detectedPatterns: JSON.parse((row.detected_patterns as string) || '[]'),\n   182â†’  };\n   183â†’}\n   184â†’\n   185â†’function rowToSuggestion(row: Record<string, unknown>): GenreEvolutionSuggestion {\n   186â†’  return {\n   187â†’    id: row.id as string,\n   188â†’    subGenre: row.sub_genre as SubGenreId,\n   189â†’    label: row.label as string,\n   190â†’    description: (row.description as string) || '',\n   191â†’    confidence: (row.confidence as number) || 0,\n   192â†’    patterns: JSON.parse((row.patterns as string) || '[]'),\n   193â†’    status: row.status as GenreEvolutionSuggestion['status'],\n   194â†’    proposedChanges: JSON.parse((row.proposed_changes as string) || '{}'),\n   195â†’    createdAt: row.created_at as string,\n   196â†’    resolvedAt: (row.resolved_at as string) || null,\n   197â†’  };\n   198â†’}\n   199â†’"}]},"parent_tool_use_id":"toolu_01EsENNLoCmE98gLcj81XK6C","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"8954422b-5fa8-460c-b1cc-cedaca2be087"}
