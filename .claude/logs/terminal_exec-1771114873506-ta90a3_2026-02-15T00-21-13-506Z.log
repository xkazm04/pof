 as number) === 1,\n    49â†’    promptLength: row.prompt_length as number,\n    50â†’    success: (row.success as number) === 1,\n    51â†’    durationMs: row.duration_ms as number,\n    52â†’    startedAt: row.started_at as string,\n    53â†’    completedAt: row.completed_at as string,\n    54â†’  };\n    55â†’}\n    56â†’\n    57â†’// â”€â”€ CRUD â”€â”€\n    58â†’\n    59â†’export function recordSession(data: {\n    60â†’  moduleId: string;\n    61â†’  sessionKey: string;\n    62â†’  prompt: string;\n    63â†’  hadProjectContext: boolean;\n    64â†’  success: boolean;\n    65â†’  durationMs: number;\n    66â†’  startedAt: string;\n    67â†’  completedAt: string;\n    68â†’}): SessionRecord {\n    69â†’  ensureSessionAnalyticsTable();\n    70â†’  const db = getDb();\n    71â†’\n    72â†’  const preview = data.prompt.slice(0, 80).replace(/\\n/g, ' ');\n    73â†’\n    74â†’  const result = db.prepare(`\n    75â†’    INSERT INTO session_analytics\n    76â†’      (module_id, session_key, prompt, prompt_preview, had_project_context, prompt_length, success, duration_ms, started_at, completed_at)\n    77â†’    VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)\n    78â†’  `).run(\n    79â†’    data.moduleId,\n    80â†’    data.sessionKey,\n    81â†’    data.prompt,\n    82â†’    preview,\n    83â†’    data.hadProjectContext ? 1 : 0,\n    84â†’    data.prompt.length,\n    85â†’    data.success ? 1 : 0,\n    86â†’    data.durationMs,\n    87â†’    data.startedAt,\n    88â†’    data.completedAt,\n    89â†’  );\n    90â†’\n    91â†’  return getRecord(result.lastInsertRowid as number)!;\n    92â†’}\n    93â†’\n    94â†’export function getRecord(id: number): SessionRecord | null {\n    95â†’  ensureSessionAnalyticsTable();\n    96â†’  const row = getDb()\n    97â†’    .prepare('SELECT * FROM session_analytics WHERE id = ?')\n    98â†’    .get(id) as Record<string, unknown> | undefined;\n    99â†’  return row ? rowToRecord(row) : null;\n   100â†’}"}]},"parent_tool_use_id":"toolu_01EsENNLoCm