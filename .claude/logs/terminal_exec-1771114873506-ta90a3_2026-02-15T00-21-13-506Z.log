[2026-02-15T00:24:13.308Z] [STDOUT] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01FNcjzAqCZcs19wrYy71qoc","type":"tool_result","content":"     1â†’/**\n     2â†’ * Claude Terminal Stream API Route (CLI-based)\n     3â†’ * Event-driven SSE â€” subscribes to execution events instead of polling.\n     4â†’ */\n     5â†’\n     6â†’import { NextRequest } from 'next/server';\n     7â†’import {\n     8â†’  getExecution,\n     9â†’  startExecution,\n    10â†’  subscribeToExecution,\n    11â†’  type CLIExecutionEvent,\n    12â†’} from '@/lib/claude-terminal/cli-service';\n    13â†’\n    14â†’interface SSEEvent {\n    15â†’  type: string;\n    16â†’  data: Record<string, unknown>;\n    17â†’  timestamp: number;\n    18â†’}\n    19â†’\n    20â†’export async function GET(request: NextRequest) {\n    21â†’  const { searchParams } = new URL(request.url);\n    22â†’  const executionId = searchParams.get('executionId');\n    23â†’  const projectPath = searchParams.get('projectPath');\n    24â†’  const prompt = searchParams.get('prompt');\n    25â†’  const resumeSessionId = searchParams.get('resumeSessionId');\n    26â†’\n    27â†’  let activeExecutionId = executionId;\n    28â†’\n    29â†’  if (!activeExecutionId && projectPath && prompt) {\n    30â†’    activeExecutionId = startExecution(\n    31â†’      decodeURIComponent(projectPath),\n    32â†’      decodeURIComponent(prompt),\n    33â†’      resumeSessionId ? decodeURIComponent(resumeSessionId) : undefined\n    34â†’    );\n    35â†’  }\n    36â†’\n    37â†’  if (!activeExecutionId) {\n    38â†’    return new Response('Execution ID or (projectPath + prompt) required', { status: 400 });\n    39â†’  }\n    40â†’\n    41â†’  const encoder = new TextEncoder();\n    42â†’  let isStreamClosed = false;\n    43â†’\n    44â†’  const stream = new ReadableStream({\n    45â†’    start(controller) {\n    46â†’      const sendEvent = (event: SSEEvent) => {\n    47â†’        if (isStreamClosed) return;\n    48â†’        try {\n    49â†’          controller.enqueue(encoder.encode(`data: ${JSON.stringify(event)}\\n\\n`));\n    50â†’        } catch {\n    51â†’          isStreamClosed = true;\n    52â†’        }\n    53â†’      };\n    54â†’\n    55â†’      const closeStream = () => {\n    56â†’        if (isStreamClosed) return;\n    57â†’        isStreamClosed = true;\n    58â†’        try { controller.close(); } catch { /* already closed */ }\n    59â†’      };\n    60â†’\n    61â†’      sendEvent({\n    62â†’        type: 'connected',\n    63â†’        data: { executionId: activeExecutionId },\n    64â†’        timestamp: Date.now(),\n    65â†’      });\n    66â†’\n    67â†’      const convertEvent = (cliEvent: CLIExecutionEvent): SSEEvent => {\n    68â†’        switch (cliEvent.type) {\n    69â†’          case 'init':\n    70â†’            return { type: 'connected', data: { executionId: activeExecutionId, sessionId: cliEvent.data.sessionId, model: cliEvent.data.model, tools: cliEvent.data.tools, version: cliEvent.data.version }, timestamp: cliEvent.timestamp };\n    71â†’          case 'text':\n    72â†’            return { type: 'message', data: { type: 'assistant', content: cliEvent.data.content, model: cliEvent.data.model }, timestamp: cliEvent.timestamp };\n    73â†’          case 'tool_use':\n    74â†’            return { type: 'tool_use', data: { toolUseId: cliEvent.data.id, toolName: cliEvent.data.name, toolInput: cliEvent.data.input }, timestamp: cliEvent.timestamp };\n    75â†’          case 'tool_result':\n    76â†’            return { type: 'tool_result', data: { toolUseId: cliEvent.data.toolUseId, content: cliEvent.data.content }, timestamp: cliEvent.timestamp };\n    77â†’          case 'result':\n    78â†’            return { type: 'result', data: { sessionId: cliEvent.data.sessionId, usage: cliEvent.data.usage, durationMs: cliEvent.data.durationMs, totalCostUsd: cliEvent.data.costUsd, isError: cliEvent.data.isError }, timestamp: cliEvent.timestamp };\n    79â†’          case 'error':\n    80â†’            return { type: 'error', data: { error: cliEvent.data.message, exitCode: cliEvent.data.exitCode }, timestamp: cliEvent.timestamp };\n    81â†’          default:\n    82â†’            return { type: 'stdout', data: cliEvent.data, timestamp: cliEvent.timestamp };\n    83â†’        }\n    84â†’      };\n    85â†’\n    86â†’      // Replay any events that arrived between startExecution and this SSE connection\n    87â†’      const execution = getExecution(activeExecutionId!);\n    88â†’      if (!execution) {\n    89â†’        sendEvent({ type: 'error', data: { error: 'Execution not found' }, timestamp: Date.now() });\n    90â†’        closeStream();\n    91â†’        return;\n    92â†’      }\n    93â†’\n    94â†’      for (const event of execution.events) {\n    95â†’        if (event.type === 'stdout') continue;\n    96â†’        sendEvent(convertEvent(event));\n    97â†’        if (event.type === 'result' || event.type === 'error') {\n    98â†’          closeStream();\n    99â†’          return;\n   100â†’        }\n   101â†’      }\n   102â†’\n   103â†’      // If execution already finished during replay\n   104â†’      if (execution.status !== 'running') {\n   105â†’        sendEvent({\n   106â†’          type: execution.status === 'completed' ? 'result' : 'error',\n   107â†’          data: { status: execution.status, sessionId: execution.sessionId },\n   108â†’          timestamp: Date.now(),\n   109â†’        });\n   110â†’        closeStream();\n   111â†’        return;\n   112â†’      }\n   113â†’\n   114â†’      // Subscribe to future events (event-driven, no polling)\n   115â†’      const unsubscribe = subscribeToExecution(activeExecutionId!, (cliEvent) => {\n   116â†’        if (isStreamClosed) { unsubscribe?.(); return; }\n   117â†’        if (cliEvent.type === 'stdout') return;\n   118â†’        sendEvent(convertEvent(cliEvent));\n   119â†’        if (cliEvent.type === 'result' || cliEvent.type === 'error') {\n   120â†’          unsubscribe?.();\n   121â†’          closeStream();\n   122â†’        }\n   123â†’      });\n   124â†’\n   125â†’      if (!unsubscribe) {\n   126â†’        sendEvent({ type: 'error', data: { error: 'Failed to subscribe to execution' }, timestamp: Date.now() });\n   127â†’        closeStream();\n   128â†’        return;\n   129â†’      }\n   130â†’\n   131â†’      // Heartbeat to keep the connection alive\n   132â†’      const heartbeatInterval = setInterval(() => {\n   133â†’        if (isStreamClosed) { clearInterval(heartbeatInterval); unsubscribe(); return; }\n   134â†’        try {\n   135â†’          sendEvent({ type: 'heartbeat', data: { executionId: activeExecutionId, timestamp: Date.now() }, timestamp: Date.now() });\n   136â†’        } catch {\n   137â†’          isStreamClosed = true;\n   138â†’          clearInterval(heartbeatInterval);\n   139â†’          unsubscribe();\n   140â†’        }\n   141â†’      }, 15000);\n   142â†’\n   143â†’      // Safety timeout: if execution takes too long, clean up\n   144â†’      setTimeout(() => {\n   145â†’        if (!isStreamClosed) {\n   146â†’          unsubscribe();\n   147â†’          clearInterval(heartbeatInterval);\n   148â†’          closeStream();\n   149â†’        }\n   150â†’      }, 6100000); // Slightly longer than the 100-minute execution timeout\n   151â†’    },\n   152â†’    cancel() {\n   153â†’      isStreamClosed = true;\n   154â†’    },\n   155â†’  });\n   156â†’\n   157â†’  return new Response(stream, {\n   158â†’    headers: {\n   159â†’      'Content-Type': 'text/event-stream',\n   160â†’      'Cache-Control': 'no-cache, no-transform',\n   161â†’      'Connection': 'keep-alive',\n   162â†’      'X-Accel-Buffering': 'no',\n   163â†’    },\n   164â†’  });\n   165â†’}\n   166â†’"}]},"parent_tool_use_id":"toolu_01EsENNLoCmE98gLcj81XK6C","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"e6d12c9c-f3e1-408e-8ffe-6a42d46718d3"}
