WHERE module_id = ? ORDER BY category, feature_name')\n    50→    .all(moduleId) as RawRow[];\n    51→  return rows.map(toFeatureRow);\n    52→}\n    53→\n    54→export function getFeatureSummary(moduleId: string): FeatureSummary {\n    55→  const rows = getDb()\n    56→    .prepare(\n    57→      `SELECT status, COUNT(*) as cnt FROM feature_matrix WHERE module_id = ? GROUP BY status`\n    58→    )\n    59→    .all(moduleId) as { status: string; cnt: number }[];\n    60→\n    61→  const summary: FeatureSummary = { total: 0, implemented: 0, partial: 0, missing: 0, unknown: 0 };\n    62→  for (const row of rows) {\n    63→    const count = row.cnt;\n    64→    summary.total += count;\n    65→    if (row.status in summary) {\n    66→      summary[row.status as keyof Omit<FeatureSummary, 'total'>] = count;\n    67→    }\n    68→  }\n    69→  return summary;\n    70→}\n    71→\n    72→export interface UpsertFeature {\n    73→  featureName: string;\n    74→  category: string;\n    75→  status: FeatureStatus;\n    76→  description: string;\n    77→  filePaths: string[];\n    78→  reviewNotes: string;\n    79→  qualityScore?: number | null;\n    80→  nextSteps?: string;\n    81→  lastReviewedAt?: string | null;\n    82→}\n    83→\n    84→export function upsertFeatures(moduleId: string, features: UpsertFeature[]): void {\n    85→  const db = getDb();\n    86→  const stmt = db.prepare(`\n    87→    INSERT INTO feature_matrix (module_id, feature_name, category, status, description, file_paths, review_notes, quality_score, next_steps, last_reviewed_at, updated_at)\n    88→    VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?, datetime('now'))\n    89→    ON CONFLICT(module_id, feature_name) DO UPDATE SET\n    90→      category = excluded.category,\n    91→      status = excluded.status,\n    92→      description = excluded.description,\n    93→      file_paths = excluded.file_paths,\n    94→      review_notes = excluded.review_notes,\n    95→      quality_score = excluded.quality_score,\n  