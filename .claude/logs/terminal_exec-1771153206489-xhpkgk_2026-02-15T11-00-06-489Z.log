5→  missing: number;\n   156→  unknown: number;\n   157→  avgQuality: number | null;\n   158→}\n   159→\n   160→export function captureReviewSnapshot(moduleId: string): void {\n   161→  const db = getDb();\n   162→  const row = db\n   163→    .prepare(\n   164→      `SELECT\n   165→         COUNT(*) as total,\n   166→         SUM(CASE WHEN status = 'implemented' THEN 1 ELSE 0 END) as implemented,\n   167→         SUM(CASE WHEN status = 'partial' THEN 1 ELSE 0 END) as partial,\n   168→         SUM(CASE WHEN status = 'missing' THEN 1 ELSE 0 END) as missing,\n   169→         SUM(CASE WHEN status = 'unknown' THEN 1 ELSE 0 END) as unknown,\n   170→         AVG(CASE WHEN quality_score IS NOT NULL THEN quality_score END) as avg_quality,\n   171→         MAX(last_reviewed_at) as last_reviewed\n   172→       FROM feature_matrix\n   173→       WHERE module_id = ?`,\n   174→    )\n   175→    .get(moduleId) as {\n   176→    total: number;\n   177→    implemented: number;\n   178→    partial: number;\n   179→    missing: number;\n   180→    unknown: number;\n   181→    avg_quality: number | null;\n   182→    last_reviewed: string | null;\n   183→  };\n   184→\n   185→  if (!row || row.total === 0) return;\n   186→\n   187→  db.prepare(\n   188→    `INSERT INTO review_snapshots (module_id, reviewed_at, total, implemented, partial, missing, unknown, avg_quality)\n   189→     VALUES (?, ?, ?, ?, ?, ?, ?, ?)`,\n   190→  ).run(\n   191→    moduleId,\n   192→    row.last_reviewed ?? new Date().toISOString(),\n   193→    row.total,\n   194→    row.implemented,\n   195→    row.partial,\n   196→    row.missing,\n   197→    row.unknown,\n   198→    row.avg_quality,\n   199→  );\n   200→}\n   201→\n   202→export function getReviewHistory(moduleId: string, limit = 20): ReviewSnapshot[] {\n   203→  const rows = getDb()\n   204→    .prepare(\n   205→      `SELECT id, module_id, reviewed_at, total, implemented, partial, missing, unknown, avg_quality\n   206→       FROM review_snapshots\n   207→       WHERE module_id = ?\n   208→       ORDER BY reviewed_at ASC\n   209→       LIMIT ?`,\n   210→    )\n   211→    .all(moduleId, limit) as {\n   212→    id: number;\n   213→    module_id: string;\n   214→    reviewed_at: string;\n   215→    total: number;\n   216→    implemented: number;\n   217→    partial: number;\n   218→    missing: number;\n   219→    unknown: number;\n   220→    avg_quality: number | null;\n   221→  }[];\n   222→\n   223→  return rows.map((r) => ({\n   224→    id: r.id,\n   225→    moduleId: r.module_id,\n   226→    reviewedAt: r.reviewed_at,\n   227→    total: r.total,\n   228→    implemented: r.implemented,\n   229→    partial: r.partial,\n   230→    missing: r.missing,\n   231→    unknown: r.unknown,\n   232→    avgQuality: r.avg_quality !== null ? Math.round(r.avg_quality * 10) / 10 : null,\n   233→  }));\n   234→}\n   235→\n   236→export function getAllReviewHistory(limit = 20): Record<string, ReviewSnapshot[]> {\n   237→  const rows = getDb()\n   238→    .prepare(\n   239→      `SELECT id, module_id, reviewed_at, total, implemented, partial, missing, unknown, avg_quality\n   240→       FROM (\n   241→         SELECT *, ROW_NUMBER() OVER (PARTITION BY module_id ORDER BY reviewed_at DESC) as rn\n   242→         FROM review_snapshots\n   243→       )\n   244→       WHERE rn <= ?\n   245→       ORDER BY module_id, reviewed_at ASC`,\n   246→    )\n   247→    .all(limit) as {\n   248→    id: number;\n   249→    module_id: string;"}]},"parent_tool_use_id":"toolu_01EjzwQwjnQnRkHF9NgL3t2v","session_id":"a278ac89-7d0c-4fa9-b1bf-860beb088d62","uuid":"d70f416a-a31b-4374-b9d0-6890dc163cd1"}
