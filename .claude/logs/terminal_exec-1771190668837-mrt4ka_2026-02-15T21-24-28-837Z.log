ning\n    38→  useEffect(() => {\n    39→    const handler = (e: BeforeUnloadEvent) => {\n    40→      const sessions = useCLIPanelStore.getState().sessions;\n    41→      const hasRunning = Object.values(sessions).some((s) => s.isRunning);\n    42→      if (hasRunning) {\n    43→        e.preventDefault();\n    44→      }\n    45→    };\n    46→    window.addEventListener('beforeunload', handler);\n    47→    return () => window.removeEventListener('beforeunload', handler);\n    48→  }, []);\n    49→\n    50→  // Wait for Zustand persist to rehydrate from localStorage\n    51→  useEffect(() => {\n    52→    setHydrated(true);\n    53→  }, []);\n    54→\n    55→  if (!hydrated) {\n    56→    return (\n    57→      <div className=\"h-screen flex items-center justify-center bg-background\">\n    58→        <motion.div\n    59→          initial={{ opacity: 0 }}\n    60→          animate={{ opacity: 1 }}\n    61→          transition={{ duration: 0.22 }}\n    62→          className=\"flex flex-col items-center gap-3\"\n    63→        >\n    64→          <Gamepad2 className=\"w-12 h-12 text-[#00ff88]\" />\n    65→          <span className=\"text-lg font-semibold text-text tracking-wide\">\n    66→            POF\n    67→          </span>\n    68→          <div className=\"w-5 h-5 border-2 border-accent-strong border-t-[#00ff88] rounded-full animate-spin\" />\n    69→        </motion.div>\n    70→      </div>\n    71→    );\n    72→  }\n    73→\n    74→  if (!isSetupComplete) {\n    75→    return <SetupWizard />;\n    76→  }\n    77→\n    78→  return (\n    79→    <div className=\"h-screen flex flex-col overflow-hidden bg-background\">\n    80→      <TopBar />\n    81→      <div className=\"flex-1 flex overflow-hidden\">\n    82→        <Sidebar />\n    83→        <ModuleRenderer />\n    84→        <ActivityFeedPanel />\n    85→      </div>\n    86→      <CLIBottomPanel />\n    87→      <GlobalSearchPanel />\n    88→      <EventBusDevTools />\n    89→    </div>\n    90→  );\n    91→}\n    92→"}]},"parent_tool_use_id":"toolu_0128nKw4DLQTstmXKQAhgvhd","session_id":"b4e1396f-43a9-4be7-ae62-7daeaae74aee","uuid":"72d34358-7c3d-4757-87a7-2d976115285c"}
