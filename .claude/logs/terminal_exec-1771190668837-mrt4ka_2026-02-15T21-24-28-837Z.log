→  if (isNaN(then)) return 'Unknown';\n   497→\n   498→  const minutes = Math.floor(diffMs / 60000);\n   499→  if (minutes < 1) return 'Just now';\n   500→  if (minutes < 60) return `${minutes}m ago`;\n   501→\n   502→  const hours = Math.floor(minutes / 60);\n   503→  if (hours < 24) return `${hours}h ago`;\n   504→\n   505→  const days = Math.floor(hours / 24);\n   506→  if (days < 7) return `${days}d ago`;\n   507→\n   508→  const weeks = Math.floor(days / 7);\n   509→  if (weeks < 5) return `${weeks}w ago`;\n   510→\n   511→  const months = Math.floor(days / 30);\n   512→  return `${months}mo ago`;\n   513→}\n   514→\n   515→// --- Project stats summary ---\n   516→\n   517→// Precompute total checklist items per module (static — never changes at runtime)\n   518→const MODULE_CHECKLIST_COUNTS: { moduleId: string; total: number }[] = SUB_MODULES\n   519→  .filter((m) => m.checklist && m.checklist.length > 0)\n   520→  .map((m) => ({ moduleId: m.id, total: m.checklist!.length }));\n   521→\n   522→const TOTAL_CHECKLIST_ITEMS = MODULE_CHECKLIST_COUNTS.reduce((sum, m) => sum + m.total, 0);\n   523→const MODULE_ITEM_IDS: Record<string, string[]> = Object.fromEntries(\n   524→  SUB_MODULES\n   525→    .filter((m) => m.checklist && m.checklist.length > 0)\n   526→    .map((m) => [m.id, m.checklist!.map((c) => c.id)]),\n   527→);\n   528→\n   529→const EMPTY_PROGRESS: Record<string, Record<string, boolean>> = {};\n   530→\n   531→function ProjectStats() {\n   532→  const checklistProgress = useModuleStore((s) => s.checklistProgress) || EMPTY_PROGRESS;\n   533→\n   534→  const stats = useMemo(() => {\n   535→    let completed = 0;\n   536→    let modulesComplete = 0;\n   537→\n   538→    for (const { moduleId, total } of MODULE_CHECKLIST_COUNTS) {\n   539→      const progress = checklistProgress[moduleId];\n   540→      if (!progress) continue;\n   541→\n   542→      const itemIds = MODULE_ITEM_IDS[moduleId];\n   543→      let moduleCompleted = 0;\n   544→      for (cons