</select>\n   629→      <span className=\"text-text-muted\">-</span>\n   630→      <select\n   631→        value={max}\n   632→        onChange={(e) => {\n   633→          const v = parseInt(e.target.value);\n   634→          onMaxChange(v);\n   635→          if (v < min) onMinChange(v);\n   636→        }}\n   637→        className=\"bg-transparent text-text text-xs outline-none cursor-pointer [&>option]:bg-surface-hover\"\n   638→      >\n   639→        {[1, 2, 3, 4, 5].map((n) => (\n   640→          <option key={n} value={n}>{n}</option>\n   641→        ))}\n   642→      </select>\n   643→    </div>\n   644→  );\n   645→}\n   646→\n   647→function SortButton({\n   648→  label,\n   649→  sortKey: key,\n   650→  currentKey,\n   651→  currentDir,\n   652→  onToggle,\n   653→}: {\n   654→  label: string;\n   655→  sortKey: SortKey;\n   656→  currentKey: SortKey;\n   657→  currentDir: SortDir;\n   658→  onToggle: (key: SortKey) => void;\n   659→}) {\n   660→  const isActive = currentKey === key;\n   661→  const SortDirIcon = isActive ? (currentDir === 'asc' ? ArrowUp : ArrowDown) : ArrowUpDown;\n   662→\n   663→  return (\n   664→    <button\n   665→      onClick={() => onToggle(key)}\n   666→      className=\"flex items-center gap-1 px-1.5 py-1 rounded text-xs font-medium transition-all\"\n   667→      style={{\n   668→        color: isActive ? 'var(--text)' : 'var(--text-muted)',\n   669→        backgroundColor: isActive ? 'var(--border)' : 'transparent',\n   670→      }}\n   671→    >\n   672→      {label}\n   673→      <SortDirIcon className=\"w-3 h-3\" style={{ opacity: isActive ? 1 : 0.4 }} />\n   674→    </button>\n   675→  );\n   676→}\n   677→\n   678→function formatRelativeTime(dateStr: string): { label: string; dotColor: string; isOutdated: boolean } {\n   679→  const ms = Date.now() - new Date(dateStr).getTime();\n   680→  const minutes = Math.floor(ms / 60000);\n   681→  const hours = Math.floor(ms / 3600000);\n   682→  const days = Math.floor(ms / 8640000