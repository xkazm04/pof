stItem);\n    47→\n    48→  const allTabIds = ['overview', 'roadmap', ...extraTabs.map((t) => t.id)];\n    49→  const [activeTab, setActiveTab] = useState(allTabIds[0]);\n    50→  const [toast, setToast] = useState<{ message: string; type: 'success' | 'error' } | null>(null);\n    51→\n    52→  useEffect(() => {\n    53→    if (!toast) return;\n    54→    const timer = setTimeout(() => setToast(null), 3000);\n    55→    return () => clearTimeout(timer);\n    56→  }, [toast]);\n    57→\n    58→  // --- Checklist CLI session ---\n    59→  const [activeItemId, setActiveItemId] = useState<string | null>(null);\n    60→  const [lastCompletedItemId, setLastCompletedItemId] = useState<string | null>(null);\n    61→\n    62→  const handleChecklistComplete = useCallback((success: boolean) => {\n    63→    if (success && activeItemId) {\n    64→      setChecklistItem(moduleId, activeItemId, true);\n    65→      setLastCompletedItemId(activeItemId);\n    66→      setTimeout(() => setLastCompletedItemId(null), 2000);\n    67→    }\n    68→    setActiveItemId(null);\n    69→  }, [activeItemId, moduleId, setChecklistItem]);\n    70→\n    71→  const checklistCli = useModuleCLI({\n    72→    moduleId,\n    73→    sessionKey: `${moduleId}-cli`,\n    74→    label: moduleLabel,\n    75→    accentColor,\n    76→    onComplete: handleChecklistComplete,\n    77→  });\n    78→\n    79→  const sendChecklistPrompt = useCallback(async (itemId: string, prompt: string) => {\n    80→    setActiveItemId(itemId);\n    81→    const task = TaskFactory.checklist(moduleId, itemId, prompt, moduleLabel);\n    82→    checklistCli.execute(task);\n    83→  }, [checklistCli, moduleId, moduleLabel]);\n    84→\n    85→  // --- Review CLI session ---\n    86→  const [reviewRefetchTrigger, setReviewRefetchTrigger] = useState(0);\n    87→\n    88→  const handleReviewComplete = useCallback(async (success: boolean) => {\n    89→    if (!success) return;\n    90→    // Claude POSTs results directly to the import API during the session,\n    91→    // so by the time this fires the data is already in the DB.\n    92→    // Just refresh the UI after a short delay for the DB write to settle.\n    93→    await new Promise((r) => setTimeout(r, 300));\n    94→    setReviewRefetchTrigger((n) => n + 1);\n    95→    setToast({ message: 'Review complete — features updated', type: 'success' });\n    96→  }, []);\n    97→\n    98→  const reviewCli = useModuleCLI({\n    99→    moduleId,\n   100→    sessionKey: `${moduleId}-review`,\n   101→    label: `${moduleLabel} Review`,\n   102→    accentColor,\n   103→    onComplete: handleReviewComplete,\n   104→  });\n   105→\n   106→  // --- Fix CLI session ---\n   107→  const fixCli = useModuleCLI({\n   108→    moduleId,\n   109→    sessionKey: `${moduleId}-fix`,\n   110→    label: `${moduleLabel} Fix`,\n   111→    accentColor,\n   112→  });\n   113→\n   114→  const handleFix = useCallback(async (feature: FeatureRow) => {\n   115→    if (!feature.nextSteps) return;\n   116→    const task = TaskFactory.featureFix(moduleId, feature, `${moduleLabel} Fix`);\n   117→    fixCli.execute(task);\n   118→  }, [fixCli, moduleId, moduleLabel]);\n   119→\n   120→  const handleSync = useCallback(async () => {\n   121→    try {\n   122→      const res = await fetch('/api/feature-matrix/import', {\n   123→        method: 'POST',\n   124→        headers: { 'Content-Type': 'application/json' },\n   125→        body: JSON.stringify({ moduleId, projectPath }),\n   126→      });\n   127→      if (!res.ok) {\n   128→        const err = await res.json().catch(() => ({ error: 'Sync failed' }));\n   129→        setToast({ message: err.error ?? `Sync failed (${res.status})`, type: 'error' });\n   130→        return;\n   131→      }\n   132→      const data = await res.json();\n   133→      setToast({ message: `Imported ${data.imported} features`, type: 'success' });\n   134→    } catch (err) {\n   135→      setToast({ message: err instanceof Error ? err.message : 'Failed to sync review results', type: 'error' });\n   136→    }\n   137→  }, [moduleId, projectPath]);\n   138→\n   139→  const startReview = useCallback(async () => {\n   140→    const defs = MODULE_FEATURE_DEFINITIONS[moduleId] ?? [];\n   141→    if (defs.length === 0) return;\n   142→    const appOrigin = typeof window !== 'undefined' ? window.location.origin : 'http://localhost:3000';\n   143→    const task = TaskFactory.featureReview(moduleId, moduleLabel, defs, appOrigin, `${moduleLabel} Review`);\n   144→    reviewCli.execute(task);\n   145→  }, [moduleId, moduleLabel, reviewCli]);\n   146→\n   147→  const isAnyRunning = checklistCli.isRunning || reviewCli.isRunning || fixCli.isRunning;\n   148→\n   149→  return (\n   150→    <div className=\"flex h-full relative\">\n   151→      {/* Main content */}\n   152→      <div className=\"flex-1 overflow-y-auto p-6 min-w-0\">\n   153→        {/* Header */}\n   154→        <div className=\"flex items-center gap-3 mb-4\">\n   155→          <div className=\"relative flex items-center\">\n   156→            <Icon className=\"w-6 h-6\" style={{ color: accentColor }} />\n   157→            <div className=\"ml-0.5\">\n   158→              <ContextHealthBadge />\n   159→            </div>\n   160→          </div>\n   161→          <div>\n   162→            <h1 className=\"text-lg font-semibold text-text\">{moduleLabel}</h1>\n   163→            <p className=\"text-xs text-text-muted\">{moduleDescription}</p>\n   164→          </div>\n   165→        </div>\n   166→\n   167→        {/* Tab bar */}\n   168→        <div className=\"flex items-center gap-1 mb-5 border-b border-border\">\n   169→          <TabButton label=\"Overview\" active={activeTab === 'overview'} onClick={() => setActiveTab('overview')} accentColor={accentColor} />\n   170→          <TabButton label=\"Roadmap\" active={activeTab === 'roadmap'} onClick={() => setActiveTab('roadmap')} accentColor={accentColor} />\n   171→          {extraTabs.map((tab) => (\n   172→            <TabButton key={tab.id} label={tab.label} active={activeTab === tab.id} onClick={() => setActiveTab(tab.id)} accentColor={accentColor} />\n   173→          ))}\n   174→        </div>\n   175→\n   176→        {/* Tab content */}\n   177→        {activeTab === 'overview' && (\n   178→          <FeatureMatrix\n   179→            key={reviewRefetchTrigger}\n   180→            moduleId={moduleId}\n   181→            accentColor={accentColor}\n   182→            onReview={startReview}\n   183→            onSync={handleSync}\n   184→            isReviewing={reviewCli.isRunning}\n   185→            onFix={handleFix}\n   186→            isFixing={fixCli.isRunning}\n   187→          />\n   188→        )}\n   189→\n   190→        {activeTab === 'roadmap' && (\n   191→          checklist.length > 0 ? (\n   192→            <RoadmapChecklist\n   193→              items={checklist}\n   194→              subModuleId={moduleId}\n   195→              onRunPrompt={sendChecklistPrompt}\n   196→              accentColor={accentColor}\n   197→              isRunning={checklistCli.isRunning}\n   198→              activeItemId={activeItemId}\n   199→              lastCompletedItemId={lastCompletedItemId}\n   200→            />\n   201→          ) : (\n   202→            <p className=\"text-sm text-text-muted\">No checklist items defined for this module yet.</p>\n   203→          )\n   204→        )}\n   205→\n   206→        {extraTabs.map((tab) => (\n   207→          activeTab === tab.id ? <div key={tab.id}>{tab.render()}</div> : null\n   208→        ))}\n   209→      </div>\n   210→\n   211→      {/* Right panel — Quick Actions */}\n   212→      <div className=\"w-56 border-l border-border bg-surface-deep flex-shrink-0\">\n   213→        <QuickActionsPanel\n   214→          actions={quickActions}\n   215→          onRunPrompt={(prompt) => {\n   216→            const task = TaskFactory.quickAction(moduleId, prompt, moduleLabel);\n   217→            checklistCli.execute(task);\n   218→          }}\n   219→          accentColor={accentColor}\n   220→          isRunning={isAnyRunning}\n   221→          moduleLabel={moduleLabel}\n   222→          moduleId={moduleId}\n   223→        />\n   224→      </div>\n   225→\n   226→      {/* Toast notification */}\n   227→      {toast && (\n   228→        <div\n   229→          className={`absolute bottom-4 right-4 z-50 flex items-center gap-2 px-3 py-2 rounded-lg text-xs font-medium shadow-lg border animate-in fade-in slide-in-from-bottom-2 ${\n   230→            toast.type === 'success'\n   231→              ? 'bg-surface border-green-500/30 text-green-400'\n   232→              : 'bg-surface border-red-500/30 text-red-400'\n   233→          }`}\n   234→        >\n   235→          <span\n   236→            className=\"w-1.5 h-1.5 rounded-full flex-shrink-0\"\n   237→            style={{ backgroundColor: toast.type === 'success' ? '#4ade80' : '#f87171' }}\n   238→          />\n   239→          {toast.message}\n   240→        </div>\n   241→      )}\n   242→    </div>\n   243→  );\n   244→}\n   245→\n   246→function TabButton({ label, active, onClick, accentColor }: {\n   247→  label: string;\n   248→  active: boolean;\n   249→  onClick: () => void;\n   250→  accentColor: string;\n   251→}) {\n   252→  return (\n   253→    <button\n   254→      onClick={onClick}\n   255→      className={`px-4 py-2 text-xs font-medium transition-colors relative ${\n   256→        active ? 'text-text' : 'text-text-muted hover:text-text'\n   257→      }`}\n   258→    >\n   259→      {label}\n   260→      {active && (\n   261→        <span\n   262→          className=\"absolute bottom-0 left-0 right-0 h-0.5 rounded-t\"\n   263→          style={{ backgroundColor: accentColor }}\n   264→        />\n   265→      )}\n   266→    </button>\n   267→  );\n   268→}\n   269→"}]},"parent_tool_use_id":"toolu_01UKUfdD3mufnHPTSjdHijKa","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"8c90d1cd-eddf-45cf-98f7-119faefdf5ef"}
