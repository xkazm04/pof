 690→  // Green <24h, amber 1-7d, red >7d\n   691→  const dotColor = hours < 24 ? '#4ade80' : days <= 7 ? '#fbbf24' : '#f87171';\n   692→  const isOutdated = days > 7;\n   693→\n   694→  return { label, dotColor, isOutdated };\n   695→}\n   696→\n   697→function SummaryBar({ summary }: { summary: { total: number; implemented: number; partial: number; missing: number; unknown: number } }) {\n   698→  if (summary.total === 0) return null;\n   699→\n   700→  const segments: { status: FeatureStatus; count: number }[] = [\n   701→    { status: 'implemented', count: summary.implemented },\n   702→    { status: 'partial', count: summary.partial },\n   703→    { status: 'missing', count: summary.missing },\n   704→    { status: 'unknown', count: summary.unknown },\n   705→  ];\n   706→\n   707→  return (\n   708→    <div className=\"flex-1 space-y-1.5\">\n   709→      <div className=\"flex items-center justify-between\">\n   710→        <div className=\"flex items-center gap-3\">\n   711→          {segments.map((s) =>\n   712→            s.count > 0 ? (\n   713→              <span key={s.status} className=\"flex items-center gap-1 text-2xs\" style={{ color: STATUS_CONFIG[s.status].color }}>\n   714→                <span className=\"w-1.5 h-1.5 rounded-full\" style={{ backgroundColor: STATUS_CONFIG[s.status].color }} />\n   715→                {s.count} {STATUS_CONFIG[s.status].label.toLowerCase()}\n   716→              </span>\n   717→            ) : null\n   718→          )}\n   719→        </div>\n   720→        <span className=\"text-xs text-text-muted-hover\">\n   721→          {summary.implemented}/{summary.total}\n   722→        </span>\n   723→      </div>\n   724→      <div className=\"h-1.5 bg-border rounded-full overflow-hidden flex\">\n   725→        {segments.map((s) =>\n   726→          s.count > 0 ? (\n   727→            <div\n   728→              key={s.status}\n   729→              className=\"h-full transition-all duration-500\"\n   730→              style={{\n   731→                width: `${(s.count / summary.total) * 100}%`,\n   732→                backgroundColor: STATUS_CONFIG[s.status].color,\n   733→                opacity: 0.8,\n   734→              }}\n   735→            />\n   736→          ) : null\n   737→        )}\n   738→      </div>\n   739→    </div>\n   740→  );\n   741→}\n   742→\n   743→function StatusFilterChips({\n   744→  summary,\n   745→  activeFilters,\n   746→  onToggle,\n   747→}: {\n   748→  summary: { total: number; implemented: number; partial: number; missing: number; unknown: number };\n   749→  activeFilters: Set<FeatureStatus>;\n   750→  onToggle: (status: FeatureStatus) => void;\n   751→}) {\n   752→  if (summary.total === 0) return null;\n   753→\n   754→  const statuses: FeatureStatus[] = ['implemented', 'partial', 'missing', 'unknown'];\n   755→\n   756→  return (\n   757→    <div className=\"flex items-center gap-1.5 flex-wrap\">\n   758→      {statuses.map((status) => {\n   759→        const cfg = STATUS_CONFIG[status];\n   760→        const count = summary[status];\n   761→        const isActive = activeFilters.has(status);\n   762→\n   763→        return (\n   764→          <button\n   765→            key={status}\n   766→            onClick={() => onToggle(status)}\n   767→            className=\"flex items-center gap-1.5 px-2.5 py-1 rounded-full text-xs font-medium transition-all\"\n   768→            style={\n   769→              isActive\n   770→                ? {\n   771→                    backgroundColor: `${cfg.color}20`,\n   772→                    color: cfg.color,\n   773→                    border: `1px solid ${cfg.color}50`,\n   774→                  }\n   775→                : {\n   776→                    backgroundColor: 'transparent',\n   777→                    color: `${cfg.color}80`,\n   778→                    border: `1px solid ${cfg.color}25`,\n   779→                  }\n   780→            }\n   781→          >\n   782→            <span\n   783→              className=\"w-1.5 h-1.5 rounded-full\"\n   784→              style={{\n   785→                backgroundColor: isActive ? cfg.color : `${cfg.color}50`,\n   786→              }}\n   787→            />\n   788→            {cfg.label}\n   789→            <span style={{ opacity: 0.7 }}>{count}</span>\n   790→          </button>\n   791→        );\n   792→      })}\n   793→    </div>\n   794→  );\n   795→}\n   796→\n   797→function QualitySparkline({\n   798→  snapshots,\n   799→  accentColor,\n   800→}: {\n   801→  snapshots: ReviewSnapshot[];\n   802→  accentColor: string;\n   803→}) {\n   804→  const qualityPoints = snapshots\n   805→    .map((s) => s.avgQuality)\n   806→    .filter((q): q is number => q !== null);\n   807→\n   808→  if (qualityPoints.length < 2) return null;\n   809→\n   810→  const w = 64;\n   811→  const h = 24;\n   812→  const pad = 2;\n   813→  const min = Math.max(0, Math.min(...qualityPoints) - 0.5);\n   814→  const max = Math.min(5, Math.max(...qualityPoints) + 0.5);\n   815→  const range = max - min || 1;\n   816→\n   817→  const points = qualityPoints.map((q, i) => {\n   818→    const x = pad + (i / (qualityPoints.length - 1)) * (w - pad * 2);\n   819→    const y = h - pad - ((q - min) / range) * (h - pad * 2);\n   820→    return { x, y };\n   821→  });\n   822→\n   823→  const pathD = points.map((p, i) => `${i === 0 ? 'M' : 'L'}${p.x},${p.y}`).join(' ');\n   824→\n   825→  // Trend: compare last vs first\n   826→  const first = qualityPoints[0];\n   827→  const last = qualityPoints[qualityPoints.length - 1];\n   828→  const trend = last - first;\n   829→  const TrendIcon = trend > 0.2 ? TrendingUp : trend < -0.2 ? TrendingDown : Minus;\n   830→  const trendColor = trend > 0.2 ? '#4ade80' : trend < -0.2 ? '#f87171' : 'var(--text-muted)';\n   831→\n   832→  return (\n   833→    <div className=\"flex items-center gap-1.5 flex-shrink-0\" title={`Quality trend: ${qualityPoints.map((q) => q.toFixed(1)).join(' → ')}`}>\n   834→      <svg w