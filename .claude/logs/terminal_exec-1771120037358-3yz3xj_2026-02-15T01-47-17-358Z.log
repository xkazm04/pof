unt\n    37→  useEffect(() => {\n    38→    fetchSuggestions(subModuleId);\n    39→  }, [fetchSuggestions, subModuleId]);\n    40→\n    41→  const completedCount = items.filter((item) => progress[item.id]).length;\n    42→  const progressPercent = items.length > 0 ? (completedCount / items.length) * 100 : 0;\n    43→\n    44→  return (\n    45→    <div className=\"space-y-4\">\n    46→      {/* Progress header */}\n    47→      <div className=\"space-y-2\">\n    48→        <div className=\"flex items-center justify-between text-xs\">\n    49→          <span className=\"text-text-muted\">Progress</span>\n    50→          <span className=\"text-text font-medium\">{completedCount} / {items.length}</span>\n    51→        </div>\n    52→        <div className=\"h-1.5 bg-border rounded-full overflow-hidden\">\n    53→          <div\n    54→            className=\"h-full rounded-full transition-all duration-500 ease-out\"\n    55→            style={{ width: `${progressPercent}%`, backgroundColor: accentColor }}\n    56→          />\n    57→        </div>\n    58→      </div>\n    59→\n    60→      {/* Checklist items */}\n    61→      <StaggerContainer className=\"space-y-2\">\n    62→        {items.map((item, index) => {\n    63→          const checked = !!progress[item.id];\n    64→          const isActive = activeItemId === item.id;\n    65→          const justCompleted = lastCompletedItemId === item.id;\n    66→          const matchingSuggestion = !checked && !isActive\n    67→            ? suggestions.find((s) =>\n    68→                item.label.toLowerCase().includes(s.pattern.title.toLowerCase().split(' ')[0]) ||\n    69→                s.pattern.tags.some((t) => item.label.toLowerCase().includes(t)),\n    70→              )\n    71→            : undefined;\n    72→          return (\n    73→            <StaggerItem\n    74→              key={item.id}\n    75→              className={`group p-3 rounded-lg border transition-all duration-300 ${\n    76→                justCompleted\n    77→                  ? 'bg-green-900/20 border-green-500/40'\n    78→                  : checked\n    79→                    ? 'bg-[#0d1a0d] border-[#1a3a1a]'\n    80→                    : isActive\n    81→                      ? 'bg-[#111130] border-[#2e2e6a]'\n    82→                      : 'bg-surface border-border hover:border-border-bright'\n    83→              }`}\n    84→              onMouseEnter={() => setHoveredItemId(item.id)}\n    85→              onMouseLeave={() => setHoveredItemId(null)}\n    86→            >\n    87→              <div className=\"flex items-start gap-3\">\n    88→                {/* Checkbox */}\n    89→                <button\n    90→                  onClick={() => toggleItem(subModuleId, item.id)}\n    91→                  className={`mt-0.5 flex-shrink-0 w-5 h-5 rounded border-2 flex items-center justify-center transition-all ${\n    92→                    checked\n    93→                      ? 'border-green-500 bg-green-500/20'\n    94→                      : 'border-[#3e3e6a] hover:border-[#5e5e8a]'\n    95→                  }`}\n    96→                >\n    97→                  {checked && <Check className=\"w-3 h-3 text-green-400\" />}\n    98→                </button>\n    99→\n   100→                {/* Content */}\n   101→                <div className=\"flex-1 min-w-0\">\n   102→                  <div className=\"flex items-center gap-2\">\n   103→                    <span className=\"text-xs text-text-muted font-mono\">{index + 1}.</span>\n   104→                    <span className={`text-xs font-medium ${checked ? 'text-text-muted line-through' : 'text-text'}`}>\n   105→                      {item.label}\n   106→                    </span>\n   107→                    {isActive && (\n   108→                      <span className=\"flex items-center gap-1 text-2xs px-1.5 py-0.5 rounded bg-[#00ff88]/10 text-[#00ff88]\">\n   109→                        <Loader2 className=\"w-2.5 h-2.5 animate-spin\" />\n   110→                        running\n   111→                      </span>\n   112→                    )}\n   113→                    {justCompleted && (\n   114→                      <span className=\"text-2xs px-1.5 py-0.5 rounded bg-green-500/15 text-green-400 animate-pulse\">\n   115→                        done\n   116→                      </span>\n   117→                    )}\n   118→                  </div>\n   119→                  <p className={`text-xs mt-1 leading-relaxed ${checked ? 'text-[#4a4e6a]' : 'text-text-muted'}`}>\n   120→                    {item.description}\n   121→                  </p>\n   122→                  {/* Pattern suggestion hint */}\n   123→                  {matchingSuggestion && hoveredItemId === item.id && (\n   124→                    <div className=\"flex items-center gap-1.5 mt-1.5 px-2 py-1 bg-violet-400/5 border border-violet-400/15 rounded text-2xs text-violet-400\">\n   125→                      <Sparkles className=\"w-3 h-3 flex-shrink-0\" />\n   126→                      <span>\n   127→                        <strong>{Math.round(matchingSuggestion.pattern.successRate * 100)}% success</strong>\n   128→                        {' — '}\n   129→                        {matchingSuggestion.pattern.approach} approach\n   130→                        {matchingSuggestion.pattern.sessionCount > 1 && ` (${matchingSuggestion.pattern.sessionCount} sessions)`}\n   131→                      </span>\n   132→                    </div>\n   133→                  )}\n   134→                </div>\n   135→\n   136→                {/* Do it with Claude button */}\n   137→                {!checked && !isActive && (\n   138→                  <button\n   139→                    onClick={() => onRunPrompt(item.id, item.prompt)}\n   140→                    disabled={isRunning}\n   141→                    className=\"flex-shrink-0 flex items-center gap-1.5 px-2.5 py-1.5 rounded-md text-xs font-medium transition-all opacity-0 group-hover:opacity-100 disabled:opacity-50\"\n   142→                    style={{\n   143→                      backgroundColor: `${accentColor}15`,\n   144→                      color: accentColor,\n   145→                      border: `1px solid ${accentColor}30`,\n   146→                    }}\n   147→                  >\n   148→                    <Play className=\"w-3 h-3\" />\n   149→                    Claude\n   150→                  </button>\n   151→                )}\n   152→              </div>\n   153→            </StaggerItem>\n   154→          );\n   155→        })}\n   156→      </StaggerContainer>\n   157→    </div>\n   158→  );\n   159→}\n   160→"}]},"parent_tool_use_id":"toolu_01UKUfdD3mufnHPTSjdHijKa","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"e1d539d8-2108-417e-9cb6-b5ab4d2cf243"}
