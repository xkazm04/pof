, [fetchDashboard]);\n    45‚Üí\n    46‚Üí  return { dashboard, isLoading, error, retry: fetchDashboard, refetch: fetchDashboard };\n    47‚Üí}\n    48‚Üí\n    49‚Üí// ‚îÄ‚îÄ Prompt suggestions hook ‚îÄ‚îÄ\n    50‚Üí\n    51‚Üíexport function usePromptSuggestions(moduleId: string) {\n    52‚Üí  const [suggestions, setSuggestions] = useState<PromptSuggestion[]>([]);\n    53‚Üí  const timeoutRef = useRef<ReturnType<typeof setTimeout> | undefined>(undefined);\n    54‚Üí\n    55‚Üí  const fetchSuggestions = useCallback(async (prompt: string) => {\n    56‚Üí    if (!prompt.trim() || !moduleId) {\n    57‚Üí      setSuggestions([]);\n    58‚Üí      return;\n    59‚Üí    }\n    60‚Üí\n    61‚Üí    // Debounce\n    62‚Üí    if (timeoutRef.current) clearTimeout(timeoutRef.current);\n    63‚Üí    timeoutRef.current = setTimeout(async () => {\n    64‚Üí      try {\n    65‚Üí        const data = await apiFetch<{ suggestions: PromptSuggestion[] }>(\n    66‚Üí          `/api/session-analytics?action=suggestions&moduleId=${encodeURIComponent(moduleId)}&prompt=${encodeURIComponent(prompt)}`\n    67‚Üí        );\n    68‚Üí        setSuggestions(data.suggestions ?? []);\n    69‚Üí      } catch {\n    70‚Üí        // Silently ignore ‚Äî suggestions are non-critical\n    71‚Üí      }\n    72‚Üí    }, 500);\n    73‚Üí  }, [moduleId]);\n    74‚Üí\n    75‚Üí  return { suggestions, fetchSuggestions };\n    76‚Üí}\n    77‚Üí\n    78‚Üí// ‚îÄ‚îÄ Session recorder (fire and forget) ‚îÄ‚îÄ\n    79‚Üí\n    80‚Üíexport async function recordSessionOutcome(data: {\n    81‚Üí  moduleId: string;\n    82‚Üí  sessionKey: string;\n    83‚Üí  prompt: string;\n    84‚Üí  hadProjectContext: boolean;\n    85‚Üí  success: boolean;\n    86‚Üí  durationMs: number;\n    87‚Üí  startedAt: string;\n    88‚Üí}): Promise<void> {\n    89‚Üí  try {\n    90‚Üí    await apiFetch<unknown>('/api/session-analytics', {\n    91‚Üí      method: 'POST',\n    92‚Üí      headers: { 'Content-Type': 'application/json' },\n    93‚Üí      body: JSON.stringify({\n    94‚Üí        ...data,\n    95‚Üí        completedAt: new Date().toISOString(),\n    96‚Üí      }),\n    97‚Üí    });\n    98‚Üí  } catch (err) {\n    99‚Üí    console.error('Failed to record session outcome:', err);\n   100‚Üí  }\n   101‚Üí}\n   102‚Üí"}]},"parent_tool_use_id":"toolu_012cR5qiLALApMctsTiLEVAN","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"30682102-4072-42fe-b42c-ee70c3f32336"}
