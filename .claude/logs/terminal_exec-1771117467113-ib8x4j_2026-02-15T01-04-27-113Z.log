  54‚Üí  message: string;\n    55‚Üí  /** Improvement factor (e.g., 3.0 = \"3x more likely to succeed\") */\n    56‚Üí  factor: number;\n    57‚Üí  /** How confident we are (0-1), based on sample size */\n    58‚Üí  confidence: number;\n    59‚Üí  /** Actionable suggestion */\n    60‚Üí  suggestion: string;\n    61‚Üí}\n    62‚Üí\n    63‚Üí// ‚îÄ‚îÄ Per-module prompt quality score ‚îÄ‚îÄ\n    64‚Üí\n    65‚Üíexport interface PromptQualityScore {\n    66‚Üí  moduleId: string;\n    67‚Üí  score: number; // 0-100\n    68‚Üí  trend: 'improving' | 'stable' | 'declining';\n    69‚Üí  recentSuccessRate: number; // last 10 sessions\n    70‚Üí  overallSuccessRate: number;\n    71‚Üí  sessionsRecorded: number;\n    72‚Üí}\n    73‚Üí\n    74‚Üí// ‚îÄ‚îÄ Aggregate dashboard stats ‚îÄ‚îÄ\n    75‚Üí\n    76‚Üíexport interface AnalyticsDashboard {\n    77‚Üí  totalSessions: number;\n    78‚Üí  overallSuccessRate: number;\n    79‚Üí  totalDurationMs: number;\n    80‚Üí  moduleStats: ModuleStats[];\n    81‚Üí  insights: PromptInsight[];\n    82‚Üí  qualityScores: PromptQualityScore[];\n    83‚Üí  recentSessions: SessionRecord[];\n    84‚Üí}\n    85‚Üí\n    86‚Üí// ‚îÄ‚îÄ Prompt suggestion (shown before sending) ‚îÄ‚îÄ\n    87‚Üí\n    88‚Üíexport interface PromptSuggestion {\n    89‚Üí  type: 'add-context' | 'shorten' | 'lengthen' | 'rephrase' | 'module-tip';\n    90‚Üí  message: string;\n    91‚Üí  confidence: number;\n    92‚Üí}\n    93‚Üí"}]},"parent_tool_use_id":"toolu_012cR5qiLALApMctsTiLEVAN","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"a742c08d-35af-408e-87f1-1dbd5c240bd1"}
