2â†’        const pending = getPendingSuggestions();\n    93â†’        const pendingGenres = new Set(pending.map(s => s.subGenre));\n    94â†’        for (const suggestion of newSuggestions) {\n    95â†’          if (!pendingGenres.has(suggestion.subGenre)) {\n    96â†’            saveSuggestion(suggestion);\n    97â†’          }\n    98â†’        }\n    99â†’\n   100â†’        return NextResponse.json({\n   101â†’          snapshot,\n   102â†’          newSuggestions: newSuggestions.filter(s => !pendingGenres.has(s.subGenre)),\n   103â†’        });\n   104â†’      }\n   105â†’\n   106â†’      case 'resolve': {\n   107â†’        const suggestionId = body.suggestionId as string;\n   108â†’        const resolveAction = body.resolveAction as 'accept' | 'dismiss';\n   109â†’        if (!suggestionId || !resolveAction || (resolveAction !== 'accept' && resolveAction !== 'dismiss')) {\n   110â†’          return NextResponse.json({ error: 'suggestionId and resolveAction required' }, { status: 400 });\n   111â†’        }\n   112â†’        resolveSuggestion(suggestionId, resolveAction);\n   113â†’        return NextResponse.json({ ok: true });\n   114â†’      }\n   115â†’\n   116â†’      default:\n   117â†’        return NextResponse.json({ error: `Unknown action: ${action}` }, { status: 400 });\n   118â†’    }\n   119â†’  } catch (err) {\n   120â†’    return NextResponse.json(\n   121â†’      { error: err instanceof Error ? err.message : 'Internal error' },\n   122â†’      { status: 500 },\n   123â†’    );\n   124â†’  }\n   125â†’}\n   126â†’"}]},"parent_tool_use_id":"toolu_01KW1LJr1GhDp1C27vbQrufr","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"f3bdf475-ce2a-4769-bd6f-6a5aa60a77dd"}
