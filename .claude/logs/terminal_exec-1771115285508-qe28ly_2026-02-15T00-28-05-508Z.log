assive engine directories.\n    58â†’ */\n    59â†’async function collectHeaders(dir: string, maxFiles = 500): Promise<string[]> {\n    60â†’  const results: string[] = [];\n    61â†’\n    62â†’  async function walk(current: string) {\n    63â†’    if (results.length >= maxFiles) return;\n    64â†’    try {\n    65â†’      const entries = await fs.readdir(current, { withFileTypes: true });\n    66â†’      for (const entry of entries) {\n    67â†’        if (results.length >= maxFiles) return;\n    68â†’        const full = path.join(current, entry.name);\n    69â†’        if (entry.isDirectory() && !entry.name.startsWith('.')) {\n    70â†’          await walk(full);\n    71â†’        } else if (entry.isFile() && entry.name.endsWith('.h')) {\n    72â†’          results.push(full);\n    73â†’        }\n    74â†’      }\n    75â†’    } catch {\n    76â†’      // Skip unreadable directories\n    77â†’    }\n    78â†’  }\n    79â†’\n    80â†’  await walk(dir);\n    81â†’  return results;\n    82â†’}\n    83â†’\n    84â†’/**\n    85â†’ * Recursively collect all .cpp and .h files (for count).\n    86â†’ */\n    87â†’async function countSourceFiles(dir: string): Promise<number> {\n    88â†’  let count = 0;\n    89â†’\n    90â†’  async function walk(current: string) {\n    91â†’    try {\n    92â†’      const entries = await fs.readdir(current, { withFileTypes: true });\n    93â†’      for (const entry of entries) {\n    94â†’        const full = path.join(current, entry.name);\n    95â†’        if (entry.isDirectory() && !entry.name.startsWith('.')) {\n    96â†’          await walk(full);\n    97â†’        } else if (entry.isFile() && (entry.name.endsWith('.h') || entry.name.endsWith('.cpp'))) {\n    98â†’          count++;\n    99â†’        }\n   100â†’      }\n   101â†’    } catch {\n   102â†’      // Skip unreadable directories\n   103â†’    }\n   104â†’  }\n   105â†’\n   106â†’  await walk(dir);\n   107â†’  return count;\n   108â†’}\n   109â†’\n   110â†’/**\n   111â†’ *