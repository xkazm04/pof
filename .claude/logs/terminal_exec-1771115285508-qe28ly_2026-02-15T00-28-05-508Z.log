st unsubscribe = subscribe((events: FileChangeEvent[]) => {\n    48â†’        if (isStreamClosed) return;\n    49â†’        try {\n    50â†’          controller.enqueue(encoder.encode(`data: ${JSON.stringify({ type: 'changes', events })}\\n\\n`));\n    51â†’        } catch {\n    52â†’          isStreamClosed = true;\n    53â†’          unsubscribe();\n    54â†’        }\n    55â†’      });\n    56â†’\n    57â†’      // Send keepalive every 30s\n    58â†’      const keepalive = setInterval(() => {\n    59â†’        if (isStreamClosed) {\n    60â†’          clearInterval(keepalive);\n    61â†’          return;\n    62â†’        }\n    63â†’        try {\n    64â†’          controller.enqueue(encoder.encode(`: keepalive\\n\\n`));\n    65â†’        } catch {\n    66â†’          isStreamClosed = true;\n    67â†’          clearInterval(keepalive);\n    68â†’          unsubscribe();\n    69â†’        }\n    70â†’      }, 30_000);\n    71â†’\n    72â†’      // Cleanup on client disconnect\n    73â†’      request.signal.addEventListener('abort', () => {\n    74â†’        isStreamClosed = true;\n    75â†’        clearInterval(keepalive);\n    76â†’        unsubscribe();\n    77â†’        // Stop watching if no more subscribers\n    78â†’        const state = getWatcherState();\n    79â†’        if (state.subscriberCount <= 0) {\n    80â†’          stopWatching();\n    81â†’        }\n    82â†’        try { controller.close(); } catch { /* already closed */ }\n    83â†’      });\n    84â†’    },\n    85â†’  });\n    86â†’\n    87â†’  return new Response(stream, {\n    88â†’    headers: {\n    89â†’      'Content-Type': 'text/event-stream',\n    90â†’      'Cache-Control': 'no-cache, no-transform',\n    91â†’      'Connection': 'keep-alive',\n    92â†’    },\n    93â†’  });\n    94â†’}\n    95â†’\n    96â†’export async function POST(request: NextRequest) {\n    97â†’  try {\n    98â†’    const body = await request.json();\n    99â†’    const action = body.action as string;\n  