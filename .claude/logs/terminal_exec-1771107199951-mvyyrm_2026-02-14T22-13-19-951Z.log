/* Actions */}\n    88→        <div className=\"flex items-center gap-1\">\n    89→          <button\n    90→            onClick={() => onPackage(profile)}\n    91→            className=\"flex items-center gap-1 px-2 py-1 rounded text-2xs font-medium text-white transition-colors\"\n    92→            style={{ backgroundColor: `${color}cc` }}\n    93→            title=\"Package\"\n    94→          >\n    95→            <Play className=\"w-2.5 h-2.5\" />\n    96→            Package\n    97→          </button>\n    98→          <button\n    99→            onClick={() => setExpanded(!expanded)}\n   100→            className=\"p-1 rounded text-text-muted hover:text-text hover:bg-surface-hover transition-colors\"\n   101→          >\n   102→            {expanded ? <ChevronDown className=\"w-3 h-3\" /> : <ChevronRight className=\"w-3 h-3\" />}\n   103→          </button>\n   104→        </div>\n   105→      </div>\n   106→\n   107→      {/* Expanded content */}\n   108→      <AnimatePresence>\n   109→        {expanded && (\n   110→          <motion.div\n   111→            initial={{ height: 0, opacity: 0 }}\n   112→            animate={{ height: 'auto', opacity: 1 }}\n   113→            exit={{ height: 0, opacity: 0 }}\n   114→            transition={{ duration: 0.15 }}\n   115→            className=\"overflow-hidden\"\n   116→          >\n   117→            <div className=\"px-3 pb-3 border-t border-border/50 pt-2 space-y-2.5\">\n   118→              {/* Cook settings summary */}\n   119→              <div>\n   120→                <span className=\"text-2xs uppercase tracking-wider text-text-muted font-medium\">Cook Settings</span>\n   121→                <div className=\"flex flex-wrap gap-1 mt-1\">\n   122→                  {profile.cookSettings.useIoStore && <Badge label=\"IoStore\" />}\n   123→                  {profile.cookSettings.iterativeCooking && <Badge label=\"Iterative\" />}\n   124→                  {profile.cookSettings.compressTextures && <Badge label=\"Tex Compress\" />}\n   125→                  {profile.cookSettings.mapsToInclude.length > 0 && (\n   126→                    <Badge label={`${profile.cookSettings.mapsToInclude.length} maps`} />\n   127→                  )}\n   128→                  {profile.cookSettings.mapsToInclude.length === 0 && <Badge label=\"All maps\" />}\n   129→                  {profile.stage && <Badge label=\"Stage\" />}\n   130→                  {profile.archive && <Badge label=\"Archive\" />}\n   131→                </div>\n   132→              </div>\n   133→\n   134→              {/* UAT Command preview */}\n   135→              {uatCommand && (\n   136→                <div>\n   137→                  <div className=\"flex items-center justify-between\">\n   138→                    <span className=\"text-2xs uppercase tracking-wider text-text-muted font-medium\">UAT Command</span>\n   139→                    <button\n   140→                      onClick={() => {\n   141→                        navigator.clipboard.writeText(uatCommand).then(() => {\n   142→                          setCmdCopied(true);\n   143→                          setTimeout(() => setCmdCopied(false), 2000);\n   144→                        });\n   145→                      }}\n   146→                      className=\"flex items-center gap-0.5 text-2xs text-text-muted hover:text-text transition-colors\"\n   147→                    >\n   148→                      <Copy className=\"w-2.5 h-2.5\" />\n   149→                      {cmdCopied ? 'Copied!' : 'Copy'}\n   150→                    </button>\n   151→                  </div>\n   152→                  <pre className=\"mt-1 text-2xs font-mono text-[#9ca0be] bg-background rounded p-2 overflow-x-auto whitespace-pre-wrap break-all leading-relaxed\">\n   153→                    {uatCommand}\n   154→                  </pre>\n   155→                </div>\n   156→              )}\n   157→\n   158→              {/* Platform notes */}\n   159→              {notes.length > 0 && (\n   160→                <div>\n   161→                  <span className=\"text-2xs uppercase tracking-wider text-text-muted font-medium\">Platform Notes</span>\n   162→                  <div className=\"mt-1 space-y-0.5\">\n   163→                    {notes.map((note, i) => (\n   164→                      <div key={i} className=\"flex items-start gap-1 text-2xs text-text-muted-hover\">\n   165→                        <AlertCircle className=\"w-2.5 h-2.5 text-text-muted flex-shrink-0 mt-px\" />\n   166→                        {note}\n   167→                      </div>\n   168→                    ))}\n   169→                  </div>\n   170→                </div>\n   171→              )}\n   172→\n   173→              {/* Profile actions */}\n   174→              <div className=\"flex items-center gap-2 pt-1 border-t border-border/30\">\n   175→                <button\n   176→                  onClick={() => onEdit(profile)}\n   177→                  className=\"text-2xs text-blue-400 hover:text-blue-300 transition-colors\"\n   178→                >\n   179→                  Edit\n   180→                </button>\n   181→                {!profile.isDefault && (\n   182→                  <button\n   183→                    onClick={() => onSetDefault(profile.id)}\n   184→                    className=\"text-2xs text-yellow-400 hover:text-yellow-300 transition-colors\"\n   185→                  >\n   186→                    Set Default\n   187→                  </button>\n   188→                )}\n   189→                <button\n   190→                  onClick={() => onDelete(profile.id)}\n   191→                  className=\"flex items-center gap-0.5 text-2xs text-red-400/60 hover:text-red-400 transition-colors ml-auto\"\n   192→                >\n   193→                  <Trash2 className=\"w-2.5 h-2.5\" />\n   194→                  Delete\n   195→                </button>\n   196→              </div>\n   197→            </div>\n   198→          </motion.div>\n   199→        )}\n   200→      </AnimatePresence>\n   201→    </motion.div>\n   202→  );\n   203→}\n   204→\n   205→function Badge({ label }: { label: string }) {\n   206→  return (\n   207→    <span className=\"text-2xs text-text-muted-hover bg-surface-hover px-1.5 py-px rounded\">\n   208→      {label}\n   209→    </span>\n   210→  );\n   211→}\n   212→"}]},"parent_tool_use_id":"toolu_013EpuYUHEaizivhc1JawMg3","session_id":"0b456cab-4d56-4498-b76c-d3531c020462","uuid":"fa840b77-4999-4f19-8d27-fb6a8d7d814b"}
