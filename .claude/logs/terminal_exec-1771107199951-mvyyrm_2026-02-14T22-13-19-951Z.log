[2026-02-14T22:13:58.682Z] [STDOUT] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_013dSk7SoPFbhU9a3651jitF","type":"tool_result","content":"     1→'use client';\n     2→\n     3→import { useState, useMemo, useCallback, useEffect, useRef } from 'react';\n     4→import {\n     5→  Radar as RadarIcon,\n     6→  AlertTriangle,\n     7→  Check,\n     8→  ChevronDown,\n     9→  ChevronUp,\n    10→  Loader2,\n    11→  Play,\n    12→  TrendingDown,\n    13→  TrendingUp,\n    14→  Zap,\n    15→  X,\n    16→} from 'lucide-react';\n    17→import { useEvaluatorStore } from '@/stores/evaluatorStore';\n    18→import { useModuleCLI } from '@/hooks/useModuleCLI';\n    19→import { TaskFactory } from '@/lib/cli-task';\n    20→import { MODULE_LABELS } from '@/lib/module-registry';\n    21→import type { EvaluatorReport, ModuleScore, Recommendation } from '@/types/evaluator';\n    22→\n    23→const EVAL_ACCENT = '#ef4444';\n    24→\n    25→// ── Score → color ──\n    26→\n    27→function scoreColor(score: number): string {\n    28→  if (score >= 80) return '#4ade80';\n    29→  if (score >= 60) return '#fbbf24';\n    30→  if (score >= 40) return '#fb923c';\n    31→  return '#f87171';\n    32→}\n    33→\n    34→function scoreBg(score: number): string {\n    35→  if (score >= 80) return '#4ade8012';\n    36→  if (score >= 60) return '#fbbf2412';\n    37→  if (score >= 40) return '#fb923c12';\n    38→  return '#f8717112';\n    39→}\n    40→\n    41→// ── Priority color ──\n    42→\n    43→const PRIORITY_COLORS: Record<string, { text: string; bg: string; border: string }> = {\n    44→  critical: { text: '#f87171', bg: '#f8717112', border: '#f8717125' },\n    45→  high: { text: '#fb923c', bg: '#fb923c12', border: '#fb923c25' },\n    46→  medium: { text: '#fbbf24', bg: '#fbbf2412', border: '#fbbf2425' },\n    47→  low: { text: 'var(--text-muted)', bg: 'var(--text-muted)12', border: 'var(--text-muted)25' },\n    48→};\n    49→\n    50→// ── Radar chart geometry helpers ──\n    51→\n    52→const RADAR_CX = 160;\n    53→const RADAR_CY = 140;\n    54→const RADAR_R = 110;\n    55→const RADAR_RINGS = 5; // 20, 40, 60, 80, 100\n    56→\n    57→function polarToXY(angle: number, radius: number): { x: number; y: number } {\n    58→  // Start from top (-90°), go clockwise\n    59→  const rad = ((angle - 90) * Math.PI) / 180;\n    60→  return {\n    61→    x: RADAR_CX + Math.cos(rad) * radius,\n    62→    y: RADAR_CY + Math.sin(rad) * radius,\n    63→  };\n    64→}\n    65→\n    66→// ── Component ──\n    67→\n    68→export function ProjectHealthDashboard() {\n    69→  const lastScan = useEvaluatorStore((s) => s.lastScan);\n    70→  const scanHistory = useEvaluatorStore((s) => s.scanHistory);\n    71→  const isScanning = useEvaluatorStore((s) => s.isScanning);\n    72→  const setScanning = useEvaluatorStore((s) => s.setScanning);\n    73→  const setLastScan = useEvaluatorStore((s) => s.setLastScan);\n    74→  const addScanToHistory = useEvaluatorStore((s) => s.addScanToHistory);\n    75→\n    76→  const [selectedModule, setSelectedModule] = useState<string | null>(null);\n    77→  const [showHistoryOverlay, setShowHistoryOverlay] = useState(false);\n    78→  const [regressionAlerts, setRegressionAlerts] = useState<RegressionAlert[]>([]);\n    79→\n    80→  // ── Fix CLI session ──\n    81→\n    82→  const fixCli = useModuleCLI({\n    83→    moduleId: 'ai-behavior', // evaluator doesn't have its own sub-module, use generic\n    84→    sessionKey: 'evaluator-fix',\n    85→    label: 'Evaluator Fix',\n    86→    accentColor: EVAL_ACCENT,\n    87→  });\n    88→\n    89→  const handleFix = useCallback(\n    90→    (rec: Recommendation) => {\n    91→      const task = TaskFactory.askClaude('ai-behavior', rec.suggestedPrompt, 'Evaluator Fix');\n    92→      fixCli.execute(task);\n    93→    },\n    94→    [fixCli],\n    95→  );\n    96→\n    97→  // ── Regression detection ──\n    98→\n    99→  useEffect(() => {\n   100→    if (scanHistory.length < 2) return;\n   101→    const current = scanHistory[scanHistory.length - 1];\n   102→    const previous = scanHistory[scanHistory.length - 2];\n   103→\n   104→    const alerts: RegressionAlert[] = [];\n   105→\n   106→    // Overall score regression\n   107→    if (current.overallScore < previous.overallScore - 5) {\n   108→      alerts.push({\n   109→        id: 'overall',\n   110→        message: `Overall health dropped from ${previous.overallScore} → ${current.overallScore}`,\n   111→        severity: 'high',\n   112→      });\n   113→    }\n   114→\n   115→    // Per-module regressions\n   116→    const prevMap = new Map(previous.moduleScores.map((m) => [m.moduleId, m.score]));\n   117→    for (const ms of current.moduleScores) {\n   118→      const prevScore = prevMap.get(ms.moduleId);\n   119→      if (prevScore != null && ms.score < prevScore - 10) {\n   120→        alerts.push({\n   121→          id: ms.moduleId,\n   122→          message: `${MODULE_LABELS[ms.moduleId] ?? ms.moduleId} dropped from ${prevScore} → ${ms.score}`,\n   123→          severity: ms.score < 40 ? 'critical' : 'medium',\n   124→        });\n   125→      }\n   126→    }\n   127→\n   128→    if (alerts.length > 0) {\n   129→      setRegressionAlerts(alerts);\n   130→    }\n   131→  }, [scanHistory]);\n   132→\n   133→  const dismissAlert = useCallback((id: string) => {\n   134→    setRegressionAlerts((prev) => prev.filter((a) => a.id !== id));\n   135→  }, []);\n   136→\n   137→  // ── Radar data ──\n   138→\n   139→  const radarData = useMemo(() => {\n   140→    if (!lastScan || lastScan.moduleScores.length === 0) return null;\n   141→    const scores = lastScan.moduleScores;\n   142→    const angleStep = 360 / scores.length;\n   143→    return scores.map((ms, i) => ({\n   144→      ...ms,\n   145→      angle: i * angleStep,\n   146→      label: MODULE_LABELS[ms.moduleId] ?? ms.moduleId,\n   147→    }));\n   148→  }, [lastScan]);\n   149→\n   150→  // Previous scan for overlay\n   151→  const prevRadarData = useMemo(() => {\n   152→    if (!showHistoryOverlay || scanHistory.length < 2) return null;\n   153→    const prev = scanHistory[scanHistory.length - 2];\n   154→    if (!prev || prev.moduleScores.length === 0) return null;\n   155→    const scores = prev.moduleScores;\n   156→    const angleStep = 360 / scores.length;\n   157→    return scores.map((ms, i) => ({\n   158→      ...ms,\n   159→      angle: i * angleStep,\n   160→    }));\n   161→  }, [showHistoryOverlay, scanHistory]);\n   162→\n   163→  // Build the radar polygon path\n   164→  const radarPath = useMemo(() => {\n   165→    if (!radarData) return '';\n   166→    return radarData\n   167→      .map((d) => {\n   168→        const r = (d.score / 100) * RADAR_R;\n   169→        const { x, y } = polarToXY(d.angle, r);\n   170→        return `${x},${y}`;\n   171→      })\n   172→      .join(' ');\n   173→  }, [radarData]);\n   174→\n   175→  const prevRadarPath = useMemo(() => {\n   176→    if (!prevRadarData) return '';\n   177→    return prevRadarData\n   178→      .map((d) => {\n   179→        const r = (d.score / 100) * RADAR_R;\n   180→        const { x, y } = polarToXY(d.angle, r);\n   181→        return `${x},${y}`;\n   182→      })\n   183→      .join(' ');\n   184→  }, [prevRadarData]);\n   185→\n   186→  // ── Selected module detail ──\n   187→\n   188→  const selectedDetail = useMemo(() => {\n   189→    if (!selectedModule || !lastScan) return null;\n   190→    const ms = lastScan.moduleScores.find((m) => m.moduleId === selectedModule);\n   191→    const recs = lastScan.recommendations.filter((r) => r.moduleId === selectedModule);\n   192→    return ms ? { ...ms, recommendations: recs, label: MODULE_LABELS[ms.moduleId] ?? ms.moduleId } : null;\n   193→  }, [selectedModule, lastScan]);\n   194→\n   195→  // ── Health pulse (based on overall score) ──\n   196→\n   197→  const healthPulseColor = lastScan\n   198→    ? scoreColor(lastScan.overallScore)\n   199→    : 'var(--text-muted)';\n   200→\n   201→  return (\n   202→    <div className=\"space-y-5\">\n   203→      {/* ── Regression alerts ── */}\n   204→      {regressionAlerts.length > 0 && (\n   205→        <div className=\"space-y-2\">\n   206→          {regressionAlerts.map((alert) => (\n   207→            <div\n   208→              key={alert.id}\n   209→              className=\"flex items-center gap-3 px-4 py-2.5 rounded-lg border\"\n   210→              style={{\n   211→                backgroundColor: PRIORITY_COLORS[alert.severity]?.bg ?? '#f8717112',\n   212→                borderColor: PRIORITY_COLORS[alert.severity]?.border ?? '#f8717125',\n   213→              }}\n   214→            >\n   215→              <TrendingDown className=\"w-4 h-4 flex-shrink-0\" style={{ color: PRIORITY_COLORS[alert.severity]?.text ?? '#f87171' }} />\n   216→              <span className=\"text-xs text-text flex-1\">{alert.message}</span>\n   217→              <span\n   218→                className=\"text-2xs font-semibold uppercase tracking-wider px-1.5 py-0.5 rounded\"\n   219→                style={{\n   220→                  color: PRIORITY_COLORS[alert.severity]?.text,\n   221→                  backgroundColor: PRIORITY_COLORS[alert.severity]?.bg,\n   222→                }}\n   223→              >\n   224→                Regression\n   225→              </span>\n   226→              <button\n   227→                onClick={() => dismissAlert(alert.id)}\n   228→                className=\"p-0.5 rounded hover:bg-[#ffffff10] transition-colors\"\n   229→              >\n   230→                <X className=\"w-3 h-3 text-text-muted\" />\n   231→              </button>\n   232→            </div>\n   233→          ))}\n   234→        </div>\n   235→      )}\n   236→\n   237→      {/* ── Top row: Radial gauge + info + scan button ── */}\n   238→      <div className=\"flex items-center gap-5\">\n   239→        {/* Radial Score Gauge */}\n   240→        <div className=\"flex-shrink-0\">\n   241→  