n    99→      setConnectingFrom(null);\n   100→      return;\n   101→    }\n   102→    const exists = connections.some(\n   103→      (c) => (c.fromId === connectingFrom && c.toId === toId) || (c.fromId === toId && c.toId === connectingFrom)\n   104→    );\n   105→    if (!exists) {\n   106→      const newConn: RoomConnection = {\n   107→        id: `conn-${Date.now()}`,\n   108→        fromId: connectingFrom,\n   109→        toId,\n   110→        bidirectional: true,\n   111→        condition: '',\n   112→      };\n   113→      onUpdateConnections([...connections, newConn]);\n   114→    }\n   115→    setConnectingFrom(null);\n   116→  }, [connectingFrom, connections, onUpdateConnections]);\n   117→\n   118→  const deleteConnection = useCallback((connId: string) => {\n   119→    onUpdateConnections(connections.filter((c) => c.id !== connId));\n   120→  }, [connections, onUpdateConnections]);\n   121→\n   122→  // ── Drag handling ──\n   123→\n   124→  const getSVGPoint = useCallback((e: React.MouseEvent | MouseEvent) => {\n   125→    if (!svgRef.current) return { x: 0, y: 0 };\n   126→    const rect = svgRef.current.getBoundingClientRect();\n   127→    return {\n   128→      x: e.clientX - rect.left - pan.x,\n   129→      y: e.clientY - rect.top - pan.y,\n   130→    };\n   131→  }, [pan]);\n   132→\n   133→  const handleMouseDown = useCallback((e: React.MouseEvent, roomId: string) => {\n   134→    if (readOnly) return;\n   135→    e.stopPropagation();\n   136→\n   137→    if (connectingFrom) {\n   138→      completeConnection(roomId);\n   139→      return;\n   140→    }\n   141→\n   142→    const pt = getSVGPoint(e);\n   143→    const room = rooms.find((r) => r.id === roomId);\n   144→    if (!room) return;\n   145→\n   146→    setDragState({\n   147→      roomId,\n   148→      offsetX: pt.x - room.x,\n   149→      offsetY: pt.y - room.y,\n   150→    });\n   151→    onSelectRoom(roomId);\n   152→  }, [readOnly, connectingFrom, completeConnection, getSVGPoint, rooms, onSelectRoom]);\n   153→\n   154→  const handleMouseMove = useCallback((e: React.MouseEvent) => {\n   155→    if (isPanning) {\n   156→      setPan({\n   157→        x: e.clientX - panStart.current.x + panStart.current.panX,\n   158→        y: e.clientY - panStart.current.y + panStart.current.panY,\n   159→      });\n   160→      return;\n   161→    }\n   162→    if (!dragState) return;\n   163→    const pt = getSVGPoint(e);\n   164→    onUpdateRooms(rooms.map((r) =>\n   165→      r.id === dragState.roomId\n   166→        ? { ...r, x: pt.x - dragState.offsetX, y: pt.y - dragState.offsetY }\n   167→        : r\n   168→    ));\n   169→  }, [dragState, isPanning, getSVGPoint, rooms, onUpdateRooms]);\n   170→\n   171→  const handleMouseUp = useCallback(() => {\n   172→    setDragState(null);\n   173→    setIsPanning(false);\n   174→  }, []);\n   175→\n   176→  const handleSvgMouseDown = useCallback((e: React.MouseEvent) => {\n   177→    if (e.target === svgRef.current || (e.target as SVGElement).tagName === 'rect') {\n   178→      if (connectingFrom) {\n   179→        setConnectingFrom(null);\n   180→        return;\n   181→      }\n   182→      panStart.current = { x: e.clientX, y: e.clientY, panX: pan.x, panY: pan.y };\n   183→      setIsPanning(true);\n   184→      onSelectRoom(null);\n   185→    }\n   186→  }, [connectingFrom, pan, onSelectRoom]);\n   187→\n   188→  // ── Get room center for connections ──\n   189→\n   190→  const getRoomCenter = useCallback((roomId: string) => {\n   191→    const room = rooms.find((r) => r.id === roomId);\n   192→    if (!room) return { x: 0, y: 0 };\n   193→    return { x: room.x + ROOM_W / 2, y: room.y + ROOM_H / 2 };\n   194→  }, [rooms]);\n   195→\n   196→  return (\n   197→    <div className=\"relative w-full h-full bg-[#080818] rounded-lg border border-border overflow-hidden\">\n   198→      {/* Toolbar */}\n   199→      {!readOnly && (\n   200→        <div className=\"absolute top-2 left-2 z-10 flex items-center gap-1\">"}]