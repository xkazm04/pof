\n   378→              const labelPos = polarToXY(d.angle, RADAR_R + 16);\n   379→              const isSelected = selectedModule === d.moduleId;\n   380→              const color = scoreColor(d.score);\n   381→\n   382→              return (\n   383→                <g key={d.moduleId} className=\"cursor-pointer\" onClick={() => setSelectedModule(isSelected ? null : d.moduleId)}>\n   384→                  {/* Hit area */}\n   385→                  <circle cx={x} cy={y} r={12} fill=\"transparent\" />\n   386→                  {/* Dot */}\n   387→                  <circle\n   388→                    cx={x}\n   389→                    cy={y}\n   390→                    r={isSelected ? 5 : 4}\n   391→                    fill={isSelected ? color : `${color}80`}\n   392→                    stroke={isSelected ? color : 'none'}\n   393→                    strokeWidth={isSelected ? 2 : 0}\n   394→                  />\n   395→                  {/* Score badge on selected */}\n   396→                  {isSelected && (\n   397→                    <>\n   398→                      <rect\n   399→                        x={x - 12}\n   400→                        y={y - 20}\n   401→                        width={24}\n   402→                        height={14}\n   403→                        rx={4}\n   404→                        fill=\"var(--surface-deep)\"\n   405→                        stroke={color}\n   406→                        strokeWidth={0.5}\n   407→                      />\n   408→                      <text\n   409→                        x={x}\n   410→                        y={y - 10}\n   411→                        textAnchor=\"middle\"\n   412→                        className=\"text-2xs font-bold\"\n   413→                        fill={color}\n   414→                      >\n   415→                        {d.score}\n   416→                      </text>\n   417→                    </>\n   418→                  )}\n   419→                  {/* Label */}\n   420→     