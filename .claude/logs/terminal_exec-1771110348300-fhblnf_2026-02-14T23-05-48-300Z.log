í    const executionId = searchParams.get('executionId');\n    50‚Üí\n    51‚Üí    if (!executionId) {\n    52‚Üí      return apiError('Execution ID is required', 400);\n    53‚Üí    }\n    54‚Üí\n    55‚Üí    const execution = getExecution(executionId);\n    56‚Üí    if (!execution) {\n    57‚Üí      return apiError('Execution not found', 404);\n    58‚Üí    }\n    59‚Üí\n    60‚Üí    const aborted = abortExecution(executionId);\n    61‚Üí    return apiSuccess({\n    62‚Üí      aborted,\n    63‚Üí      message: aborted ? 'Execution aborted' : 'Failed to abort execution',\n    64‚Üí    });\n    65‚Üí  } catch (error) {\n    66‚Üí    console.error('Claude Terminal abort error:', error);\n    67‚Üí    return apiError(error instanceof Error ? error.message : 'Failed to abort execution');\n    68‚Üí  }\n    69‚Üí}\n    70‚Üí\n    71‚Üíexport async function GET(request: NextRequest) {\n    72‚Üí  try {\n    73‚Üí    const { searchParams } = new URL(request.url);\n    74‚Üí    const executionId = searchParams.get('executionId');\n    75‚Üí\n    76‚Üí    if (!executionId) {\n    77‚Üí      return apiError('Execution ID is required', 400);\n    78‚Üí    }\n    79‚Üí\n    80‚Üí    const execution = getExecution(executionId);\n    81‚Üí    if (!execution) {\n    82‚Üí      return apiError('Execution not found', 404);\n    83‚Üí    }\n    84‚Üí\n    85‚Üí    return apiSuccess({\n    86‚Üí      execution: {\n    87‚Üí        id: execution.id,\n    88‚Üí        projectPath: execution.projectPath,\n    89‚Üí        status: execution.status,\n    90‚Üí        sessionId: execution.sessionId,\n    91‚Üí        startTime: execution.startTime,\n    92‚Üí        endTime: execution.endTime,\n    93‚Üí        eventCount: execution.events.length,\n    94‚Üí        logFilePath: execution.logFilePath,\n    95‚Üí      },\n    96‚Üí    });\n    97‚Üí  } catch (error) {\n    98‚Üí    console.error('Claude Terminal status error:', error);\n    99‚Üí    return apiError(error instanceof Error ? e