t (saves current, restores target) */\n    46‚Üí  switchProject: (projectId: string) => Promise<void>;\n    47‚Üí  /** Remove a project from the recent list */\n    48‚Üí  removeRecentProject: (projectId: string) => Promise<void>;\n    49‚Üí}\n    50‚Üí\n    51‚Üí/** Cache duration: 5 minutes. Avoids re-scanning on every prompt. */\n    52‚Üíconst SCAN_CACHE_MS = 5 * 60 * 1000;\n    53‚Üí\n    54‚Üíexport const useProjectStore = create<ProjectState>()(\n    55‚Üí  persist(\n    56‚Üí    (set, get) => ({\n    57‚Üí      projectName: '',\n    58‚Üí      projectPath: '',\n    59‚Üí      ueVersion: '5.5.4',\n    60‚Üí      isSetupComplete: false,\n    61‚Üí      isNewProject: true,\n    62‚Üí      setupStep: 0,\n    63‚Üí\n    64‚Üí      dynamicContext: null,\n    65‚Üí      isScanning: false,\n    66‚Üí      scanError: null,\n    67‚Üí\n    68‚Üí      recentProjects: [],\n    69‚Üí\n    70‚Üí      setProject: (data) => set((state) => ({ ...state, ...data })),\n    71‚Üí\n    72‚Üí      completeSetup: () => {\n    73‚Üí        set({ isSetupComplete: true });\n    74‚Üí        // Auto-save to recent when setup completes\n    75‚Üí        setTimeout(() => get().saveToRecent(), 100);\n    76‚Üí      },\n    77‚Üí\n    78‚Üí      resetProject: () => set({\n    79‚Üí        projectName: '',\n    80‚Üí        projectPath: '',\n    81‚Üí        ueVersion: '5.5.4',\n    82‚Üí        isSetupComplete: false,\n    83‚Üí        isNewProject: true,\n    84‚Üí        setupStep: 0,\n    85‚Üí        dynamicContext: null,\n    86‚Üí        isScanning: false,\n    87‚Üí        scanError: null,\n    88‚Üí      }),\n    89‚Üí\n    90‚Üí      scanProject: async () => {\n    91‚Üí        const { projectPath, projectName, isScanning, dynamicContext } = get();\n    92‚Üí        if (!projectPath || !projectName || isScanning) return;\n    93‚Üí\n    94‚Üí        // Return cached if still fresh\n    95‚Üí        if (dynamicContext?.scannedAt) {\n    96‚Üí          const age = Date.now() - new Date(dynamicContext.scannedAt).getTime();\n    97‚Üí          if (age < SCAN_CACHE_MS) return;\n    98‚Üí        }\n    99‚Üí\n   100‚Üí        set({ isScanning: true, scanError: null });\n   101‚Üí\n   102‚Üí        try {\n   103‚Üí          const data = await apiFetch<{\n   104‚Üí            scannedAt: string;\n   105‚Üí            classes: DynamicProjectContext['classes'];\n   106‚Üí            plugins: DynamicProjectContext['plugins'];\n   107‚Üí            buildDependencies: DynamicProjectContext['buildDependencies'];\n   108‚Üí            sourceFileCount: number;\n   109‚Üí          }>('/api/filesystem/scan-project', {\n   110‚Üí            method: 'POST',\n   111‚Üí            headers: { 'Content-Type': 'application/json' },\n   112‚Üí            body: JSON.stringify({ projectPath, moduleName: projectName }),\n   113‚Üí          });\n   114‚Üí          set({\n   115‚Üí            isScanning: false,\n   116‚Üí            scanError: null,\n   117‚Üí            dynamicContext: {\n   118‚Üí              scannedAt: data.scannedAt,\n   119‚Üí              classes: data.classes,\n   120‚Üí              plugins: data.plugins,\n   121‚Üí              buildDependencies: data.buildDependencies,\n   122‚Üí              sourceFileCount: data.sourceFileCount,\n   123‚Üí            },\n   124‚Üí          });\n   125‚Üí        } catch (err) {\n   126‚Üí          set({\n   127‚Üí            isScanning: false,\n   128‚Üí            scanError: err instanceof Error ? err.message : 'Failed to scan project',\n   129‚Üí          });\n   130‚Üí        }\n   131‚Üí      },\n   132‚Üí\n   133‚Üí      saveToRecent: async () => {\n   134‚Üí        const { projectName, projectPath, ueVersion, isSetupComplete } = get();\n   135‚Üí        if (!projectName || !projectPath || !isSetupComplete) return;\n   136‚Üí\n   137‚Üí        // Grab checklist progress from moduleStore (cross-store read)\n   138‚Üí        let checklistProgress = {};\n   139‚Üí        try {\n   140‚Üí          const moduleStoreRaw = localStorage.getItem('pof-modules');\n   141‚Üí          if (moduleStoreRaw) {\n   142‚Üí            const parsed = JSON.parse(moduleStoreRaw);\n   143‚Üí            checklistProgress = parsed?.state?.checklistProgress ?? {};\n   144‚Üí          }\n   145‚Üí        } catch { /* ignore */ }\n   146‚Üí\n   147‚Üí        try {\n   148‚Üí          await apiFetch('/api/recent-projects', {\n   149‚Üí            method: 'POST',\n   150‚Üí            headers: { 'Content-Type': 'application/json' },\n   151‚Üí            body: JSON.stringify({\n   152‚Üí              action: 'save',\n   153‚Üí              projectName,\n   154‚Üí              projectPath,\n   155‚Üí              ueVersion,\n   156‚Üí              checklistProgress,\n   157‚Üí            }),\n   158‚Üí          });\n   159‚Üí          // Refresh the list\n   160‚Üí          get().loadRecentProjects();\n   161‚Üí        } catch {\n   162‚Üí          // Silent fail ‚Äî not critical\n   163‚Üí        }\n   164‚Üí      },\n   165‚Üí\n   166‚Üí      loadRecentProjects: async () => {\n   167‚Üí        try {\n   168‚Üí          const projects = await apiFetch<RecentProject[]>('/api/recent-projects');\n   169‚Üí          set({ recentProjects: projects });\n   170‚Üí        } catch {\n   171‚Üí          // Silent fail\n   172‚Üí        }\n   173‚Üí      },\n   174‚Üí\n   175‚Üí      switchProject: async (projectId: string) => {\n   176‚Üí        const { recentProjects, projectPath, isSetupComplete } = get();\n   177‚Üí        const target = recentProjects.find((p) => p.id === projectId);\n   178‚Üí        if (!target) return;\n   179‚Üí\n   180‚Üí        // Save current project state before switching\n   181‚Üí        if (projectPath && isSetupComplete) {\n   182‚Üí          await get().saveToRecent();\n   183‚Üí        }\n   184‚Üí\n   185‚Üí        // Touch the target project's last_opened_at\n   186‚Üí        try {\n   187‚Üí          await apiFetch('/api/recent-projects', {\n   188‚Üí            method: 'POST',\n   189‚Üí            headers: { 'Content-Type': 'application/json' },\n   190‚Üí            body: JSON.stringify({\n   191‚Üí              action: 'touch',\n   192‚Üí              projectId: target.id,\n   193‚Üí            }),\n   194‚Üí          });\n   195‚Üí        } catch {\n   196‚Üí          // Continue switching even if touch fails\n   197‚Üí        }\n   198‚Üí\n   199‚Üí        // Restore the target project's state\n   200‚Üí        set({\n   201‚Üí          projectName: target.projectName,\n   202‚Üí          projectPath: target.projectPath,\n   203‚Üí          ueVersion: target.ueVersion,\n   204‚Üí          isSetupComplete: true,\n   205‚Üí          isNewProject: false,\n   206‚Üí          setupStep: 0,\n   207‚Üí          dynamicContext: null, // Will re-scan\n   208‚Üí          isScanning: false,\n   209‚Üí          scanError: null,\n   210‚Üí        });\n   211‚Üí\n   212‚Üí        // Trigger a scan for the new project\n   213‚Üí        setTimeout(() => get().scanProject(), 200);\n   214‚Üí        // Refresh recent list\n   215‚Üí        get().loadRecentProjects();\n   216‚Üí      },\n   217‚Üí\n   218‚Üí      removeRecentProject: async (projectId: string) => {\n   219‚Üí        try {\n   220‚Üí          await apiFetch('/api/recent-projects', {\n   221‚Üí            method: 'POST',\n   222‚Üí            headers: { 'Content-Type': 'application/json' },\n   223‚Üí            body: JSON.stringify({ action: 'remove', projectId }),\n   224‚Üí          });\n   225‚Üí          set((state) => ({\n   226‚Üí            recentProjects: state.recentProjects.filter((p) => p.id !== projectId),\n   227‚Üí          }));\n   228‚Üí        } catch {\n   229‚Üí          // Silent fail\n   230‚Üí        }\n   231‚Üí      },\n   232‚Üí    }),\n   233‚Üí    {\n   234‚Üí      name: 'pof-project',\n   235‚Üí      storage: createJSONStorage(() => localStorage),\n   236‚Üí      partialize: (state) => ({\n   237‚Üí        projectName: state.projectName,\n   238‚Üí        projectPath: state.projectPath,\n   239‚Üí        ueVersion: state.ueVersion,\n   240‚Üí        isSetupComplete: state.isSetupComplete,\n   241‚Üí        isNewProject: state.isNewProject,\n   242‚Üí        setupStep: state.setupStep,\n   243‚Üí        dynamicContext: state.dynamicContext,\n   244‚Üí      }),\n   245‚Üí    }\n   246‚Üí  )\n   247‚Üí);\n   248‚Üí"}]},"parent_tool_use_id":"toolu_01N7BcwGfu6i8rphTBozC7LV","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"d95a4b40-f4a6-493d-b538-22799edf0331"}
