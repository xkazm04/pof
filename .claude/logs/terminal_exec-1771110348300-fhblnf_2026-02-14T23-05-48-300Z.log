 35‚Üí    await scanProject(projectPath, dynamicContext);\n    36‚Üí  };\n    37‚Üí\n    38‚Üí  if (loading) {\n    39‚Üí    return (\n    40‚Üí      <div className=\"flex items-center justify-center py-20\">\n    41‚Üí        <Loader2 className=\"w-5 h-5 text-text-muted animate-spin\" />\n    42‚Üí      </div>\n    43‚Üí    );\n    44‚Üí  }\n    45‚Üí\n    46‚Üí  const patterns = stats?.detectedPatterns ?? [];\n    47‚Üí  const suggestions = stats?.activeSuggestions ?? [];\n    48‚Üí  const accepted = stats?.acceptedSubGenres ?? [];\n    49‚Üí\n    50‚Üí  return (\n    51‚Üí    <div className=\"space-y-5\">\n    52‚Üí      {/* Header + Scan button */}\n    53‚Üí      <motion.div\n    54‚Üí        initial={{ opacity: 0, y: 8 }}\n    55‚Üí        animate={{ opacity: 1, y: 0 }}\n    56‚Üí        transition={{ duration: 0.25 }}\n    57‚Üí        className=\"flex items-center justify-between\"\n    58‚Üí      >\n    59‚Üí        <div className=\"flex items-center gap-2.5\">\n    60‚Üí          <div\n    61‚Üí            className=\"w-8 h-8 rounded-lg flex items-center justify-center\"\n    62‚Üí            style={{ backgroundColor: `${ACCENT}12`, border: `1px solid ${ACCENT}20` }}\n    63‚Üí          >\n    64‚Üí            <Dna className=\"w-4 h-4\" style={{ color: ACCENT }} />\n    65‚Üí          </div>\n    66‚Üí          <div>\n    67‚Üí            <h2 className=\"text-sm font-medium text-text\">Genre Evolution</h2>\n    68‚Üí            <p className=\"text-2xs text-text-muted\">\n    69‚Üí              {stats?.totalScans ?? 0} scan{(stats?.totalScans ?? 0) !== 1 ? 's' : ''}\n    70‚Üí              {stats?.lastScanAt && (\n    71‚Üí                <> &middot; last {new Date(stats.lastScanAt).toLocaleDateString()}</>\n    72‚Üí              )}\n    73‚Üí            </p>\n    74‚Üí          </div>\n    75‚Üí        </div>\n    76‚Üí\n    77‚Üí        <button\n    78‚Üí          onClick={handleScan}\n    79‚Üí          disabled={scanning || !projectPath}\n    80‚Üí          className=\"flex items-center gap-1.5 px-3 py-1.5 rounded-lg text-xs font-medium transition-all disabled:opacity-40\"\n    81‚Üí          style={{\n    82‚Üí            backgroundColor: `${ACCENT}15`,\n    83‚Üí            color: ACCENT,\n    84‚Üí            border: `1px solid ${ACCENT}30`,\n    85‚Üí          }}\n    86‚Üí        >\n    87‚Üí          {scanning ? (\n    88‚Üí            <Loader2 className=\"w-3 h-3 animate-spin\" />\n    89‚Üí          ) : (\n    90‚Üí            <Scan className=\"w-3 h-3\" />\n    91‚Üí          )}\n    92‚Üí          {scanning ? 'Scanning...' : 'Scan Project'}\n    93‚Üí        </button>\n    94‚Üí      </motion.div>\n    95‚Üí\n    96‚Üí      {/* Empty state */}\n    97‚Üí      {patterns.length === 0 && suggestions.length === 0 && accepted.length === 0 && (\n    98‚Üí        <EmptyState onScan={handleScan} scanning={scanning} hasProject={!!projectPath} />\n    99‚Üí      )}\n   100‚Üí"}]},"parent_tool_use_id":"toolu_01N7BcwGfu6i8rphTBozC7LV","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"b31bf622-95fe-4929-9801-e2d190ac6b1d"}
