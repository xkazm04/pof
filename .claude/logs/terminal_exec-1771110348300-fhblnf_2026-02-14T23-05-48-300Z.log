{\n    45‚Üí            ...state.moduleHealth,\n    46‚Üí            [moduleId]: { ...existing, ...health },\n    47‚Üí          },\n    48‚Üí        };\n    49‚Üí      }),\n    50‚Üí\n    51‚Üí      toggleChecklistItem: (subModuleId, itemId) => set((state) => {\n    52‚Üí        const moduleProgress = state.checklistProgress[subModuleId] ?? {};\n    53‚Üí        return {\n    54‚Üí          checklistProgress: {\n    55‚Üí            ...state.checklistProgress,\n    56‚Üí            [subModuleId]: {\n    57‚Üí              ...moduleProgress,\n    58‚Üí              [itemId]: !moduleProgress[itemId],\n    59‚Üí            },\n    60‚Üí          },\n    61‚Üí        };\n    62‚Üí      }),\n    63‚Üí\n    64‚Üí      setChecklistItem: (subModuleId, itemId, checked) => set((state) => {\n    65‚Üí        const moduleProgress = state.checklistProgress[subModuleId] ?? {};\n    66‚Üí        if (moduleProgress[itemId] === checked) return state;\n    67‚Üí        return {\n    68‚Üí          checklistProgress: {\n    69‚Üí            ...state.checklistProgress,\n    70‚Üí            [subModuleId]: {\n    71‚Üí              ...moduleProgress,\n    72‚Üí              [itemId]: checked,\n    73‚Üí            },\n    74‚Üí          },\n    75‚Üí        };\n    76‚Üí      }),\n    77‚Üí    }),\n    78‚Üí    {\n    79‚Üí      name: 'pof-modules',\n    80‚Üí      storage: createJSONStorage(() => localStorage),\n    81‚Üí    }\n    82‚Üí  )\n    83‚Üí);\n    84‚Üí"}]},"parent_tool_use_id":"toolu_01N7BcwGfu6i8rphTBozC7LV","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"7d383dce-87bf-467e-baa9-904a387f2b94"}
