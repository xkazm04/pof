 session_id?: string;\n    63‚Üí  };\n    64‚Üí  duration_ms?: number;\n    65‚Üí  cost_usd?: number;\n    66‚Üí  is_error?: boolean;\n    67‚Üí}\n    68‚Üí\n    69‚Üíexport type CLIMessage = CLISystemMessage | CLIAssistantMessage | CLIUserMessage | CLIResultMessage;\n    70‚Üí\n    71‚Üíexport interface CLIExecutionEvent {\n    72‚Üí  type: 'init' | 'text' | 'tool_use' | 'tool_result' | 'result' | 'error' | 'stdout';\n    73‚Üí  data: Record<string, unknown>;\n    74‚Üí  timestamp: number;\n    75‚Üí}\n    76‚Üí\n    77‚Üíexport type CLIEventListener = (event: CLIExecutionEvent) => void;\n    78‚Üí\n    79‚Üíexport interface CLIExecution {\n    80‚Üí  id: string;\n    81‚Üí  projectPath: string;\n    82‚Üí  prompt: string;\n    83‚Üí  process: ChildProcess | null;\n    84‚Üí  sessionId?: string;\n    85‚Üí  status: 'running' | 'completed' | 'error' | 'aborted';\n    86‚Üí  startTime: number;\n    87‚Üí  endTime?: number;\n    88‚Üí  events: CLIExecutionEvent[];\n    89‚Üí  logFilePath?: string;\n    90‚Üí  /** Listeners notified immediately when new events arrive */\n    91‚Üí  listeners: Set<CLIEventListener>;\n    92‚Üí}\n    93‚Üí\n    94‚Üíconst globalForExecutions = globalThis as unknown as {\n    95‚Üí  cliActiveExecutions: Map<string, CLIExecution> | undefined;\n    96‚Üí};\n    97‚Üí\n    98‚Üíconst activeExecutions = globalForExecutions.cliActiveExecutions ?? new Map<string, CLIExecution>();\n    99‚Üí\n   100‚Üíif (!globalForExecutions.cliActiveExecutions) {\n   101‚Üí  globalForExecutions.cliActiveExecutions = activeExecutions;\n   102‚Üí}\n   103‚Üí\n   104‚Üífunction getLogsDirectory(projectPath: string): string {\n   105‚Üí  return path.join(projectPath, '.claude', 'logs');\n   106‚Üí}\n   107‚Üí\n   108‚Üífunction ensureLogsDirectory(projectPath: string): void {\n   109‚Üí  const logsDir = getLogsDirectory(projectPath);\n   110‚Üí  if (!fs.existsSync(logsDir)) {\n   111‚Üí    fs.mkdirSync(logsDir, { recursive: true });\n   112‚Üí  }\n   113‚Üí}\n   114‚Üí\n   115‚Üífunction getLogFilePath(projectPath: string, executionId: string): string {\n   116‚Üí  const timestamp = new Date().toISOString().replace(/[:.]/g, '-');\n   117‚Üí  const sanitized = executionId.replace(/[^a-zA-Z0-9-_]/g, '_').slice(0, 50);\n   118‚Üí  return path.join(getLogsDirectory(projectPath), `terminal_${sanitized}_${timestamp}.log`);\n   119‚Üí}\n   120‚Üí\n   121‚Üíexport function parseStreamJsonLine(line: string): CLIMessage | null {\n   122‚Üí  try {\n   123‚Üí    const trimmed = line.trim();\n   124‚Üí    if (!trimmed || trimmed.startsWith('[')) return null;\n   125‚Üí    return JSON.parse(trimmed) as CLIMessage;\n   126‚Üí  } catch {\n   127‚Üí    return null;\n   128‚Üí  }\n   129‚Üí}\n   130‚Üí\n   131‚Üíexport function extractTextContent(msg: CLIAssistantMessage): string {\n   132‚Üí  return msg.message.content.filter(c => c.type === 'text').map(c => c.text || '').join('\\n');\n   133‚Üí}\n   134‚Üí\n   135‚Üíexport function extractToolUses(msg: CLIAssistantMessage): Array<{ id: string; name: string; input: Record<string, unknown> }> {\n   136‚Üí  return msg.message.content.filter(c => c.type === 'tool_use').map(c => ({ id: c.id || '', name: c.name || '', input: c.input || {} }));\n   137‚Üí}\n   138‚Üí\n   139‚Üíexport function startExecution(\n   140‚Üí  projectPath: string,\n   141‚Üí  prompt: string,\n   142‚Üí  resumeSessionId?: string,\n   143‚Üí  onEvent?: (event: CLIExecutionEvent) => void\n   144‚Üí): string {\n   145‚Üí  const executionId = `exec-${Date.now()}-${Math.random().toString(36).slice(2, 8)}`;\n   146‚Üí\n   147‚Üí  // Ensure project directory exists (for fresh projects the dir may not exist yet)\n   148‚Üí  if (!fs.existsSync(projectPath)) {\n   149‚Üí    fs.mkdirSync(projectPath, { recursive: true });\n   150‚Üí  }"}]},"parent_tool_use_id":"toolu_01N7BcwGfu6i8rphTBozC7LV","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"6189c328-3818-4b3f-af70-45b50725be68"}
