   onDelete={async () => {\n    45‚Üí            await director.deleteSession(session.id);\n    46‚Üí            handleBackToOverview();\n    47‚Üí          }}\n    48‚Üí          simulating={director.simulating}\n    49‚Üí          getFindings={director.getFindings}\n    50‚Üí          getEvents={director.getEvents}\n    51‚Üí        />\n    52‚Üí      );\n    53‚Üí    }\n    54‚Üí  }\n    55‚Üí\n    56‚Üí  return (\n    57‚Üí    <div className=\"h-full flex flex-col\">\n    58‚Üí      {/* Header */}\n    59‚Üí      <div className=\"px-6 pt-6 pb-0\">\n    60‚Üí        <div className=\"flex items-center gap-3 mb-4\">\n    61‚Üí          <div\n    62‚Üí            className=\"w-8 h-8 rounded-lg flex items-center justify-center\"\n    63‚Üí            style={{ backgroundColor: `${ACCENT}15`, border: `1px solid ${ACCENT}30` }}\n    64‚Üí          >\n    65‚Üí            <Clapperboard className=\"w-4.5 h-4.5\" style={{ color: ACCENT }} />\n    66‚Üí          </div>\n    67‚Üí          <div>\n    68‚Üí            <h1 className=\"text-lg font-semibold text-text\">AI Game Director</h1>\n    69‚Üí            <p className=\"text-xs text-text-muted\">\n    70‚Üí              Autonomous playtesting agent that plays and critiques your game\n    71‚Üí            </p>\n    72‚Üí          </div>\n    73‚Üí        </div>\n    74‚Üí\n    75‚Üí        {/* Tab bar */}\n    76‚Üí        <div className=\"flex items-center gap-1 border-b border-border\">\n    77‚Üí          <TabButton\n    78‚Üí            label=\"Overview\"\n    79‚Üí            icon={LayoutDashboard}\n    80‚Üí            active={activeTab === 'overview'}\n    81‚Üí            onClick={() => setActiveTab('overview')}\n    82‚Üí            accent={ACCENT}\n    83‚Üí          />\n    84‚Üí          <TabButton\n    85‚Üí            label=\"New Session\"\n    86‚Üí            icon={Activity}\n    87‚Üí            active={activeTab === 'new-session'}\n    88‚Üí            onClick={() => setActiveTab('new-session')}\n    89‚Üí            accent={ACCENT}\n    90‚Üí          />\n    91‚Üí          <TabButton\n    92‚Üí            label=\"All Findings\"\n    93‚Üí            icon={FileSearch}\n    94‚Üí            active={activeTab === 'findings'}\n    95‚Üí            onClick={() => setActiveTab('findings')}\n    96‚Üí            accent={ACCENT}\n    97‚Üí          />\n    98‚Üí        </div>\n    99‚Üí      </div>\n   100‚Üí"}]},"parent_tool_use_id":"toolu_01N7BcwGfu6i8rphTBozC7LV","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"2bad91af-0f4d-4448-a44e-e923bc5e543d"}
