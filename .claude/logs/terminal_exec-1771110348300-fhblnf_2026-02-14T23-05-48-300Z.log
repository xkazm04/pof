      const startTime = new Date(taskStartRef.current).getTime();\n    73‚Üí          const durationMs = Date.now() - startTime;\n    74‚Üí          const hadProjectContext = lastPromptRef.current.includes('## Project Context')\n    75‚Üí            || lastPromptRef.current.includes('## Build Command');\n    76‚Üí\n    77‚Üí          recordSessionOutcome({\n    78‚Üí            moduleId: moduleIdRef.current,\n    79‚Üí            sessionKey: sessionKeyRef.current,\n    80‚Üí            prompt: lastPromptRef.current,\n    81‚Üí            hadProjectContext,\n    82‚Üí            success,\n    83‚Üí            durationMs,\n    84‚Üí            startedAt: taskStartRef.current,\n    85‚Üí          });\n    86‚Üí        }\n    87‚Üí\n    88‚Üí        onCompleteRef.current?.(success);\n    89‚Üí      }, 50);\n    90‚Üí    }\n    91‚Üí    prevRunningRef.current = isRunning;\n    92‚Üí  }, [isRunning]);\n    93‚Üí\n    94‚Üí  const sendPrompt = useCallback(\n    95‚Üí    (prompt: string) => {\n    96‚Üí      // Track for analytics\n    97‚Üí      lastPromptRef.current = prompt;\n    98‚Üí      taskStartRef.current = new Date().toISOString();\n    99‚Üí\n   100‚Üí      let tabId = findSessionByKey(opts.sessionKey);\n   101‚Üí      if (!tabId) {\n   102‚Üí        tabId = createSession({\n   103‚Üí          label: opts.label,\n   104‚Üí          accentColor: opts.accentColor,\n   105‚Üí          moduleId: opts.moduleId,\n   106‚Üí          sessionKey: opts.sessionKey,\n   107‚Üí          projectPath,\n   108‚Üí        });\n   109‚Üí      }\n   110‚Üí      setActiveTab(tabId);\n   111‚Üí\n   112‚Üí      // Small delay to allow the terminal component to mount and attach its event listener\n   113‚Üí      setTimeout(() => {\n   114‚Üí        window.dispatchEvent(\n   115‚Üí          new CustomEvent('pof-cli-prompt', {\n   116‚Üí            detail: { tabId, prompt },\n   117‚Üí          })\n   118‚Üí        );\n   119‚Üí      }, 100);\n   120‚Üí    },\n   121‚Üí    [findSessionByK