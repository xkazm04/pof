 animate={{ opacity: 1 }}\n    41‚Üí          transition={{ duration: 0.2 }}\n    42‚Üí          className=\"flex flex-col items-center gap-3\"\n    43‚Üí        >\n    44‚Üí          <Gamepad2 className=\"w-12 h-12 text-[#00ff88]\" />\n    45‚Üí          <span className=\"text-lg font-semibold text-text tracking-wide\">\n    46‚Üí            POF\n    47‚Üí          </span>\n    48‚Üí          <div className=\"w-5 h-5 border-2 border-[#00ff88]/30 border-t-[#00ff88] rounded-full animate-spin\" />\n    49‚Üí        </motion.div>\n    50‚Üí      </div>\n    51‚Üí    );\n    52‚Üí  }\n    53‚Üí\n    54‚Üí  if (!isSetupComplete) {\n    55‚Üí    return <SetupWizard />;\n    56‚Üí  }\n    57‚Üí\n    58‚Üí  return (\n    59‚Üí    <div className=\"h-screen flex flex-col overflow-hidden bg-background\">\n    60‚Üí      <TopBar />\n    61‚Üí      <div className=\"flex-1 flex overflow-hidden\">\n    62‚Üí        <Sidebar />\n    63‚Üí        <ModuleRenderer />\n    64‚Üí        <ActivityFeedPanel />\n    65‚Üí      </div>\n    66‚Üí      <CLIBottomPanel />\n    67‚Üí    </div>\n    68‚Üí  );\n    69‚Üí}\n    70‚Üí"}]},"parent_tool_use_id":"toolu_01N7BcwGfu6i8rphTBozC7LV","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"674bca54-d328-44a2-bafd-78a04e99670a"}
