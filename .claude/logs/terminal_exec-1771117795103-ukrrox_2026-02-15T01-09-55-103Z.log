lineTerminalHeight(Math.max(minHeight, Math.min(maxHeight, startHeight + delta)));\n    42‚Üí      };\n    43‚Üí\n    44‚Üí      const onMouseUp = () => {\n    45‚Üí        document.body.style.cursor = '';\n    46‚Üí        document.body.style.userSelect = '';\n    47‚Üí        document.removeEventListener('mousemove', onMouseMove);\n    48‚Üí        document.removeEventListener('mouseup', onMouseUp);\n    49‚Üí      };\n    50‚Üí\n    51‚Üí      document.addEventListener('mousemove', onMouseMove);\n    52‚Üí      document.addEventListener('mouseup', onMouseUp);\n    53‚Üí    },\n    54‚Üí    [height, minHeight, maxHeight, setInlineTerminalHeight]\n    55‚Üí  );\n    56‚Üí\n    57‚Üí  if (!session) return null;\n    58‚Üí\n    59‚Üí  return (\n    60‚Üí    <motion.div\n    61‚Üí      className=\"border-t border-border bg-surface-deep flex flex-col overflow-hidden\"\n    62‚Üí      style={{ height }}\n    63‚Üí      initial={{ height: 0, opacity: 0 }}\n    64‚Üí      animate={{ height, opacity: 1 }}\n    65‚Üí      exit={{ height: 0, opacity: 0 }}\n    66‚Üí      transition={{ type: 'spring', stiffness: 300, damping: 30, mass: 0.8 }}\n    67‚Üí    >\n    68‚Üí      {/* Resize handle (top edge ‚Äî drag up to grow) */}\n    69‚Üí      <div\n    70‚Üí        onMouseDown={handleResizeMouseDown}\n    71‚Üí        className=\"h-2 w-full cursor-ns-resize bg-border hover:bg-border-bright transition-colors flex items-center justify-center group shrink-0\"\n    72‚Üí        style={{ boxShadow: '0 -2px 4px rgba(0,0,0,0.3)' }}\n    73‚Üí      >\n    74‚Üí        <div className=\"flex items-center gap-1\">\n    75‚Üí          <div className=\"w-[3px] h-[3px] rounded-full bg-border-bright group-hover:bg-text-muted group-hover:w-1 group-hover:h-1 transition-all\" />\n    76‚Üí          <div className=\"w-[3px] h-[3px] rounded-full bg-border-bright group-hover:bg-text-muted group-hover:w-1 group-hover:h-1 transition-all\" />\n    77‚Üí          <div className=\"w-[3px] h-[3px] round