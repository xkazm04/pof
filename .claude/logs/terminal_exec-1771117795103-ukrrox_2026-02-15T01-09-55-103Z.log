ide react-window.\n   239‚Üí */\n   240‚Üíconst TAIL_COUNT = 8;\n   241‚Üí\n   242‚Üíexport function TerminalOutput({\n   243‚Üí  logs, isStreaming, queuePendingCount,\n   244‚Üí  scrollRef, listRef, onScroll,\n   245‚Üí  buildParseCache, onBuildFix,\n   246‚Üí  scrollBtnVisible, isAutoScroll, unseenCount, onScrollToBottom,\n   247‚Üí}: TerminalOutputProps) {\n   248‚Üí  const [expandedGroups, setExpandedGroups] = useState<Set<string>>(new Set());\n   249‚Üí  const [expandedPairs, setExpandedPairs] = useState<Set<string>>(new Set());\n   250‚Üí\n   251‚Üí  const toggleGroup = useCallback((id: string) => {\n   252‚Üí    setExpandedGroups((prev) => {\n   253‚Üí      const next = new Set(prev);\n   254‚Üí      if (next.has(id)) next.delete(id); else next.add(id);\n   255‚Üí      return next;\n   256‚Üí    });\n   257‚Üí  }, []);\n   258‚Üí\n   259‚Üí  const togglePair = useCallback((id: string) => {\n   260‚Üí    setExpandedPairs((prev) => {\n   261‚Üí      const next = new Set(prev);\n   262‚Üí      if (next.has(id)) next.delete(id); else next.add(id);\n   263‚Üí      return next;\n   264‚Üí    });\n   265‚Üí  }, []);\n   266‚Üí\n   267‚Üí  // Split logs: older logs go to the virtualizer, recent tail gets grouped rendering\n   268‚Üí  const { virtualizedLogs, tailLogs } = useMemo(() => {\n   269‚Üí    const splitIndex = Math.max(0, logs.length - TAIL_COUNT);\n   270‚Üí    return {\n   271‚Üí      virtualizedLogs: logs.slice(0, splitIndex),\n   272‚Üí      tailLogs: logs.slice(splitIndex),\n   273‚Üí    };\n   274‚Üí  }, [logs]);\n   275‚Üí\n   276‚Üí  const groupedTailLogs = useMemo(() => groupLogs(tailLogs), [tailLogs]);\n   277‚Üí\n   278‚Üí  // Track which log IDs have been seen so we only animate new arrivals\n   279‚Üí  const seenIdsRef = useRef<Set<string>>(new Set());\n   280‚Üí  useEffect(() => {\n   281‚Üí    // Mark all current log ids as seen after render\n   282‚Üí    const timer = requestAnimationFrame(() => {\n   283‚Üí      for (const log of logs) {\n   284‚Üí        seenIdsRef.current.add(log.id);\n   285‚Üí      }\n   286‚Üí    });\n   287‚Üí    return () => cancelAnimationFrame(timer);\n   288‚Üí  }, [logs]);\n   289‚Üí\n   290‚Üí  const isNewEntry = useCallback((id: string) => !seenIdsRef.current.has(id), []);\n   291‚Üí\n   292‚Üí  const renderSingleLog = useCallback((log: LogEntry) => {\n   293‚Üí    const parsed = buildParseCache.current.get(log.id);\n   294‚Üí    if (parsed && parsed.isBuildOutput) {\n   295‚Üí      const errors = parsed.diagnostics.filter((d) => d.severity === 'error');\n   296‚Üí      const warningGroups = aggregateWarnings(parsed.diagnostics);\n   297‚Üí      return (\n   298‚Üí        <div>\n   299‚Üí          <div className=\"flex items-start gap-2 px-3 py-0.5 hover:bg-surface-hover/40 transition-colors duration-150\">\n   300‚Üí            <span className=\"flex-shrink-0 mt-0.5\">{getLogIcon(log.type, log.toolName)}</span>\n   301‚Üí            <span className={`text-xs leading-relaxed break-all ${getLogTextClass(log.type)}`}>{formatLogContent(log)}</span>\n   302‚Üí          </div>\n   303‚Üí          {errors.map((d) => (\n   304‚Üí            <ErrorCard key={d.id} diagnostic={d} onFix={onBuildFix} />\n   305‚Üí          ))}\n   306‚Üí          {warningGroups.length > 0 && (\n   307‚Üí            <WarningAggregator groups={warningGroups} onFix={onBuildFix} />\n   308‚Üí          )}\n   309‚Üí          {parsed.summary && <BuildSummaryCard summary={parsed.summary} />}\n   310‚Üí        </div>\n   311‚Üí      );\n   312‚Üí    }\n   313‚Üí\n   314‚Üí    return (\n   315‚Üí      <div className=\"flex items-start gap-2 px-3 py-0.5 hover:bg-surface-hover/40 transition-colors duration-150\">\n   316‚Üí        <span className=\"flex-shrink-0 mt-0.5\">{getLogIcon(log.type, log.toolName)}</span>\n   317‚Üí        <span className={`text-xs leading-relaxed break-all ${getLogTextClass(log.type)}`}>{formatLogContent(log)}</span>\n   318‚Üí      </div>\n   319‚Üí    );\n   320‚Üí  }, [buildParseCache, onBuildFix]);\n   321‚Üí\n   322‚Üí  const renderGroupedEntries = (entries: GroupedLogEntry[]) => (\n   323‚Üí    <>\n   324‚Üí      {entries.map((entry) => {\n   325‚Üí        const key = entry.kind === 'single' ? entry.log.id : entry.id;\n   326‚Üí        const animClass = isNewEntry(key) ? 'log-enter' : '';\n   327‚Üí        return (\n   328‚Üí          <div key={key} className={animClass}>\n   329‚Üí            {entry.kind === 'single' ? (\n   330‚Üí              renderSingleLog(entry.log)\n   331‚Üí            ) : entry.kind === 'tool_pair' ? (\n   332‚Üí              <ToolPairRow toolUse={entry.toolUse} toolResult={entry.toolResult} isExpanded={expandedPairs.has(entry.toolUse.id)} onToggle={() => togglePair(entry.toolUse.id)} buildParsed={buildParseCache.current.get(entry.toolResult.id)} onBuildFix={onBuildFix} />\n   333‚Üí            ) : (\n   334‚Üí              <ToolBatchRow pairs={entry.pairs} isExpanded={expandedGroups.has(entry.id)} onToggle={() => toggleGroup(entry.id)} expandedPairs={expandedPairs} onTogglePair={togglePair} buildCache={buildParseCache.current} onBuildFix={onBuildFix} />\n   335‚Üí            )}\n   336‚Üí          </div>\n   337‚Üí        );\n   338‚Üí      })}\n   339‚Üí    </>\n   340‚Üí  );\n   341‚Üí\n   342‚Üí  // Track scroll button visibility with CSS class\n   343‚Üí  const [scrollBtnMounted, setScrollBtnMounted] = useState(false);\n   344‚Üí  useEffect(() => {\n   345‚Üí    if (scrollBtnVisible && !isAutoScroll && logs.length > 10) {\n   346‚Üí      setScrollBtnMounted(true);\n   347‚Üí    } else {\n   348‚Üí      // Small delay for exit transition\n   349‚Üí      const timer = setTimeout(() => setScrollBtnMounted(false), 150);\n   350‚Üí      return () => clearTimeout(timer);\n   351‚Üí    }\n   352‚Üí  }, [scrollBtnVisible, isAutoScroll, logs.length]);\n   353‚Üí\n   354‚Üí  const showScrollBtn = scrollBtnVisible && !isAutoScroll && logs.length > 10;\n   355‚Üí\n   356‚Üí  return (\n   357‚Üí    <div ref={scrollRef} onScroll={onScroll} className=\"flex-1 overflow-y-auto\">\n   358‚Üí      {logs.length === 0 ? (\n   359‚Üí        <div className=\"flex items-center justify-center h-full text-text-muted text-xs\">\n   360‚Üí          {queuePendingCount > 0 ? 'Waiting to start...' : 'Enter a prompt to start'}\n   361‚Üí        </div>\n   362‚Üí      ) : (\n   363‚Üí        <div className=\"py-1\">\n   364‚Üí          {/* Virtualized older logs ‚Äî always active from line 1 */}\n   365‚Üí          {virtualizedLogs.length > 0 && (\n   366‚Üí            <List<LogRowData>\n   367‚Üí              listRef={listRef}\n   368‚Üí              defaultHeight={Math.min(virtualizedLogs.length * LOG_ITEM_HEIGHT, 400)}\n   369‚Üí              rowCount={virtualizedLogs.length}\n   370‚Üí              rowHeight={LOG_ITEM_HEIGHT}\n   371‚Üí              overscanCount={OVERSCAN_COUNT}\n   372‚Üí              rowComponent={LogRow}\n   373‚Üí              rowProps={{ logs: virtualizedLogs }}\n   374‚Üí            />\n   375‚Üí          )}\n   376‚Üí          {/* Recent tail ‚Äî grouped with CSS slide-in transitions */}\n   377‚Üí          {renderGroupedEntries(groupedTailLogs)}\n   378‚Üí        </div>\n   379‚Üí      )}\n   380‚Üí\n   381‚Üí      {isStreaming && (\n   382‚Üí        <div className=\"flex items-center gap-2 px-3 py-1 text-[#3b82f6] text-xs\">\n   383‚Üí          <Loader2 className=\"w-3 h-3 animate-spin\" />\n   384‚Üí          <span>Working...</span>\n   385‚Üí        </div>\n   386‚Üí      )}\n   387‚Üí\n   388‚Üí      {/* Scroll to bottom pill ‚Äî CSS transitions instead of AnimatePresence */}\n   389‚Üí      {scrollBtnMounted && (\n   390‚Üí        <div\n   391‚Üí          className={`sticky bottom-2 flex justify-end pr-2 pointer-events-none -mt-7 z-10 ${showScrollBtn ? 'scroll-btn-enter' : 'scroll-btn-exit'}`}\n   392‚Üí        >\n   393‚Üí          <button\n   394‚Üí            onClick={onScrollToBottom}\n   395‚Üí            className=\"pointer-events-auto flex items-center gap-1 h-6