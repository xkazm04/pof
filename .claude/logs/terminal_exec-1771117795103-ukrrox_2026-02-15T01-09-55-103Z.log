zedLogCount = useMemo(() => Math.max(0, tq.logs.length - 8), [tq.logs.length]);\n    37‚Üí\n    38‚Üí  const scroll = useScrollSync({\n    39‚Üí    logCount: tq.logs.length,\n    40‚Üí    visible,\n    41‚Üí    virtualizedLogCount,\n    42‚Üí    listRef,\n    43‚Üí  });\n    44‚Üí\n    45‚Üí  // Keep the indirection ref pointing to the real callback\n    46‚Üí  useEffect(() => {\n    47‚Üí    addUnseenCountRef.current = scroll.addUnseenCount;\n    48‚Üí  }, [scroll.addUnseenCount]);\n    49‚Üí\n    50‚Üí  // --- Submit / Resume / BuildFix ---\n    51‚Üí\n    52‚Üí  const pendingPromptRef = useRef<string | null>(null);\n    53‚Üí\n    54‚Üí  const handleSubmit = useCallback(async (resume = false) => {\n    55‚Üí    if (!input.trim() || tq.isStreaming) return;\n    56‚Üí    const prompt = input.trim();\n    57‚Üí    setInput('');\n    58‚Üí    history.resetHeight();\n    59‚Üí    history.pushHistory(prompt);\n    60‚Üí    await tq.submitPrompt(prompt, resume && tq.sessionId !== null);\n    61‚Üí  }, [input, tq, history]);\n    62‚Üí\n    63‚Üí  const handleInputSubmit = useCallback((resume: boolean) => {\n    64‚Üí    handleSubmit(resume);\n    65‚Üí  }, [handleSubmit]);\n    66‚Üí\n    67‚Üí  const handleResume = useCallback(() => {\n    68‚Üí    setInput('Continue');\n    69‚Üí    setTimeout(() => handleSubmit(true), 0);\n    70‚Üí  }, [handleSubmit]);\n    71‚Üí\n    72‚Üí  // Listen for pof-cli-prompt events\n    73‚Üí  useEffect(() => {\n    74‚Üí    const handler = (e: Event) => {\n    75‚Üí      const { tabId, prompt } = (e as CustomEvent).detail;\n    76‚Üí      if (tabId !== instanceId) return;\n    77‚Üí      pendingPromptRef.current = prompt;\n    78‚Üí      setInput(prompt);\n    79‚Üí    };\n    80‚Üí    window.addEventListener('pof-cli-prompt', handler);\n    81‚Üí    return () => window.removeEventListener('pof-cli-prompt', handler);\n    82‚Üí  }, [instanceId]);\n    83‚Üí\n    84‚Üí  // Auto-submit when input is set from a pof-cli-prompt event\n    85‚Üí  useEffect(() => {\n    86‚Üí    if (pendingPromptRef.current && input === pendingPromptRef.current && !tq.isStreaming) {\n    87‚Üí      pendingPromptRef.current = null;\n    88‚Üí      const timer = setTimeout(() => handleSubmit(tq.sessionId !== null), 50);\n    89‚Üí      return () => clearTimeout(timer);\n    90‚Üí    }\n    91‚Üí  }, [input, tq.isStreaming, tq.sessionId, handleSubmit]);\n    92‚Üí\n    93‚Üí  // Build fix prompt injection\n    94‚Üí  const handleBuildFix = useCallback((prompt: string) => {\n    95‚Üí    if (tq.isStreaming) return;\n    96‚Üí    setInput(prompt);\n    97‚Üí    pendingPromptRef.current = prompt;\n    98‚Üí  }, [tq.isStreaming]);\n    99‚Üí\n   100‚Üí  // --- Derived counts ---\n   101‚Üí\n   102‚Üí  const editCount = tq.fileChanges.filter(c => c.changeType === 'edit').length;\n   103‚Üí  const writeCount = tq.fileChanges.filter(c => c.changeType === 'write').length;\n   104‚Üí  const queuePendingCount = taskQueue.filter(t => t.status === 'pending').length;\n   105‚Üí\n   106‚Üí  return (\n   107‚Üí    <div className={`flex flex-col bg-background overflow-hidden ${className}`}>\n   108‚Üí      <TerminalHeader\n   109‚Üí        title={title}\n   110‚Üí        sessionId={tq.sessionId}\n   111‚Üí        isStreaming={tq.isStreaming}\n   112‚Üí        executionInfo={tq.executionInfo}\n   113‚Üí        lastResult={tq.lastResult}\n   114‚Üí        logFilePath={tq.logFilePath}\n   115‚Üí        editCount={editCount}\n   116‚Üí        writeCount={writeCount}\n   117‚Üí        queuePendingCount={queuePendingCount}\n   118‚Üí        onClear={tq.handleClear}\n   119‚Üí        onResume={handleResume}\n   120‚Üí      />\n   121‚Üí\n   122‚Üí      <TerminalOutput\n   123‚Üí        logs={tq.logs}\n   124‚Üí        isStreaming={tq.isStreaming}\n   125‚Üí        queuePendingCount={queuePendingCount}\n   126‚Üí        scrollRef={scroll.scrollRef}\n   127‚Üí        listRef={listRef}\n   128‚Üí        onScroll={scroll.handleScroll}\n   129‚Üí        buildParseCache={tq.buildParseCache}\n   130‚Üí        onBuildFix={handleBuildFix}\n   131‚Üí        scrollBtnVisible={scroll.scrollBtnVisible}\n   132‚Üí        isAutoScroll={scroll.isAutoScroll}\n   133‚Üí        unseenCount={scroll.unseenCount}\n   134‚Üí        onScrollToBottom={scroll.scrollToBottom}\n   135‚Üí      />\n   136‚Üí\n   137‚Üí      <TerminalInput\n   138‚Üí        input={input}\n   139‚Üí        setInput={setInput}\n   140‚Üí        inputRef={history.inputRef}\n   141‚Üí        isStreaming={tq.isStreaming}\n   142‚Üí        onSubmit={handleInputSubmit}\n   143‚Üí        onAbort={tq.handleAbort}\n   144‚Üí        onNavigateHistory={history.navigateHistory}\n   145‚Üí      />\n   146‚Üí    </div>\n   147‚Üí  );\n   148‚Üí}\n   149‚Üí"}]},"parent_tool_use_id":"toolu_01JwsGXhfpGRwevuAnZuNUE9","session_id":"dd826e2f-f18b-4b43-8186-c7a7070a449e","uuid":"46f2fcd8-1493-4446-9928-d2ac012ca184"}
