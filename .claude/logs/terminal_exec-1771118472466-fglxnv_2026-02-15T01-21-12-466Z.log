ate(dynamicContext.scannedAt).getTime();\n    97â†’          if (age < SCAN_CACHE_MS) return;\n    98â†’        }\n    99â†’\n   100â†’        set({ isScanning: true, scanError: null });\n   101â†’\n   102â†’        try {\n   103â†’          const data = await apiFetch<{\n   104â†’            scannedAt: string;\n   105â†’            classes: DynamicProjectContext['classes'];\n   106â†’            plugins: DynamicProjectContext['plugins'];\n   107â†’            buildDependencies: DynamicProjectContext['buildDependencies'];\n   108â†’            sourceFileCount: number;\n   109â†’          }>('/api/filesystem/scan-project', {\n   110â†’            method: 'POST',\n   111â†’            headers: { 'Content-Type': 'application/json' },\n   112â†’            body: JSON.stringify({ projectPath, moduleName: projectName }),\n   113â†’          });\n   114â†’          set({\n   115â†’            isScanning: false,\n   116â†’            scanError: null,\n   117â†’            dynamicContext: {\n   118â†’              scannedAt: data.scannedAt,\n   119â†’              classes: data.classes,\n   120â†’              plugins: data.plugins,\n   121â†’              buildDependencies: data.buildDependencies,\n   122â†’              sourceFileCount: data.sourceFileCount,\n   123â†’            },\n   124â†’          });\n   125â†’        } catch (err) {\n   126â†’          set({\n   127â†’            isScanning: false,\n   128â†’            scanError: err instanceof Error ? err.message : 'Failed to scan project',\n   129â†’          });\n   130â†’        }\n   131â†’      },\n   132â†’\n   133â†’      saveToRecent: async () => {\n   134â†’        const { projectName, projectPath, ueVersion, isSetupComplete } = get();\n   135â†’        if (!projectName || !projectPath || !isSetupComplete) return;\n   136â†’\n   137â†’        // Grab checklist progress from moduleStore (cross-store read)\n   138â†’        let checklistProgress = {};\n   139â†’        try {\n   140â†’          const moduleStoreRaw = localStorage.getItem('pof-modules');\n   141â†’          if (moduleStoreRaw) {\n   142â†’            const parsed = JSON.parse(moduleStoreRaw);\n   143â†’            checklistProgress = parsed?.state?.checklistProgress ?? {};\n   144â†’          }\n   145â†’        } catch { /* ignore */ }\n   146â†’\n   147â†’        try {\n   148â†’          await apiFetch('/api/recent-projects', {\n   149â†’            method: 'POST',\n   150â†’            headers: { 'Content-Type': 'application/json' },\n   151â†’            body: JSON.stringify({\n   152â†’              action: 'save',\n   153â†’              projectName,\n   154â†’              projectPath,\n   155â†’              ueVersion,\n   156â†’              checklistProgress,\n   157â†’            }),\n   158â†’          });\n   159â†’          // Refresh the list\n   160â†’          get().loadRecentProjects();\n   161â†’        } catch {\n   162â†’          // Silent fail â€” not critical\n   163â†’        }\n   164â†’      },\n   165â†’\n   166â†’      loadRecentProjects: async () => {\n   167â†’        try {\n   168â†’          const projects = await apiFetch<RecentProject[]>('/api/recent-projects');\n   169â†’          set({ recentProjects: projects });\n   170â†’        } catch {\n   171â†’          // Silent fail\n   172â†’        }\n   173â†’      },\n   174â†’\n   175â†’      switchProject: async (projectId: string) => {\n   176â†’        const { recentProjects, projectPath, isSetupComplete } = get();\n   177â†’        const target = recentProjects.find((p) => p.id === projectId);\n   178â†’        if (!target) return;\n   179â†’\n   180â†’        // Save current project state before switching\n   181â†’        if (projectPath && isSetupComplete) {\n   182â†’          await get().saveToRecent();\n   183â†’        }\n   184â†’\n   185â†’        // Touch the target project's last_opened_at\n   186â†’        try {\n   187â†’          await apiFetch('/api/recent-projects', {\n   188â†’            method: