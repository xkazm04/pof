   82â†’    if (!trimmed || trimmed === projectName) {\n    83â†’      setRenaming(false);\n    84â†’      return;\n    85â†’    }\n    86â†’\n    87â†’    // Update the path if it ends with the old project name\n    88â†’    let newPath = projectPath;\n    89â†’    const pathSep = projectPath.includes('/') ? '/' : '\\\\';\n    90â†’    const pathParts = projectPath.split(/[/\\\\]/);\n    91â†’    if (pathParts[pathParts.length - 1] === projectName) {\n    92â†’      pathParts[pathParts.length - 1] = trimmed;\n    93â†’      newPath = pathParts.join(pathSep);\n    94â†’    }\n    95â†’\n    96â†’    setProject({ projectName: trimmed, projectPath: newPath });\n    97â†’\n    98â†’    setRenaming(false);\n    99â†’    setDropdownOpen(false);\n   100â†’  }, [renameValue, projectName, projectPath, setProject]);\n   101â†’\n   102â†’  const handleDelete = useCallback(() => {\n   103â†’    // Clear all CLI sessions\n   104â†’    const { tabOrder, removeSession } = useCLIPanelStore.getState();\n   105â†’    for (const tabId of [...tabOrder]) {\n   106â†’      removeSession(tabId);\n   107â†’    }\n   108â†’\n   109â†’    // Reset project store (clears localStorage too)\n   110â†’    resetProject();\n   111â†’\n   112â†’    setDropdownOpen(false);\n   113â†’    setConfirmDelete(false);\n   114â†’  }, [resetProject]);\n   115â†’\n   116â†’  const handleSwitchProject = useCallback(async (project: RecentProject) => {\n   117â†’    if (project.projectPath === projectPath) {\n   118â†’      setDropdownOpen(false);\n   119â†’      setShowSwitcher(false);\n   120â†’      return;\n   121â†’    }\n   122â†’\n   123â†’    setSwitching(project.id);\n   124â†’    // Clear CLI sessions before switching\n   125â†’    const { tabOrder, removeSession } = useCLIPanelStore.getState();\n   126â†’    for (const tabId of [...tabOrder]) {\n   127â†’      removeSession(tabId);\n   128â†’    }\n   129â†’\n   130â†’    await switchProject(project.id);\n   131â†’    setSwitching(null);\n   132â†’   