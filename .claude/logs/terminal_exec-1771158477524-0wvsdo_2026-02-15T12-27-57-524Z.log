 | 'status' | 'quality' | 'reviewed';\n    33→type SortDir = 'asc' | 'desc';\n    34→\n    35→const STATUS_ORDER: Record<FeatureStatus, number> = {\n    36→  missing: 0,\n    37→  unknown: 1,\n    38→  partial: 2,\n    39→  implemented: 3,\n    40→};\n    41→\n    42→/** Read filter/sort state from URL search params */\n    43→function readUrlParams(): {\n    44→  search?: string;\n    45→  statuses?: FeatureStatus[];\n    46→  qualityMin?: number;\n    47→  qualityMax?: number;\n    48→  sortKey?: SortKey;\n    49→  sortDir?: SortDir;\n    50→} {\n    51→  if (typeof window === 'undefined') return {};\n    52→  const params = new URLSearchParams(window.location.search);\n    53→  const result: ReturnType<typeof readUrlParams> = {};\n    54→\n    55→  const q = params.get('q');\n    56→  if (q) result.search = q;\n    57→\n    58→  const st = params.get('status');\n    59→  if (st) {\n    60→    const valid = st.split(',').filter((s): s is FeatureStatus =>\n    61→      ['implemented', 'partial', 'missing', 'unknown'].includes(s)\n    62→    );\n    63→    if (valid.length > 0) result.statuses = valid;\n    64→  }\n    65→\n    66→  const qMin = params.get('qmin');\n    67→  const qMax = params.get('qmax');\n    68→  if (qMin) { const n = parseInt(qMin); if (n >= 1 && n <= 5) result.qualityMin = n; }\n    69→  if (qMax) { const n = parseInt(qMax); if (n >= 1 && n <= 5) result.qualityMax = n; }\n    70→\n    71→  const sk = params.get('sort');\n    72→  if (sk && ['name', 'status', 'quality', 'reviewed'].includes(sk)) result.sortKey = sk as SortKey;\n    73→\n    74→  const sd = params.get('dir');\n    75→  if (sd && ['asc', 'desc'].includes(sd)) result.sortDir = sd as SortDir;\n    76→\n    77→  return result;\n    78→}\n    79→\n    80→/** Write filter/sort state to URL search params (replaceState, no navigation) */\n    81→function writeUrlParams(state: {\n    82→  search: string;\n    83→  statuses: FeatureStatus[];\n    84→  qualityMin: number;\n    85→  qualityMax: number;\n    86→  sortKey: SortKey;\n    87→  sortDir: SortDir;\n    88→}) {\n    89→  if (typeof window === 'undefined') return;\n    90→  const params = new URLSearchParams(window.location.search);\n    91→\n    92→  // Only write non-default values\n    93→  if (state.search) params.set('q', state.search);\n    94→  else params.delete('q');\n    95→\n    96→  const allStatuses = state.statuses.length === 4;\n    97→  if (!allStatuses) params.set('status', state.statuses.join(','));\n    98→  else params.delete('status');\n    99→\n   100→  if (state.qualityMin > 1) params.set('qmin', String(state.qualityMin));\n   101→  else params.delete('qmin');\n   102→\n   103→  if (state.qualityMax < 5) params.set('qmax', String(state.qualityMax));\n   104→  else params.delete('qmax');\n   105→\n   106→  if (state.sortKey !== 'name' || state.sortDir !== 'asc') {\n   107→    params.set('sort', state.sortKey);\n   108→    params.set('dir', state.sortDir);\n   109→  } else {\n   110→    params.delete('sort');\n   111→    params.delete('dir');\n   112→  }\n   113→\n   114→  const qs = params.toString();\n   115→  const newUrl = qs ? `${window.location.pathname}?${qs}` : window.location.pathname;\n   116→  window.history.replaceState({}, '', newUrl);\n   117→}\n   118→\n   119→interface FeatureMatrixProps {\n   120→  moduleId: string;\n   121→  accentColor: string;\n   122→  onReview: () => void;\n   123→  onSync?: () => void;\n   124→  isReviewing: boolean;\n   125→  onFix?: (feature: FeatureRow) => void;\n   126→  isFixing?: boolean;\n   127→  onReviewFeature?: (feature: FeatureRow) => void;\n   128→}\n   129→\n   130→export function FeatureMatrix({ moduleId, accentColor, onReview, onSync, isReviewing, onFix, isFixing, onReviewFeature }: FeatureMatrixProps) {\n   131→  const { features, summary, isLoading, error, retry, refetch } = useFeatureMatrix(moduleId);\n   132→  const projectPath = useProjectStore((s) => s.projectPath);\n   133→  const [expandedRows, setExpandedRows] = useState<Set<string>>(new Set());\n   134→  const [isSyncing, setIsSyncing] = useState(false);\n   135→  const [collapsedCategories, setCollapsedCategories] = useState<Set<string>>(new Set());\n   136→  const [snapshots, setSnapshots] = useState<ReviewSnapshot[]>([]);\n   137→  const [allStatuses, setAllStatuses] = useState<Map<string, string>>(new Map());\n   138→  const [reviewProgress, setReviewProgress] = useState<{ scanned: number; total: number } | null>(null);\n   139→  const progressTimer = useRef<ReturnType<typeof setInterval> | null>(null);\n   140→  // Filter / sort / search state — initialize from URL params\n   141→  const urlInit = useMemo(() => readUrlParams(), []);\n   142→  const [searchQuery, setSearchQuery] = useState(urlInit.search ?? '');\n   143→  const [qualityMin, setQualityMin] = useState(urlInit.qualityMin ?? 1);\n   144→  const [qualityMax, setQualityMax] = useState(urlInit.qualityMax ?? 5);\n   145→  const [sortKey, setSortKey] = useState<SortKey>(urlInit.sortKey ?? 'name');\n   146→  const [sortDir, setSortDir] = useState<SortDir>(urlInit.sortDir ?? 'asc');\n   147→  const [activeFilters, setActiveFilters] = useState<Set<FeatureStatus>>(\n   148→    new Set<FeatureStatus>(urlInit.statuses ?? ['implemented', 'partial', 'missing', 'unknown'])\n   149→  );\n   150→\n   151→  // Sync filter/sort state to URL params\n   152→  useEffect(() => {\n   153→    writeUrlParams({\n   154→      search: searchQuery,\n   155→      statuses: Array.from(activeFilters) as FeatureStatus[],\n   156→      qualityMin,\n   157→      qualityMax,\n   158→      sortKey,\n   159→      sortDir,\n   160→    });\n   161→  }, [searchQuery, activeFilters, qualityMin, qualityMax, sortKey, sortDir]);\n   162→\n   163→  // Poll review progress while reviewing\n   164→  useEffect(() => {\n   165→    if (!isReviewing || !projectPath) {\n   166→      setReviewProgress(null);\n   167→      if (progressTimer.current) {\n   168→        clearInterval(progressTimer.current);\n   169→        progressTimer.current = null;\n   170→      }\n   171→      return;\n   172→    }\n   173→\n   174→    const poll = async () => {\n   175→      try {\n   176→        const params = new URLSearchParams({ moduleId, projectPath });\n   177→        const res = await fetch(`/api/feature-matrix/progress?${params}`);\n   178→        if (res.ok) {\n   179→          const data = await res.json();\n   180→          setReviewProgress({ scanned: data.scanned ?? 0, total: data.total ?? 0 });\n   181→        }\n   182→      } catch { /* silent */ }\n   183→    };\n   184→\n   185→    // Start polling after a brief delay (CLI needs time to start)\n   186→    const startTimeout = setTimeout(() => {\n   187→      poll();\n   188→      progressTimer.current = setInterval(poll, 3000);\n   189→    }, 2000);\n   190→\n   191→    return () => {\n   192→      clearTimeout(startTimeout);\n   193→      if (progressTimer.current) {\n   194→        clearInterval(progressTimer.current);\n   195→        progressTimer.current = null;\n   196→      }\n   197→    };\n   198→  }, [isReviewing, moduleId, projectPath]);\n   199→\n   200→  const fetchHistory = useCallback(async () => {\n   201→    try {\n   202→      const res = await fetch(`/api/feature-matrix/history?moduleId=${encodeURIComponent(moduleId)}`);\n   203→      if (res.ok) {\n   204→        const data = await res.json();\n   205→        setSnapshots(data.snapshots ?? []);\n   206→      }\n   207→    } catch { /* silent */ }\n   208→  }, [moduleId]);\n   209→\n   210→  // Fetch all module statuses for cross-module dependency resolution\n   211→  const fetchAllStatuses = useCallback(async () => {\n   212→    try {\n   213→      const res = await fetch('/api/feature-matrix/all-statuses');\n   214→      if (res.ok) {\n   215→        const data = await res.json();\n   216→        const map = new Map<string, string>();\n   217→        for (const row of data.statuses ?? []) {\n   218→          map.set(`${row.moduleId}::${row.featureName}`, row.status);\n   219→        }\n   220→        setAllStatuses(map);\n   221→      }\n   222→    } catch { /* silent */ }\n   223→  }, []);\n   224→\n   225→  useEffect(() => { fetchHistory(); }, [fetchHistory]);\n   226→  useEffect(() => { fetchAllStatuses(); }, [fetchAllStatuses]);\n   227→\n   228→  // Compute dependency info for all features in this module\n   229→  const depMap = useMemo(() => {\n   230→    const base = buildDependencyMap();\n   231→    // Merge current module's live features into status map (they're fresher than allStatuses)\n   232→    const merged = new Map(allStatuses);\n   233→    for (const f of features) {\n   234→      merged.set(`${f.moduleId}::${f.featureName}`, f.status);\n   235→    }\n   236→    return computeBlockers(base, merged);\n   237→  }, [allStatuses, features]);\n   238→\n   239→  const toggleRow = (featureName: string) => {\n   240→    setExpandedRows((prev) => {\n   241→      const next = new Set(prev);\n   242→      if (next.has(featureName)) next.delete(featureName);\n   243→      else next.add(featureName);\n   244→      return next;\n   245→    });\n   246→  };\n   247→\n   248→  const toggleCategory = (category: string) => {\n   249→    setCollapsedCategories((prev) => {\n   250→      const next = new Set(prev);\n   251→      if (next.has(category)) next.delete(category);\n   252→      else next.add(category);\n   253→      return next;\n   254→    });\n   255→  };\n   256→\n   257→  const toggleFilter = (status: FeatureStatus) => {\n   258→    setActiveFilters((prev) => {\n   259→      const next = new Set(prev);\n   260→      if (next.has(status)) {\n   261→        // Don't allow deactivating all chips\n   262→        if (next.size > 1) next.delete(status);\n   263→      } else {\n   264→        next.add(status);\n   265→      }\n   266→      return next;\n   267→    });\n   268→  };\n   269→\n   270→  const toggleSort = (key: SortKey) => {\n   271→    if (sortKey === key) {\n   272→      setSortDir(d => d === 'asc' ? 'desc' : 'asc');\n   273→    } else {\n   274→      setSortKey(key);\n   275→      setSortDir('asc');\n   276→    }\n   277→  };\n   278→\n   279→  // Filter features: status chips → text search → quality range\n   280→  const filtered = useMemo(() => {\n   281→    let list = features.filter((f) => activeFilters.has(f.status));\n   282→\n   283→    // Text search across feature name, description, review notes, and file paths\n   284→    if (searchQuery) {\n   285→      const q = searchQuery.toLowerCase();\n   286→      list = list.filter(\n   287→        (f) =>\n   288→          f.featureName.toLowerCase().includes(q) ||\n   289→          f.description.toLowerCase().includes(q) ||\n   290→          f.reviewNotes.toLowerCase().includes(q) ||\n   291→          f.filePaths.some((fp) => fp.toLowerCase().includes(q))\n   292→      );\n   293→    }\n   294→\n   295→    // Quality score range filter\n   296→    if (qualityMin > 1 || qualityMax < 5) {\n   297→      list = list.filter((f) => {\n   298→        if (f.qualityScore === null) return false; // unscored items hidden when quality filter active\n   299→        return f.qualityScore >= qualityMin && f.qualityScore <= qualityMax;\n   300→      });\n   301→    }\n   302→\n   303→    // Sort\n   304→    list = [...list].sort((a, b) => {\n   305→      let cmp = 0;\n   306→      switch (sortKey) {\n   307→        case 'name':\n   308→          cmp = a.featureName.localeCompare(b.featureName);\n   309→          break;\n   310→        case 'status':\n   311→          cmp = STATUS_ORDER[a.status] - STATUS_ORDER[b.status];\n   312→          break;\n   313→        case 'quality':\n   314→          cmp = (a.qualityScore ?? 0) - (b.qualityScore ?? 0);\n   315→          break;\n   316→        case 'reviewed': {\n   317→          const aT = a.lastReviewedAt ? new Date(a.lastReviewedAt).getTime() : 0;\n   318→          const bT = b.lastReviewedAt ? new Date(b.lastReviewedAt).getTime() : 0;\n   319→          cmp = aT - bT;\n   320→          break;\n   321→        }\n   322→      }\n   323→      return sortDir === 'asc' ? cmp : -cmp;\n   324→    });\n   325→\n   326→    return list;\n   327→  }, [features, activeFilters, searchQuery, qualityMin, qualityMax, sortKey, sortDir]);\n   328→\n   329→  // Group by category\n   330→  const grouped = filtered.reduce<Record<string, FeatureRow[]>>((acc, f) => {\n   331→    if (!acc[f.category]) acc[f.category] = [];\n   332→    acc[f.category].push(f);\n   333→    return acc;\n   334→  }, {});\n   335→\n   336→  const categories = Object.keys(grouped).sort();\n   337→\n   338→  const lastReviewed = features.find((f) => f.lastReviewedAt)?.lastReviewedAt;\n   339→  const neverReviewed = features.length > 0 && features.every((f) => f.status === 'unknown' && !f.lastReviewedAt);\n   340→\n   341→  if (isLoading) {\n   342→    return (\n   343→      <div className=\"flex items-center justify-center py-12\">\n   344→        <Loader2 className=\"w-5 h-5 animate-spin text-text-muted-hover\" />\n   345→      </div>\n   346→    );\n   347→  }\n   348→\n   349→  if (error) {\n   350→    return <FetchError message={error} onRetry={retry} />;\n   351→  }\n   352→\n   353→  if (neverReviewed) {\n   354→    return (\n   355→      <div className=\"flex items-center justify-center py-10\">\n   356→        <div\n   357→          className=\"flex flex-col items-center gap-3 px-10 py-8 rounded-xl max-w-sm\"\n   358→          style={{\n   359→            border: `1.5px dashed ${accentColor}38`,\n   360→            backgroundColor: `${accentColor}14`,\n   361→          }}\n   362→        >\n   363→          <RefreshCw className=\"w-8 h-8\" style={{ color: accentColor, opacity: 0.7 }} />\n   364→\n   365→          <h3 className=\"text-sm font-semibold text-text\">No review yet</h3>\n   366→\n   367→          <p className=\"text-xs text-text-muted text-center leading-relaxed\">\n   368→            Claude will scan your project source files, evaluate each feature&apos;s implementation status, and assign quality scores with actionable next steps.\n   369→          </p>\n   370→\n   371→          <span className=\"text-xs text-text-muted\">\n   372→            {features.length} feature{features.length !== 1 ? 's' : ''} to analyze\n   373→          </span>\n   374→\n   375→          <button\n   376→            onClick={onReview}\n   377→            disabled={isReviewing}\n   378→            className=\"mt-1 flex items-center gap-2 px-5 py-2.5 rounded-lg text-xs font-semibold transition-all disabled:opacity-50 hover:brightness-110\"\n   379→            style={{\n   380→              backgroundColor: `${accentColor}24`,\n   381→              color: accentColor,\n   382→              border: `1px solid ${accentColor}38`,\n   383→            }}\n   384→          >\n   385→            {isReviewing ? (\n   386→              <>\n   387→                <Loader2 className=\"w-3.5 h-3.5 animate-spin\" />\n   388→                Reviewing...\n   389→              </>\n   390→            ) : (\n   391→              <>\n   392→                <RefreshCw className=\"w-3.5 h-3.5\" />\n   393→                Review with Claude\n   394→              </>\n   395→            )}\n   396→          </button>\n   397→\n   398→          {isReviewing && reviewProgress && reviewProgress.total > 0 && (\n   399→            <ReviewProgressBar\n   400→              scanned={reviewProgress.scanned}\n   401→              total={reviewProgress.total}\n   402→              accentColor={accentColor}\n   403→            />\n   404→          )}\n   405→        </div>\n   406→      </div>\n   407→    );\n   408→  }\n   409→\n   410→  return (\n   411→    <div className=\"space-y-4\">\n   412→      {/* Summary bar + Sparkline + Review button */}\n   413→      <div className=\"flex items-center gap-4\">\n   414→        <SummaryBar summary={summary} />\n   415→        {snapshots.length >= 2 && (\n   416→          <QualitySparkline snapshots={snapshots} accentColor={accentColor} />\n   417→        )}\n   418→        <div className=\"flex items-center gap-2 flex-shrink-0\">\n   419→          {lastReviewed && (() => {\n   420→            const { label, dotColor, isOutdated } = formatRelativeTime(lastReviewed);\n   421→            return (\n   422→              <span className=\"flex items-center gap-1.5 text-xs text-text-muted\" title={new Date(lastReviewed).toLocaleString()}>\n   423→                <span className=\"w-1.5 h-1.5 rounded-full flex-shrink-0\" style={{ backgroundColor: dotColor }} />\n   424→                {label}\n   425→                {isOutdated && (\n   426→                  <span className=\"text-2xs text-[#f87171]/70 font-medium\">outdated</span>\n   427→                )}\n   428→              </span>\n   429→            );\n   430→          })()}\n   431→          {onSync && (\n   432→            <button\n   433→              onClick={async () => {\n   434→                setIsSyncing(true);\n   435→                try {\n   436→                  await onSync();\n   437→                  await refetch();\n   438→                } finally {\n   439→                  setIsSyncing(false);\n   440→                }\n   441→              }}\n   442→              disabled={isSyncing}\n   443→              className=\"flex items-center gap-1.5 px-2.5 py-1.5 rounded-md text-xs font-medium transition-all disabled:opacity-50 text-text-muted-hover hover:text-[#d0d4e8] bg-border hover:bg-[#2a2a4a] border border-[#2a2a4a]\"\n   444→              title=\"Import latest review from disk\"\n   445→            >\n   446→              {isSyncing ? (\n   447→                <Loader2 className=\"w-3 h-3 animate-spin\" />\n   448→              ) : (\n   449→                <Download className=\"w-3 h-3\" />\n   450→              )}\n   451→            </button>\n   452→          )}\n   453→          <button\n   454→            onClick={onReview}\n   455→            disabled={isReviewing}\n   456→            className=\"flex items-center gap-1.5 px-3 py-1.5 rounded-md text-xs font-medium transition-all disabled:opacity-50\"\n   457→            style={{\n   458→              backgroundColor: `${accentColor}24`,\n   459→              color: accentColor,\n   460→              border: `1px solid ${accentColor}38`,\n   461→            }}\n   462→          >\n   463→            {isReviewing ? (\n   464→              <>\n   465→                <Loader2 className=\"w-3 h-3 animate-spin\" />\n   466→                Reviewing...\n   467→              </>\n   468→            ) : (\n   469→              <>\n   470→                <RefreshCw className=\"w-3 h-3\" />\n   471→                Review with Claude\n   472→              </>\n   473→            )}\n   474→          </button>\n   475→        </div>\n   476→      </div>\n   477→\n   478→      {/* Review progress bar */}\n   479→      {isReviewing && reviewProgress && reviewProgress.total > 0 && (\n   480→        <ReviewProgressBar\n   481→          scanned={reviewProgress.scanned}\n   482→          total={reviewProgress.total}\n   483→          accentColor={accentColor}\n   484→        />\n   485→      )}\n   486→\n   487→      {/* Status filter chips */}\n   488→      <StatusFilterChips summary={summary} activeFilters={activeFilters} onToggle={toggleFilter} />\n   489→\n   490→      {/* Search + Quality filter + Sort controls */}\n   491→      <div className=\"flex items-center gap-3 flex-wrap\">\n   492→        {/* Text search */}\n   493→        <div className=\"relative flex-1 min-w-[180px]\">\n   494→          <Search className=\"absolute left-2.5 top-1/2 -translate-y-1/2 w-3.5 h-3.5 text-text-muted\" />\n   495→          <input\n   496→            type=\"text\"\n   497→            placeholder=\"Search features, notes, files...\"\n   498→            value={searchQuery}\n   499→            onChange={(e) => setSearchQuery(e.target.value)}\n   500→            className=\"w-full pl-8 pr-3 py-1.5 rounded-md bg-surface-deep border border-border text-xs text-text placeholder-text-muted focus:outline-none focus:border-[#3b3b6a] transition-colors\"\n   501→          />\n   502→        </div>\n   503→\n   504→        {/* Quality score range */}\n   505→        <QualityRangeFilter min={qualityMin} max={qualityMax} onMinChange={setQualityMin} onMaxChange={setQualityMax} />\n   506→\n   507→        {/* Sort toggles */}\n   508→        <div className=\"flex items-center gap-1\">\n   509→          <SortButton label=\"Name\" sortKey=\"name\" currentKey={sortKey} currentDir={sortDir} onToggle={toggleSort} />\n   510→          <SortButton label=\"Status\" sortKey=\"status\" currentKey={sortKey} currentDir={sortDir} onToggle={toggleSort} />\n   511→          <SortButton label=\"Quality\" sortKey=\"quality\" currentKey={sortKey} currentDir={sortDir} onToggle={toggleSort} />\n   512→          <SortButton label=\"Reviewed\" sortKey=\"reviewed\" currentKey={sortKey} currentDir={sortDir} onToggle={toggleSort} />\n   513→        </div>\n   514→      </div>\n   515→\n   516→      {/* Result count */}\n   517→      {(searchQuery || qualityMin > 1 || qualityMax < 5) && (\n   518→        <div className=\"text-xs text-text-muted\">\n   519→          Showing {filtered.length} of {features.length} features\n   520→        </div>\n   521→      )}\n   522→\n   523→      {/* Feature table grouped by category */}\n   524→      <div className=\"space-y-1\">\n   525→        {categories.map((cat) => {\n   526→          const catFeatures = grouped[cat];\n   527→          const isCollapsed = collapsedCategories.has(cat);\n   528→          const catImplemented = catFeatures.filter((f) => f.status === 'implemented').length;\n   529→\n   530→          return (\n   531→            <div key={cat}>\n   532→              {/* Category header */}\n   533→              <button\n   534→                onClick={() => toggleCategory(cat)}\n   535→                className=\"w-full flex items-center gap-2 px-3 py-2 rounded-md hover:bg-surface-hover transition-colors\"\n   536→              >\n   537→                {isCollapsed ? (\n   538→                  <ChevronRight className=\"w-3 h-3 text-text-muted-hover\" />\n   539→                ) : (\n   540→                  <ChevronDown className=\"w-3 h-3 text-text-muted-hover\" />\n   541→                )}\n   542→                <span className=\"text-xs font-semibold uppercase tracking-wider text-text-muted\">\n   543→                  {cat}\n   544→                </span>\n   545→                <span className=\"text-2xs text-text-muted\">\n   546→                  {catImplemented}/{catFeatures.length}\n   547→                </span>\n   548→              </button>\n   549→\n   550→              {/* Feature rows */}\n   551→              {!isCollapsed && (\n   552→                <StaggerContainer className=\"ml-2 space-y-px\">\n   553→                  {catFeatures.map((feature) => {\n   554→                    const featureKey = `${feature.moduleId}::${feature.featureName}`;\n   555→                    const depInfo = depMap.get(featureKey);\n   556→                    return (\n   557→                      <StaggerItem key={feature.featureName}>\n   558→                        <FeatureRowItem\n   559→                          feature={feature}\n   560→                          isExpanded={expandedRows.has(feature.featureName)}\n   561→                          onToggle={() => toggleRow(feature.featureName)}\n   562→                          depInfo={depInfo}\n   563→                          onFix={onFix}\n   564→                          isFixing={isFixing}\n   565→                          onReviewFeature={onReviewFeature}\n   566→                          accentColor={accentColor}\n   567→                        />\n   568→                      </StaggerItem>\n   569→                    );\n   570→                  })}\n   571→                </StaggerContainer>\n   572→              )}\n   573→            </div>\n   574→          );\n   575→        })}\n   