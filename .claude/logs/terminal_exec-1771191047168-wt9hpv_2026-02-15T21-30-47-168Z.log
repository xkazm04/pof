  const date = new Date(ts);\n    44→  const todayStart = new Date(now.getFullYear(), now.getMonth(), now.getDate()).getTime();\n    45→  const yesterdayStart = todayStart - 86_400_000;\n    46→  const weekStart = todayStart - (now.getDay() * 86_400_000);\n    47→\n    48→  if (ts >= todayStart) return 'Today';\n    49→  if (ts >= yesterdayStart) return 'Yesterday';\n    50→  if (ts >= weekStart) return 'Earlier this week';\n    51→  return 'Older';\n    52→}\n    53→\n    54→interface EventGroup {\n    55→  type: ActivityEventType;\n    56→  moduleId: string | undefined;\n    57→  events: ActivityEvent[];\n    58→}\n    59→\n    60→/** Group consecutive events of the same type+module within a time period */\n    61→function groupConsecutive(events: ActivityEvent[]): EventGroup[] {\n    62→  const groups: EventGroup[] = [];\n    63→  for (const event of events) {\n    64→    const last = groups[groups.length - 1];\n    65→    if (last && last.type === event.type && last.moduleId === event.moduleId) {\n    66→      last.events.push(event);\n    67→    } else {\n    68→      groups.push({ type: event.type, moduleId: event.moduleId, events: [event] });\n    69→    }\n    70→  }\n    71→  return groups;\n    72→}\n    73→\n    74→interface TimePeriodSection {\n    75→  period: TimePeriod;\n    76→  groups: EventGroup[];\n    77→}\n    78→\n    79→function buildSections(events: ActivityEvent[]): TimePeriodSection[] {\n    80→  const periodOrder: TimePeriod[] = ['Today', 'Yesterday', 'Earlier this week', 'Older'];\n    81→  const buckets = new Map<TimePeriod, ActivityEvent[]>();\n    82→  for (const p of periodOrder) buckets.set(p, []);\n    83→  for (const e of events) {\n    84→    buckets.get(getTimePeriod(e.timestamp))!.push(e);\n    85→  }\n    86→\n    87→  const sections: TimePeriodSection[] = [];\n    88→  for (const period of periodOrder) {\n    89→    const periodEvents = buckets.get(period)!;\n    90→    if (periodEvents.length === 0) continue;\n    91→    sections.push({ period, groups: groupConsecutive(periodEvents) });\n    92→  }\n    93→  return sections;\n    94→}\n    95→\n    96→// ── Time formatting ──\n    97→\n    98→function timeAgo(ts: number): string {\n    99→  const diff = Date.now() - ts;\n   100→  const seconds = Math.floor(diff / 1000);\n   101→  if (seconds < 60) return 'just now';\n   102→  const minutes = Math.floor(seconds / 60);\n   103→  if (minutes < 60) return `${minutes}m ago`;\n   104→  const hours = Math.floor(minutes / 60);\n   105→  if (hours < 24) return `${hours}h ago`;\n   106→  const days = Math.floor(hours / 24);\n   107→  return `${days}d ago`;\n   108→}\n   109→\n   110→// ── Component ──\n   111→\n   112→export function ActivityFeedPanel() {\n   113→  const events = useActivityFeedStore((s) => s.events);\n   114→  const isOpen = useActivityFeedStore((s) => s.isOpen);\n   115→  const setOpen = useActivityFeedStore((s) => s.setOpen);\n   116→  const dismissEvent = useActivityFeedStore((s) => s.dismissEvent);\n   117→  const dismissAll = useActivityFeedStore((s) => s.dismissAll);\n   118→  const { sendPromptToModule } = useModuleActions();\n   119→\n   120→  // Refresh relative timestamps every 60s\n   121→  const [, setTick] = useState(0);\n   122→  useEffect(() => {\n   123→    const id = setInterval(() => setTick((n) => n + 1), 60_000);\n   124→    return () => clearInterval(id);\n   125→  }, []);\n   126→\n   127→  const unreadCount = useMemo(() => events.filter((e) => !e.dismissed).length, [events]);\n   128→  const sections = useMemo(() => buildSections(events), [events]);\n   129→\n   130→  const handleDismiss = useCallback(\n   131→    (id: string, e: React.MouseEvent) => {\n   132→      e.stopPropagation();\n   133→      dismissEvent(id);\n   134→    },\n   135→    [dismissEvent],\n   136→  );\n   137→\n   138→  const handleAct = useCallback(\n   139→    (event: ActivityEvent) => {\n   140→      if (!event.moduleId || !event.meta?.prompt) return;\n   141→      sendPromptToModule(event.moduleId as SubModuleId, event.meta.prompt);\n   142→      dismissEvent(event.id);\n   143→    },\n   144→    [sendPromptToModule, dismissEvent],\n   145→  );\n   146→\n   147→  if (!isOpen) return null;\n   148→\n   149→  return (\n   150→    <div className=\"w-80 border-l border-border bg-surface-deep flex flex-col h-full flex-shrink-0\">\n   151→      {/* Header */}\n   152→      <div className=\"flex items-center justify-between px-4 py-3 border-b border-border\">\n   153→        <div className=\"flex items-center gap-2\">\n   154→          <Bell className=\"w-3.5 h-3.5 text-[#ef4444]\" />\n   155→          <h2 className=\"text-xs font-semibold text-text uppercase tracking-wider\">Activity</h2>\n   156→          {unreadCount > 0 && (\n   157→            <span className=\"text-2xs font-bold text-[#ef4444] bg-status-red-subtle px-1.5 py-0.5 rounded-full\">\n   158→              {unreadCount}\n   159→            </span>\n   160→          )}\n   161→        </div>\n   162→        <div className=\"flex items-center gap-1\">\n   163→          {unreadCount > 0 && (\n   164→            <button\n   165→              onClick={dismissAll}\n   166→              className=\"text-2xs text-text-muted hover:text-text px-2 py-1 rounded transition-colors hover:bg-border\"\n   167→            >\n   168→              Read all\n   169→            </button>\n   170→          )}\n   171→          <button\n   172→            onClick={() => setOpen(false)}\n   173→            className=\"p-1 rounded-md text-text-muted hover:text-text hover:bg-border transition-colors\"\n   174→          >\n   175→            <X className=\"w-3.5 h-3.5\" />\n   176→          </button>\n   177→        </div>\n   178→      </div>\n   179→\n   180→      {/* Events list */}\n   181→      <div className=\"flex-1 overflow-y-auto\">\n   182→        {events.length === 0 ? (\n   183→          <div className=\"flex flex-col items-center justify-center py-16 px-6\">\n   184→            <div className=\"w-1