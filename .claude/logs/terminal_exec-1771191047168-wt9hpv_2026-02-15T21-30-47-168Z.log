[2026-02-15T21:31:38.685Z] [STDOUT] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01UYFerU1R6ZEduVoE9r232t","type":"tool_result","content":"     1→'use client';\n     2→\n     3→import { useState, useRef, useEffect, useCallback } from 'react';\n     4→import { motion, AnimatePresence, useReducedMotion } from 'framer-motion';\n     5→import {\n     6→  Search, X, ListChecks, Box, Layers, AlertTriangle, Package,\n     7→  ArrowRight, RefreshCw, Loader2,\n     8→} from 'lucide-react';\n     9→import { useNavigationStore } from '@/stores/navigationStore';\n    10→import type { SearchResult } from '@/lib/search-index';\n    11→\n    12→// ── Type icons ───────────────────────────────────────────────────────────────\n    13→\n    14→const TYPE_META: Record<string, { icon: typeof Box; label: string; color: string }> = {\n    15→  checklist: { icon: ListChecks, label: 'Checklist', color: '#00ff88' },\n    16→  feature:   { icon: Box, label: 'Feature', color: '#60a5fa' },\n    17→  module:    { icon: Layers, label: 'Module', color: '#a78bfa' },\n    18→  category:  { icon: Layers, label: 'Category', color: '#f59e0b' },\n    19→  finding:   { icon: AlertTriangle, label: 'Finding', color: '#ef4444' },\n    20→  build:     { icon: Package, label: 'Build', color: '#94a3b8' },\n    21→};\n    22→\n    23→// ── Main component ──────────────────────────────────────────────────────────\n    24→\n    25→export function GlobalSearchPanel() {\n    26→  const [open, setOpen] = useState(false);\n    27→  const [query, setQuery] = useState('');\n    28→  const [results, setResults] = useState<SearchResult[]>([]);\n    29→  const [loading, setLoading] = useState(false);\n    30→  const [rebuilding, setRebuilding] = useState(false);\n    31→  const [activeIndex, setActiveIndex] = useState(0);\n    32→  const [lastRebuilt, setLastRebuilt] = useState<string | null>(null);\n    33→  const [activeFilter, setActiveFilter] = useState<string | null>(null);\n    34→\n    35→  const inputRef = useRef<HTMLInputElement>(null);\n    36→  const resultsRef = useRef<HTMLDivElement>(null);\n    37→  const debounceRef = useRef<ReturnType<typeof setTimeout> | null>(null);\n    38→\n    39→  const navigateToModule = useNavigationStore((s) => s.navigateToModule);\n    40→  const prefersReduced = useReducedMotion();\n    41→\n    42→  // ── Keyboard shortcut: Ctrl+K ──\n    43→  useEffect(() => {\n    44→    const handler = (e: KeyboardEvent) => {\n    45→      if ((e.ctrlKey || e.metaKey) && e.key === 'k') {\n    46→        e.preventDefault();\n    47→        setOpen((prev) => !prev);\n    48→      }\n    49→      if (e.key === 'Escape' && open) {\n    50→        e.preventDefault();\n    51→        setOpen(false);\n    52→      }\n    53→    };\n    54→    window.addEventListener('keydown', handler);\n    55→    return () => window.removeEventListener('keydown', handler);\n    56→  }, [open]);\n    57→\n    58→  // ── Focus input when opening ──\n    59→  useEffect(() => {\n    60→    if (open) {\n    61→      // Small delay to let animation start\n    62→      requestAnimationFrame(() => inputRef.current?.focus());\n    63→      // Rebuild index on first open (ensures it's fresh)\n    64→      handleRebuild(true);\n    65→    } else {\n    66→      setQuery('');\n    67→      setResults([]);\n    68→      setActiveIndex(0);\n    69→      setActiveFilter(null);\n    70→    }\n    71→    // eslint-disable-next-line react-hooks/exhaustive-deps\n    72→  }, [open]);\n    73→\n    74→  // ── Search with debounce ──\n    75→  useEffect(() => {\n    76→    if (!query.trim()) {\n    77→      setResults([]);\n    78→      setActiveIndex(0);\n    79→      return;\n    80→    }\n    81→\n    82→    if (debounceRef.current) clearTimeout(debounceRef.current);\n    83→\n    84→    debounceRef.current = setTimeout(async () => {\n    85→      setLoading(true);\n    86→      try {\n    87→        const params = new URLSearchParams({ q: query });\n    88→      