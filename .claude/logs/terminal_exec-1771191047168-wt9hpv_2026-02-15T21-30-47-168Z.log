220→                    </StaggerItem>\n   221→                  ),\n   222→                )}\n   223→              </div>\n   224→            ))}\n   225→          </StaggerContainer>\n   226→        )}\n   227→      </div>\n   228→\n   229→      {/* Footer — inbox zero */}\n   230→      {events.length > 0 && unreadCount === 0 && (\n   231→        <div className=\"px-4 py-3 border-t border-border\">\n   232→          <div className=\"flex items-center gap-2\">\n   233→            <CheckCircle2 className=\"w-3.5 h-3.5 text-[#4ade80]\" />\n   234→            <span className=\"text-xs text-[#4ade80] font-medium\">All caught up</span>\n   235→          </div>\n   236→        </div>\n   237→      )}\n   238→    </div>\n   239→  );\n   240→}\n   241→\n   242→// ── Collapsed Group ──\n   243→\n   244→function CollapsedGroup({\n   245→  group,\n   246→  onDismiss,\n   247→  onAct,\n   248→}: {\n   249→  group: EventGroup;\n   250→  onDismiss: (id: string, e: React.MouseEvent) => void;\n   251→  onAct: (event: ActivityEvent) => void;\n   252→}) {\n   253→  const [expanded, setExpanded] = useState(false);\n   254→  const config = EVENT_CONFIG[group.type] ?? EVENT_CONFIG['cli-complete'];\n   255→  const Icon = config.icon;\n   256→  const count = group.events.length;\n   257→  const moduleLabel = group.moduleId ?? '';\n   258→\n   259→  return (\n   260→    <div className=\"rounded-lg border border-border/60 overflow-hidden\">\n   261→      {/* Summary row */}\n   262→      <button\n   263→        onClick={() => setExpanded(!expanded)}\n   264→        className=\"w-full flex items-center gap-2 px-3 py-2 text-left hover:bg-surface-hover/30 transition-colors\"\n   265→      >\n   266→        {expanded\n   267→          ? <ChevronDown className=\"w-3 h-3 text-text-muted flex-shrink-0\" />\n   268→          : <ChevronRight className=\"w-3 h-3 text-text-muted flex-shrink-0\" />\n   269→        }\n   270→        <Icon className=\"w-3.5 h-3.5 flex-shrink-0\" style={{ color: config.